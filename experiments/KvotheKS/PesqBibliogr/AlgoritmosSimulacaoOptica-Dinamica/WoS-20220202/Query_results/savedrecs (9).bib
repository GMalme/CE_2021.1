@article{ WOS:000460099500015,
Author = {Molloy, Kevin and Denarie, Laurent and Vaisset, Marc and Simeon, Thierry
   and Cortes, Juan},
Title = {{Simultaneous system design and path planning: A sampling-based algorithm}},
