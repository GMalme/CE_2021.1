@article{ WOS:000587904500012,
Author = {Bunting, Gregory and Miller, Scott T. and Walsh, Timothy F. and
   Dohrmann, Clark R. and Aquino, Wilkins},
Title = {{Novel strategies for modal-based structural material identification}},
