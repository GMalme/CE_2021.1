@article{ WOS:000461576300001,
Author = {Song, Zhaoyun and Liu, Bo and Cheng, Hao},
Title = {{Adaptive particle swarm optimization with population diversity control
   and its application in tandem blade optimization}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART C-JOURNAL OF
   MECHANICAL ENGINEERING SCIENCE}},
Year = {{2019}},
Volume = {{233}},
