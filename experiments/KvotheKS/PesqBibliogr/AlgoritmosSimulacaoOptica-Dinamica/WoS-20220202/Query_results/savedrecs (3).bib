@article{ WOS:000257490500007,
Author = {Baek, Jo-Ha and Lee, Dae-Woo and Kim, Jong-Hun and Cho, Kyeum-Rae and
   Yang, Jang-Sik},
Title = {{Trajectory optimization and the control of a re-entry vehicle in TAEM
   phase}},
Journal = {{JOURNAL OF MECHANICAL SCIENCE AND TECHNOLOGY}},
Year = {{2008}},
Volume = {{22}},
Number = {{6}},
Pages = {{1099-1110}},
Month = {{JUN}},
Abstract = {{This paper investigates the optimal trajectory and the feedback
   linearization control of a re-entry vehicle during TAEM (terminal area
   energy management) phase. First, an optimization algorithm with dynamic
   pressure as the cost function is used to obtain the optimal trajectory
   in TAEM. This optimal trajectory is considered the reference for
   ensuring a stable flight path of the re-entry vehicle. The control
   inputs are the angle of attack and bank angle, which determine the total
   energy and safety of the re-entry vehicle. Second, feedback
   linearization is used to design a tracking law in the TAEM phase. This
   paper validates the optimal solution as the reference trajectory with
   HAC (heading alignment cylinder) and the tracking performance of the
   re-entry vehicle onto the reference trajectory by feedback
   linearization.}},
