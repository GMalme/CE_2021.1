@article{ WOS:000343322300017,
Author = {Mohan, S. C. and Yadav, Amit and Maiti, Dipak Kumar and Maity, Damodar},
Title = {{A comparative study on crack identification of structures from the
   changes in natural frequencies using GA and PSO}},
