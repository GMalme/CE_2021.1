@article{ WOS:000639708000039,
Author = {Gultekin, Yunus Can and van Houtum, Wim J. and Koppelaar, Arie G. C. and
   Willems, Frans M. J.},
Title = {{Comparison and Optimization of Enumerative Coding Techniques for
   Amplitude Shaping}},
Journal = {{IEEE COMMUNICATIONS LETTERS}},
Year = {{2021}},
Volume = {{25}},
Number = {{4}},
Pages = {{1231-1235}},
Month = {{APR}},
Abstract = {{The energy efficiency of enumerative coding algorithms is investigated
   for amplitude shaping in the context of binary transmission. First, a
   simple method for the calculation of the amplitude distribution is
   derived for enumerative sphere shaping (ESS). For ultra-short
   blocklengths, ESS-which orders sequences lexicographically-is shown to
   be less energy-efficient than algorithms that use energy-based ordering
   such as shell mapping. Second, ESS is optimized heuristically such that
   its energy efficiency is improved. Simulations show that optimized ESS
   achieves the same error probabilities as energy-based ordering methods
   for the additive white Gaussian noise (AWGN) channel, even at
   ultra-short blocklengths.}},
