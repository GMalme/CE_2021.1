@article{ WOS:000722002000007,
Author = {Battaglioli, Sara and James, Oran and Gibbons, Michael J. and Persoons,
   Tim},
Title = {{Simplified Steady-State Representation of Slot Synthetic Jet Actuators
   to Enable Numerical Optimization With Steady RANS Simulations}},
Journal = {{IEEE TRANSACTIONS ON COMPONENTS PACKAGING AND MANUFACTURING TECHNOLOGY}},
Year = {{2021}},
Volume = {{11}},
