@article{ WOS:000280073500010,
Author = {Nan Ying and Huang GuoQiang and Lu YuPing and Gong Ping},
Title = {{Global 4-D trajectory optimization for spacecraft}},
Journal = {{SCIENCE CHINA-TECHNOLOGICAL SCIENCES}},
Year = {{2010}},
Volume = {{53}},
Number = {{8}},
Pages = {{2097-2101}},
Month = {{AUG}},
Abstract = {{Global 4-D trajectory (x, y, z, t) is optimized for a spacecraft, which
   is launched from the Earth to fly around the Sun, just as star-drift of
   1437 asteroids in the solar system. The spacecraft trajectory is
   controlled by low thrust. The performance index of optimal trajectory is
   to maximize the rendezvous times with the intermediate asteroids, and
   also maximize the final mass. This paper provides a combined algorithm
   of global 4-D trajectory optimization. The algorithm is composed of
   dynamic programming and two-point-boundary algorithm based on optimal
   control theory. The best 4-D trajectory is obtained: the spacecraft
   flies passing 55 asteroids, and rendezvous with (following or passing
   again) asteroids for 454 days, and finally rendezvous with the asteroid
   2005SN25 on the day 60521 (MJD), the final mass of the spacecraft is
   836.53 kg.}},
