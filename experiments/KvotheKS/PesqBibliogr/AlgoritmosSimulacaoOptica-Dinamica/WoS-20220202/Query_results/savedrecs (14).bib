@article{ WOS:000073527600055,
Author = {Li, CT and Li, J and Yin, SH and Hudson, TD and McMillen, DK},
Title = {{Synthesized multi-level composite filter for synthetic-aperture radar
   image identification}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1998}},
Volume = {{146}},
Number = {{1-6}},
Pages = {{285-301}},
Month = {{JAN 15}},
Abstract = {{A novel optical correlator system to identify the Synthetic-Aperture
   Radar (SAR) image based on multilevel composite function (MCF) filters,
   constructed in input domain, is presented. The MCFs are synthesized by
   the simulated annealing algorithm, which are suitable to current spatial
   light modulators because of their limited dynamic range requirement of
   the MCFs. In this paper, we focus our investigation on the MCFs for the
   SAR image identification, including the impact of number of levels on
   discriminability and noise performance. In addition, a unique position
   encoding technique is also added in the optical system so that MCF fan
   be implemented easily. The simulated and experimental results are
   presented, which are quite consistent. (C) 1998 Elsevier Science B.V.}},
