@article{ WOS:000233867100015,
Author = {Ljubic, I and Weiskircher, R and Pferschy, U and Klau, GW and Mutzel, P
   and Fischetti, M},
Title = {{An algorithmic framework for the exact solution of the prize-collecting
   Steiner tree problem}},
