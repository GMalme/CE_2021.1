@article{ WOS:000424678900047,
Author = {Chen, Zhenning and Shao, Xinxing and Xu, Xiangyang and He, Xiaoyuan},
Title = {{Optimized digital speckle patterns for digital image correlation by
   consideration of both accuracy and efficiency}},
