@article{ WOS:A1994MT38900013,
Author = {SOZOU, PD},
Title = {{INVERSION OF MIRAGE DATA - AN OPTIMIZATION APPROACH}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{1994}},
Volume = {{11}},
Number = {{1}},
Pages = {{125-134}},
Month = {{JAN}},
Abstract = {{The inversion of atmospheric mirage data to produce a refractive-index
   profile and thus a temperature profile is considered. The method
   presented goes further than previous methods by seeking the most
   plausible profile consistent with the data. The approach is to minimize
   a composite cost function; the cost function includes novel terms to
   penalize a refractive-index profile that is not monotonic and to
   penalize a refractive-index profile that is not fully convex or fully
   concave. An algorithm for minimizing the cost function is applied to
   five sets of mirage data, giving good results.}},
