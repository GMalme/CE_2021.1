@article{ WOS:000303053500010,
Author = {Eves, James and Toropov, Vassili V. and Thompson, Harvey M. and Kapur,
   Nik and Fan, Jian and Copley, Daniel and Mincher, Adrian},
Title = {{Design optimization of supersonic jet pumps using high fidelity flow
   analysis}},
