@article{ WOS:A1997WA19400013,
Author = {Koshel, RJ and Walmsley, IA},
Title = {{Optimal design of optically side-pumped lasers}},
Journal = {{IEEE JOURNAL OF QUANTUM ELECTRONICS}},
