@article{ WOS:000361033900022,
Author = {Armstrong, Cameron R. and David, John A. and Thompson, John R.},
Title = {{Phenomenological model of stochastic, spatiotemporal, intensity dynamics
   of stimulated Brillouin scattering in a two-mode optical fiber}},
Journal = {{OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{23}},
Number = {{14}},
Pages = {{17866-17882}},
Month = {{JUL 13}},
Abstract = {{We present a simple numerical model that is used in conjunction with a
   systematic algorithm for parameter optimization to understand the
   three-dimensional stochastic intensity dynamics of stimulated Brillouin
   scattering in a two-mode optical fiber. The primary factors driving the
