@article{ WOS:000462844800046,
Author = {Doelman, Reinier and Verhaegen, Michel},
Title = {{Convex optimization-based blind deconvolution for images taken with
   coherent illumination}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{2019}},
Volume = {{36}},
Number = {{4}},
Pages = {{678-685}},
Month = {{APR 1}},
Abstract = {{A rank-constrained reformulation of the blind deconvolution problem on
   images taken with coherent illumination is proposed. Since in the
   reformulation the rank constraint is imposed on a matrix that is affine
   in the decision variables, we propose a novel convex heuristic for the
   blind deconvolution problem. The proposed heuristic allows for easy
   incorporation of prior information on the decision variables and the use
   of the phase diversity concept. The convex optimization problem can be
   iteratively re-parameterized to obtain better estimates. The proposed
   methods are demonstrated on numerically illustrative examples. (C) 2019
   Optical Society of America}},
