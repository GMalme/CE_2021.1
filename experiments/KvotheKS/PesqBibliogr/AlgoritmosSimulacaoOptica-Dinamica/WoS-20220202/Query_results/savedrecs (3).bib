@article{ WOS:A1996UT18900010,
Author = {Groenwold, AA and Stander, N and Snyman, JA},
Title = {{A pseudo-discrete rounding method for structural optimization}},
Journal = {{STRUCTURAL OPTIMIZATION}},
Year = {{1996}},
Volume = {{11}},
Number = {{3-4}},
Pages = {{218-227}},
Month = {{JUN}},
Abstract = {{A new heuristic method aimed at efficiently solving the mixed-discrete
   nonlinear programming (MDNLP) problem in structural optimization, and
   denoted selective dynamic rounding, is presented. The method is based on
   the sequential rounding of a continuous solution and is in its current
   form used for the optimal discrete sizing design of truss structures. A
   simple criterion based on discrete variable proximity is proposed for
   selecting the sequence in which variables are to be rounded, and
   allowance is made for both upward and downward rounding. While efficient
   in terms of the required number of function evaluations, the method is
   also effective in obtaining a low discrete approximation to the global
   optimum. Numerical results are presented to illustrate the effectiveness
   and efficiency of the method.}},
