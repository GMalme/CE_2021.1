@article{ WOS:000452612200015,
Author = {Zhang, Zhengyun and Li, Wei-Na and Asundi, Anand and Barbastathis,
   George},
Title = {{Simultaneous measurement and reconstruction tailoring for quantitative
   phase imaging}},
Journal = {{OPTICS EXPRESS}},
Year = {{2018}},
Volume = {{26}},
Number = {{25}},
Pages = {{32532-32553}},
Month = {{DEC 10}},
Abstract = {{We propose simultaneous measurement and reconstruction tailoring (SMaRT)
   for quantitative phase imaging; it is a joint optimization approach to
   inverse problems wherein minimizing the expected end-to-end error yields
   optimal design parameters for both the measurement and reconstruction
   processes. Using simulated and experimentally-collected data for a
   specific scenario, we demonstrate that optimizing the design of the two
   processes together reduces phase reconstruction error over past
   techniques that consider these two design problems separately. Our
   results suggest at times surprising design principles, and our approach
   can potentially inspire improved solution methods for other inverse
   problems in optics as well as the natural sciences. (C) 2018 Optical
   Society of America under the terms of the OSA Open Access Publishing
   Agreement}},
