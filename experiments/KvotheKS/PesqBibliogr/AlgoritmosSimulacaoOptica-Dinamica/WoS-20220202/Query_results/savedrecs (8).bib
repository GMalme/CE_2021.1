@article{ WOS:000501349900048,
Author = {Zhao, Yue and Li, Zan and Hao, Benjian and Shi, Jia},
Title = {{Sensor Selection for TDOA-Based Localization in Wireless Sensor Networks
   With Non-Line-of-Sight Condition}},
Journal = {{IEEE TRANSACTIONS ON VEHICULAR TECHNOLOGY}},
Year = {{2019}},
