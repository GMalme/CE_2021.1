@article{ WOS:A1992HT10300010,
Author = {PRATT, JP and HEURING, VP},
Title = {{DELAY SYNCHRONIZATION IN TIME-OF-FLIGHT OPTICAL-SYSTEMS}},
Journal = {{APPLIED OPTICS}},
Year = {{1992}},
Volume = {{31}},
Number = {{14}},
Pages = {{2430-2437}},
Month = {{MAY 10}},
Abstract = {{Time-of-flight optical computer designs must implicitly or explicitly
   allow for the synchronization of all signals at all interaction points.
   This paper details algorithms for calculating delays required for
   synchronization of optical systems, as well as the sensitivity of these
   systems to variations in delays from their nominal values. These
   algorithms, which are applied to graph models of systems, form the basis
   for an optical systems design methodology in which the designer develops
   architectures with lumped delays and idealized zero-delay devices. When
   applied to the system designs, the algorithms provide estimates of
   actual delay distributions and sensitivities.}},
