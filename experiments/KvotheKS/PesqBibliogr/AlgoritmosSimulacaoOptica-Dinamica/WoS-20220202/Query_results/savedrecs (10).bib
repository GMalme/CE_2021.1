@article{ WOS:000725819700036,
Author = {Buelta, Almudena and Olivarfs, Alberto and Staffetti, Ernesto and Aftab,
   Waqas and Mihaylova, Lyudmila},
Title = {{A Gaussian Process Iterative Learning Control for Aircraft Trajectory
   Tracking}},
