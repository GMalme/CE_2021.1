@article{ WOS:000659223100046,
Author = {Chen, Yang and Gao, Ming and Song, Xiao},
Title = {{Method to design the common aperture multi-band optical system based on
   the PSO algorithm}},
Journal = {{OPTICS EXPRESS}},
Year = {{2021}},
Volume = {{29}},
Number = {{12}},
Pages = {{18325-18335}},
Month = {{JUN 7}},
Abstract = {{A design method for a common aperture multi-band optical system based on
   particle swarm optimization (PSO) is proposed. Using the principle of
   PSO, the optimal parameters of the initial structure of optical system,
   which meet the requirements of the target function, can be calculated
   through using multiple iterations. In order to verify the design method,
   a common aperture multi-band system is created. The optical system can
   provide images in visible (0.49-0.66 mu m), near infrared (0.8-0.9 mu m)
   and medium-wave infrared (3.8-4.8 mu m) bands. The focal length of the
   optical system is 70 mm and the field of view is +/- 2.5 degrees. The
   experimental results show that the angular resolution is 1.3 mrad for
   visible light and near infrared and 4.6 mrad for medium-wave infrared.
   The optical system can produce images clearly in both the visible and
   infrared bands, which shows that a design method based on particle swarm
   optimization is feasible. (C) 2021 Optical Society of America under the
   terms of the OSA Open Access Publishing Agreement}},
