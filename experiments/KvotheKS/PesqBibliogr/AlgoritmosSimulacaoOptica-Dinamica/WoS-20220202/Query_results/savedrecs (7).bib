@article{ WOS:000327164500008,
Author = {Nikitin, Viktor V. and Duchkov, Anton A. and Andersson, Fredrik},
Title = {{Parallel algorithm of 3D wave-packet decomposition of seismic data:
   Implementation and optimization for GPU}},
Journal = {{JOURNAL OF COMPUTATIONAL SCIENCE}},
Year = {{2012}},
Volume = {{3}},
Number = {{6, SI}},
Pages = {{469-473}},
Month = {{NOV}},
Abstract = {{In this paper, we consider 3D wave-packet transform that is useful in 3D
   data processing. This transform is computationally intensive even though
   it has a computational complexity of O(N-3 log N). Here we present its
   implementation on GPUs using NVIDIA CUDA technology. The code was tested
   on different types of graphical processors achieving the average speedup
   up to 46 times on Tesla M2050 compared to CPU sequential code. Also, we
   analyzed its scalability for several GPUs. The code was tested for
   processing synthetic seismic data set: data compression, de-noising, and
   interpolation. (C) 2012 Elsevier B.V. All rights reserved.}},
