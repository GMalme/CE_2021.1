@article{ WOS:000408183900040,
Author = {Chen YongBo and Mei YueSong and Yu JianQiao and Su XiaoLong and Xu Nuo},
Title = {{Three-dimensional unmanned aerial vehicle path planning using modified
   wolf pack search algorithm}},
