@article{ WOS:000208628700010,
Author = {Besso, Piermario and Bozzi, Maurizio and Formaggi, Marco and Perregrini,
   Luca},
Title = {{Pointing enhancement techniques for deep-space antennas}},
Journal = {{INTERNATIONAL JOURNAL OF MICROWAVE AND WIRELESS TECHNOLOGIES}},
Year = {{2010}},
Volume = {{2}},
Number = {{2}},
Pages = {{211-218}},
Month = {{APR}},
Abstract = {{The present paper presents a very efficient technique for enhancing the
   pointing accuracy in beam-waveguide (BWG) antennas and its application
   to the deep space antenna DSA2 of the European Space Agency. The
   proposed technique permits to achieve a twofold result: on the one hand,
   it provides a solution to the beam aberration issue, arising when the
   antenna simultaneously receives from and transmits to a spacecraft
   moving in the transversal direction. On the other hand, it allows to
   perform a fast conical scan to enhance the pointing accuracy of the
   antenna. Both results are achieved by simple linear displacements of
   feeds and mirrors located in the lower part of the BWG, with a very
   limited deterioration of the antenna gain. The required displacements of
   feeds and mirrors are determined through a fast optimization algorithm,
   based on a top-down approach, which requires repeated physical-optics
   analyses of the lower part of the beam waveguide only, with a
   significant reduction in the computing time.}},
