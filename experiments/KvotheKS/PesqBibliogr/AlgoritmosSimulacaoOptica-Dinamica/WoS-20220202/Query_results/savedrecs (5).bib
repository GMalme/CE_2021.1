@article{ WOS:000674834700043,
Author = {Sweeney, David and Norris, Barnaby R. M. and Tuthill, Peter and Scalzo,
   Richard and Wei, Jin and Betters, Christopher H. and Leon-Saval, Sergio
   G.},
Title = {{Learning the lantern: neural network applications to broadband photonic
   lantern modeling}},
Journal = {{JOURNAL OF ASTRONOMICAL TELESCOPES INSTRUMENTS AND SYSTEMS}},
Year = {{2021}},
Volume = {{7}},
Number = {{2}},
Month = {{APR}},
Abstract = {{Photonic lanterns (PLs) allow the decomposition of highly multimodal
   light into a simplified modal basis such as single-moded and/or
   few-moded. They are increasingly finding uses in astronomy, optics, and
   telecommunications. Calculating propagation through a PL using
   traditional algorithms takes similar to 1 h per simulation on a modern
   CPU. We demonstrate that neural networks can bridge the disparate
   opto-electronic systems and, when trained, can achieve a speedup of over
   five orders of magnitude. We show that this approach can be used to
   model PLs with manufacturing defects and can be successfully generalized
   to polychromatic data. We demonstrate two uses of these neural network
   models: propagating seeing through the PL and performing global
   optimization for purposes such as PL funnels and PL nullers. (C) 2021
   Society of Photo-Optical Instrumentation Engineers (SPIE)}},
