@article{ WOS:000273860400139,
Author = {Kang, Guoguo and Tan, Qiaofeng and Wang, Xiaoling and Jin, Guofan},
Title = {{Achromatic phase retarder applied to MWIR \& LWIR dual-band}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{2}},
Pages = {{1695-1703}},
Month = {{JAN 18}},
Abstract = {{The development of the dual-band IR imaging polarimetry creates the need
   for achromatic phase retarder used in dual-band. Dielectric grating with
   the period smaller than the illuminating wavelength presents a strong
   form-birefringence. With this feature, the combination of several
   subwavelength gratings can be used as achromatic phase retarders. We
   proposed a combination of 4 subwavelength structured gratings (SWGs)
   used as an achromatic quarter-wave plate (QWP) applied to MWIR \& LWIR
   bandwidths. Design method using effective medium theory and optimization
   algorithms is described in detail. The simulation results led to the
   possibility of an dual-band achromatic QWP whose retardance deviates
   from 90 degrees by <+/- 0.75 degrees with the fast axis unfixed and by
   <+/- 1.35 degrees with the fast axis fixed over MWIR(3-5 mu m) \&
   LWIR(8-12 mu m) bandwiths. (C) 2010 Optical Society of America}},
