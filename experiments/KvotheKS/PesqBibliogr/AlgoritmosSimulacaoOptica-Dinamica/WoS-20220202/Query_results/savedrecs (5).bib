@article{ WOS:000347447200004,
Author = {Chong Zhi Song and Zhao Youqun and Wang Lu},
Title = {{Tri-objective co-evolutionary algorithm and application of suspension
   parameter design based on lizard behavior bionics}},
Journal = {{JOURNAL OF MECHANICAL SCIENCE AND TECHNOLOGY}},
Year = {{2014}},
Volume = {{28}},
Number = {{12}},
Pages = {{4857-4867}},
Month = {{DEC}},
Abstract = {{Using bionic research based on three types of male side-blotched lizard
   behavior and multiple survival mechanisms, a co-evolutionary algorithm
   for tri-objective optimization is proposed. This method takes three
   design objectives as three lizards and maps the design variables as the
   lizard population's chromosomes; three types of lizard self-genetic
   factors are formed from the chromosomes. Based on these three types of
   lizard behavior, the mapping relationship between a self-adaptive
   function and the three objective functions is established. A new
   chromosome is made with the optimal genes. Based on a convergence
   condition, the optimal chromosome is obtained with multi-generation
   evolution. Considering road surface damage and ride comfort, it sets an
   acceleration RMS, with the tire's relative dynamic load and suspension's
   maximum dynamic stroke as multi-objective functions, which establishes
   the tri-objective optimization model. The calculation results show the
   effectiveness and practicability of the proposed method.}},
