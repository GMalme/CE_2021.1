@article{ WOS:A1995QK59800003,
Author = {SCHEERMESSER, T and BRYNGDAHL, O},
Title = {{THRESHOLD ACCEPTING FOR CONSTRAINED HALF-TONING}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1995}},
Volume = {{115}},
Number = {{1-2}},
Pages = {{13-18}},
Month = {{MAR 1}},
Abstract = {{The Threshold Accepting Algorithm allows an optimization with regard to
   a scalar cost function. In this paper we present its application to
   digital halftoning. In a typical situation, a binary image is printed or
   displayed and viewed by an observer. To adapt the halftoning process to
   this situation, we consider the properties of an imaging system in the
   spectral domain and derive a cost function. This cost function is used
   to generate binary images well adapted to this situation. Further, we
   describe a way to control several cost functions simultaneously and give
   an example by adding edge enhancement to the halftoning process.}},
