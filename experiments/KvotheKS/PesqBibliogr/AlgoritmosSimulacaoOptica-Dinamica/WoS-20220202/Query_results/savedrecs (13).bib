@article{ WOS:000331674300011,
Author = {Enayatifar, Rasul and Abdullah, Abdul Hanan and Isnin, Ismail Fauzi},
Title = {{Chaos-based image encryption using a hybrid genetic algorithm and a DNA
   sequence}},
Journal = {{OPTICS AND LASERS IN ENGINEERING}},
Year = {{2014}},
Volume = {{56}},
Pages = {{83-93}},
Month = {{MAY}},
Abstract = {{The paper studies a recently developed evolutionary-based image
   encryption algorithm. A novel image encryption algorithm based on a
   hybrid model of deoxyribonucleic acid (DNA) masking, a genetic algorithm
   (GA) and a logistic map is proposed. This study uses DNA and logistic
   map functions to create the number of initial DNA masks and applies GA
   to determine the best mask for encryption. The significant advantage of
   this approach is improving the quality of DNA masks to obtain the best
   mask that is compatible with plain images. The experimental results and
   computer simulations both confirm that the proposed scheme not only
   demonstrates excellent encryption but also resists various typical
   attacks. (C) 2013 Elsevier Ltd. All rights reserved.}},
