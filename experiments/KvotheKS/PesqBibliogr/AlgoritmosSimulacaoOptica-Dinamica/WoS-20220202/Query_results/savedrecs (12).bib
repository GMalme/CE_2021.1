@article{ WOS:000074260200024,
Author = {Takasago, K and Takekawa, M and Suzuki, M and Komori, K and Kannari, F},
Title = {{Evaluation of femtosecond pulse shaping with low-loss phase-only masks}},
Journal = {{IEEE JOURNAL OF SELECTED TOPICS IN QUANTUM ELECTRONICS}},
Year = {{1998}},
Volume = {{4}},
Number = {{2}},
Pages = {{346-352}},
Month = {{MAR-APR}},
Note = {{Meeting on Ultrafast Optics, MONTEREY, CALIFORNIA, 1997}},
Abstract = {{We numerically and experimentally evaluate the performance of
   programmable femtosecond pulse shaping with specified time-dependent
   amplitude and phase profiles using phase-only masks. With the masks that
   have discrete phase levels of greater than or equal to 4 designed by
   simulated annealing optimization algorithm, various pulses with
   arbitrarily specified amplitudes, pulsewidths, and pulse intervals are
   obtained with high accuracy. However, when specifying temporal phase
