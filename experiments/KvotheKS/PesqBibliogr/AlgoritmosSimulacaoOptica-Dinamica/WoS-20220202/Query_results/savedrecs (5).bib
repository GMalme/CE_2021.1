@article{ WOS:000444705000014,
Author = {Mao, Tianyi and Cuadros, Angela P. and Ma, Xu and He, Weiji and Chen,
   Qian and Arce, Gonzalo R.},
Title = {{Fast optimization of coded apertures in X-ray computed tomography}},
Journal = {{OPTICS EXPRESS}},
Year = {{2018}},
