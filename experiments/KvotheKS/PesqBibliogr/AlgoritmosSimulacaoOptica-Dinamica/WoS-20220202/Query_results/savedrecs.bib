@article{ WOS:000281543900021,
Author = {Ma Rong and Zhong BoWen and Liu PeiQing},
Title = {{Optimization design study of low-Reynolds-number high-lift airfoils for
   the high-efficiency propeller of low-dynamic vehicles in stratosphere}},
Journal = {{SCIENCE CHINA-TECHNOLOGICAL SCIENCES}},
Year = {{2010}},
Volume = {{53}},
