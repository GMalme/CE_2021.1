@article{ WOS:000483621400002,
Author = {Chen, Chih-Hao and Nadarajah, Siva},
Title = {{GCRO with dynamic deflated restarting for solving adjoint systems of
   equations for aerodynamic shape optimization}},
Journal = {{INTERNATIONAL JOURNAL OF NUMERICAL METHODS FOR HEAT \& FLUID FLOW}},
Year = {{2019}},
Volume = {{29}},
Number = {{7, SI}},
Pages = {{2179-2205}},
