@article{ WOS:000600667300113,
Author = {Sanchez-Gonzalez, Alberto and Grange, Benjamin and Caliot, Cyril},
Title = {{Computation of canting errors in heliostats by flux map fitting:
   experimental assessment}},
Journal = {{OPTICS EXPRESS}},
Year = {{2020}},
Volume = {{28}},
Number = {{26}},
Pages = {{39868-39889}},
Month = {{DEC 21}},
Abstract = {{In solar tower plants, thousands of heliostats reflect sunlight into a
   central receiver. Heliostats consist of a subset of mirrors called
   facets that must be perfectly oriented (i.e., canted) to concentrate as
   much solar radiation as possible. This study presents and validates the
   so-called flux map fitting technique to detect and correct canting
   errors. The computed distributions were matched to a series of images
   through an optimization algorithm. According to the sensitivity
   analysis, three images spread along a single day provide sufficient
   information for the algorithm to succeed. Using this methodology, four
   heliostats at the THEMIS research facility were recanted, thereby
   substantially increasing the optical quality in three of them. The
   procedure to infer the heliostat aimpoint was assessed. (C) 2020 Optical
   Society of America under the terms of the OSA Open Access Publishing
   Agreement}},
