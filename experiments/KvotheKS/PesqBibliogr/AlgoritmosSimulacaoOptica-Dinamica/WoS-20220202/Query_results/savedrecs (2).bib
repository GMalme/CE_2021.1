@article{ WOS:000283940800003,
Author = {Skaf, Joelle and Boyd, Stephen P.},
Title = {{Design of Affine Controllers via Convex Optimization}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
