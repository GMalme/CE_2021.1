@article{ WOS:000672721600001,
Author = {Springer, Sebastian and Haario, Heikki and Susiluoto, Jouni and Bibov,
   Aleksandr and Davis, Andrew and Marzouk, Youssef},
Title = {{Efficient Bayesian inference for large chaotic dynamical systems}},
Journal = {{GEOSCIENTIFIC MODEL DEVELOPMENT}},
