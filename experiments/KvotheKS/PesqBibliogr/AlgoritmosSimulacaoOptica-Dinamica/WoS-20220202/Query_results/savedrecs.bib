@article{ WOS:000250166700001,
Author = {Wang, Di-Bao and Hsiao, Fei-Bin and Chuang, Cheng-Hsin and Lee,
   Yung-Chun},
Title = {{Algorithm optimization in molecular dynamics simulation}},
