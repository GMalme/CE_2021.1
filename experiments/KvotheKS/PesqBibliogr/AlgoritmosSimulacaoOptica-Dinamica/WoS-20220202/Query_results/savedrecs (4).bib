@article{ WOS:000702746400016,
Author = {Yoo, Dongheon and Jo, Youngjin and Nam, Seung-Woo and Chen, Chun and
   Lee, Byoungho},
Title = {{Optimization of computer-generated holograms featuring phase randomness
   control}},
Journal = {{OPTICS LETTERS}},
Year = {{2021}},
Volume = {{46}},
Number = {{19}},
Pages = {{4769-4772}},
Month = {{OCT 1}},
Abstract = {{In this Letter, we introduce a computer-generated hologram (CGH)
   optimization technique that can control the randomness of the
   reconstructed phase. The phase randomness significantly affects the
   eyebox size and depth of field in holographic near-eye displays. Our
   proposal is to synthesize the CGH through the sum of two terms computed
   from the target scene with a random phase. We set aweighting pattern for
   summation as the optimization variable, which enables the CGH to reflect
   the random phase during optimization. We evaluate the proposed algorithm
   on single-depth and multi-depth contents, and the performance is
   validated via simulations and experiments. (C) 2021 Optical Society of
   America under the terms of the OSA Open Access Publishing Agreement}},
