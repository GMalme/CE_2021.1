@article{ WOS:000520479100039,
Author = {Wang, Gang and Liu, Jietao and Sun, Xueying and He, Shunfu and Guo,
   Chengfei and Wu, Yuxiaing and Shao, Xiaopeng},
Title = {{Pose estimation of hidden object behind a thin scattering layer based on
   speckle correlation}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2020}},
Volume = {{463}},
Month = {{MAY 15}},
Abstract = {{The presence of an inhomogeneous scattering medium has brought great
   challenges to optical imaging and detection in many fields. In this
   work, we propose a speckle-autocorrelation based orthogonal projection
   algorithm to detect the flipped target hidden behind a scattering layer
   in X-Z plane and Y-Z plane. By calculating the speckle autocorrelation,
   we can quickly identify the posture of hidden target. We also develop an
   optimization method for the tracking of rotating object in X-Y plane.
   Those methods effectively expand the tracking dimensions of hidden
   target, allowing us to detect the flipping and rotational posture of the
   object behind scattering media. The reported strategy may find
   applications in many fields such as biomedical imaging, and ego-motion
   sensing.}},
