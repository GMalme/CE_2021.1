@article{ WOS:A1994MV12000014,
Author = {HRDINA, J},
Title = {{FAST COMPUTATION OF 2ND-ORDER DERIVATIVES OF REFLECTANCE, TRANSMITTANCE
   AND CORRESPONDING PHASE-SHIFTS IN THIN-FILM SYSTEMS}},
Journal = {{JOURNAL OF MODERN OPTICS}},
Year = {{1994}},
Volume = {{41}},
Number = {{1}},
Pages = {{129-136}},
Month = {{JAN}},
Abstract = {{A new fast algorithm for the computation of the second-order derivatives
   is described. The method is applicable to the evaluation of the
   second-order derivatives of reflectance, transmittance and corresponding
   phase shifts for the light impinging on the substrate from both sides
   and with respect to any parameters of the system. The time needed for
   the computation of the whole Hessian matrix is roughly linearly
   dependent on the number of layers. The precise and fast computation of
   Hessian matrix enables one to use the optimization methods of quadratic
   rate of convergence such as modified Newton method and speed up the
   optimization routines for thin film systems.}},
