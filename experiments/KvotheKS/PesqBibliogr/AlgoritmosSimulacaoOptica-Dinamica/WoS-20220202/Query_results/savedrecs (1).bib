@article{ WOS:000386784900016,
Author = {Minervino, Mauro and Vitagliano, Pier Luigi and Quagliarella, Domenico},
Title = {{Helicopter stabilizer optimization considering rotor downwash in
   forward-flight}},
Journal = {{AIRCRAFT ENGINEERING AND AEROSPACE TECHNOLOGY}},
