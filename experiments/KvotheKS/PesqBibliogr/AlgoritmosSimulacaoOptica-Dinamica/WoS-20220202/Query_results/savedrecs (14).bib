@article{ WOS:000567570200009,
Author = {Ma, Guoteng and Hu, Chunguang and Li, Shuai and Gao, Xiaoqin and Li,
   Hongbin and Hu, Xiaotang},
Title = {{Axial displacement calibration and tracking of optically trapped beads}},
Journal = {{OPTICS AND LASERS IN ENGINEERING}},
Year = {{2020}},
Volume = {{134}},
Month = {{NOV}},
Abstract = {{High-precision axial displacement tracking of trapped beads is an
   indispensable feature of optical tweezers in advanced single-molecule
   studies. Here, we demonstrate an alternative method that enables axial
   calibration and tracking to be carried out on the same sample to avoid
   unnecessary errors. This method works by applying a dynamic force
   balance on a bead trapped between a piezoelectrically driven glass slide
   and an optical trap; in this configuration, the bead can be stopped
   precisely in different positions and imaged by a camera. A simple
   gradient algorithm is used to process the images into calibration data.
   After optimization of the calibration method and samples, our method
   exhibited better than 5 nm experimental axial resolution, with a
   measurement range of +/-500 nm around the objective focus at video
   speed. Moreover, for the first time, the deviation of the focusing plane
   in dual-trap optical tweezers was measured. We confirmed the axial
   deviation between two optical traps in our setup to be similar to 10 nm,
   corresponding to a force spectroscopy gage error of similar to 1 pN.
   This approach offers a favorable solution for in-use setup updating, as
   it can be seamlessly integrated into any optical tweezers system without
   requiring new hardware updates.}},
Publisher = {{ELSEVIER SCI LTD}},
Address = {{THE BOULEVARD, LANGFORD LANE, KIDLINGTON, OXFORD OX5 1GB, OXON, ENGLAND}},
Type = {{Article}},
Language = {{English}},
