@article{ WOS:000264530700010,
Author = {El Zahab, Z. and Divo, E. and Kassab, A.},
Title = {{A meshless CFD approach for evolutionary shape optimization of bypass
   grafts anastomoses}},
Journal = {{INVERSE PROBLEMS IN SCIENCE AND ENGINEERING}},
