@article{ WOS:000394309200041,
Author = {Dorrer, C. and Hassett, J.},
Title = {{Model-based optimization of near-field binary-pixelated beam shapers}},
Journal = {{APPLIED OPTICS}},
Year = {{2017}},
Volume = {{56}},
Number = {{4}},
Pages = {{806-815}},
Month = {{FEB 1}},
Abstract = {{The optimization of components that rely on spatially dithered
   distributions of transparent or opaque pixels and an imaging system with
   far-field filtering for transmission control is demonstrated. The
   binary-pixel distribution can be iteratively optimized to lower an error
   function that takes into account the design transmission and the
   characteristics of the required far-field filter. Simulations using a
   design transmission chosen in the context of high-energy lasers show
   that the beam-fluence modulation at an image plane can be reduced by a
   factor of 2, leading to performance similar to using a non-optimized
   spatial-dithering algorithm with pixels of size reduced by a factor of 2
   without the additional fabrication complexity or cost. The optimization
   process preserves the pixel distribution statistical properties.
   Analysis shows that the optimized pixel distribution starting from a
   high-noise distribution defined by a random-draw algorithm should be
   more resilient to fabrication errors than the optimized pixel
   distributions starting from a low-noise, error-diffusion algorithm,
   while leading to similar beamshaping performance. This is confirmed by
   experimental results obtained with various pixel distributions and
   induced fabrication errors. (C) 2017 Optical Society of America}},
Publisher = {{OPTICAL SOC AMER}},
Address = {{2010 MASSACHUSETTS AVE NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Dorrer, C (Corresponding Author), Univ Rochester, Laser Energet Lab, 250 East River Rd, Rochester, NY 14623 USA.
   Dorrer, C.; Hassett, J., Univ Rochester, Laser Energet Lab, 250 East River Rd, Rochester, NY 14623 USA.}},
