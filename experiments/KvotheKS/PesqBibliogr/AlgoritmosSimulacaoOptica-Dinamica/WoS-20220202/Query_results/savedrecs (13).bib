@article{ WOS:000074519700019,
Author = {McDavid, RM and Dantzig, JA},
Title = {{Fluid flow in casting rigging systems: Modeling, validation, and optimal
   design}},
Journal = {{METALLURGICAL AND MATERIALS TRANSACTIONS B-PROCESS METALLURGY AND
   MATERIALS PROCESSING SCIENCE}},
