@article{ WOS:000636612700051,
Author = {Kiel, Thomas and Varytis, Paris and Beverungen, Bettina and Kristensen,
   Philip Trost and Busch, Kurt},
Title = {{Enhanced Faraday rotation by dielectric metasurfaces with Bayesian
   shape-optimized scatterers}},
Journal = {{OPTICS LETTERS}},
Year = {{2021}},
Volume = {{46}},
Number = {{7}},
Pages = {{1720-1723}},
Month = {{APR 1}},
Abstract = {{In this Letter, we demonstrate howto optimize the magnetooptic response
   of a Huygens metasurface composed of square arrays of all-dielectric
   nano-disk scatterers. We compare cylindrical and shape-modified disks.
   Both provide a strongly enhanced Faraday rotation that is accompanied by
   almost 100\% transmittance. The shape modification obtained via a
   Bayesian optimization algorithm results in a 50\% increase in the
   magneto-optic response compared to the best cylindrical disk, providing
   15 degrees of polarization rotation for a 260 nm thick metasurface. (C)
   2021 Optical Society of America.}},
