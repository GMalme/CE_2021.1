@article{ WOS:000694689900001,
Author = {Salamat, Babak and Letizia, Nunzio A. and Tonello, Andrea M.},
Title = {{Control Based Motion Planning Exploiting Calculus of Variations and
   Rational Functions: A Formal Approach}},
Journal = {{IEEE ACCESS}},
Year = {{2021}},
Volume = {{9}},
Pages = {{121716-121727}},
Abstract = {{This paper presents a control-based trajectory generation approach for
   unmanned aerial vehicles (UAVs) under dynamic constraints. It exploits
   the concept of optimal control to find closed-form differential
   equations that satisfy any arbitrary dynamic limitation mapped into
   kinematic constraints. Pontryagin's Minimum Principle applies to derive
   a set of differential equations in which the dynamic environment is
   considered in the constrained Hamiltonian function. In particular, we
   aim to minimize the L-2-norm of the control input avoiding dynamic
   obstacles, given initial and final boundary conditions. Lastly, this
   paper proposes a novel interpolation algorithm based on rational
   functions, referred to as rational recursive smooth trajectory (RRST)
   method. The method generates an analytic expression that approximates
   the control inputs, for which no closed-form solutions are in general
   attainable.}},
Publisher = {{IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC}},
Address = {{445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Salamat, B (Corresponding Author), Univ Klagenfurt, Chair Embedded Commun Syst, A-9020 Klagenfurt, Austria.
   Salamat, Babak; Letizia, Nunzio A.; Tonello, Andrea M., Univ Klagenfurt, Chair Embedded Commun Syst, A-9020 Klagenfurt, Austria.}},
DOI = {{10.1109/ACCESS.2021.3109125}},
ISSN = {{2169-3536}},
Keywords = {{Trajectory; Interpolation; Optimal control; Vehicle dynamics; Dynamical
   systems; Aerodynamics; Unmanned aerial vehicles; Aerospace; optimal
   control; calculus of variations; steepest descent; function
