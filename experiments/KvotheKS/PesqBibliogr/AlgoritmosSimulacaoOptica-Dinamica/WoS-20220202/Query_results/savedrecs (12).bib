@article{ WOS:000180433600005,
Author = {Goudail, F and Roux, N and Refregier, P},
Title = {{Performance parameters for detection in low-flux coherent images}},
Journal = {{OPTICS LETTERS}},
Year = {{2003}},
Volume = {{28}},
Number = {{2}},
Pages = {{81-83}},
Month = {{JAN 15}},
Abstract = {{We analyze the optimization of low-flux coherent active imagery systems
   for target detection. We demonstrate that, unlike the Fisher ratio, the
   Bhattacharyya distance is an efficient figure of merit when one uses
   detection algorithms based on the generalized likelihood ratio test for
   realistic situations when the target and the background mean values are
   unknown. For example, we show that detection capabilities can be better
   if the pulse energy is divided into four shots, whereas using more than
   ten shots does not significantly improve the results. (C) 2003 Optical
   Society of America.}},
