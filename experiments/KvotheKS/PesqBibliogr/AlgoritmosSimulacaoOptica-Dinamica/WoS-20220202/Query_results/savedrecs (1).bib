@article{ WOS:000683569900010,
Author = {Courtais, Alexis and Lesage, Francois and Privat, Yannick and Pelaingre,
   Cyril and Latifi, Abderrazak M.},
Title = {{Optimal design of a structured fixed-bed reactor using geometry
   optimization and Stratoconception printing process}},
Journal = {{COMPUTERS \& CHEMICAL ENGINEERING}},
Year = {{2021}},
Volume = {{153}},
Month = {{OCT}},
Abstract = {{The aim of this paper is to determine the shape of a fixed-bed reactor
   which maximizes the conversion rate under the constraints of process
   model equations (i.e. continuity, Navier-Stokes, and mass balance
   equations), energy dissipation, iso-volume, and manufacturing.
   Incompressible fluid, laminar flow regime and steady-state conditions in
   the reactor are the main assumptions taken into account. The
   optimization method developed is based on the adjoint system method and
   OpenFOAM framework is used as CFD solver to compute the state vector and
   its adjoint variables introduced by the optimization approach. The
   algorithm developed is then tested on two different cases, a reactor
   where a first order homogeneous reaction takes place and another one
   involving a surface reaction. The optimization results show a
   significant improvement of the conversion rate by 2.7\% in the first
   case, and by 16\% in the second one. Finally, initial and optimal shapes
   are manufactured using a 3D printing technique. (c) 2021 Elsevier Ltd.
   All rights reserved.}},
