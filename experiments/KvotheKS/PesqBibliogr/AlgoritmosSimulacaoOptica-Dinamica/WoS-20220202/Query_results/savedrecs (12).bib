@article{ WOS:000369368700026,
Author = {He, Zhenzong and Qi, Hong and Lew, Zhongyuan and Ruan, Liming and Tan,
   Heping and Luo, Kun},
Title = {{Application of the LSQR algorithm in non-parametric estimation of
   aerosol size distribution}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2016}},
Volume = {{366}},
Pages = {{154-162}},
Month = {{MAY 1}},
Abstract = {{Based on the Least Squares QR decomposition (LSQR) algorithm, the
   aerosol size distribution (ASD) is retrieved in non-parametric approach.
   The direct problem is solved by the Anomalous Diffraction Approximation
   (ADA) and the Lambert-Beer Law. An optimal wavelength selection method
   is developed to improve the retrieval accuracy of the ASD. The proposed
   optimal wavelength set is selected by the method which can make the
   measurement signals sensitive to wavelength and decrease the degree of
   the ill-condition of coefficient matrix of linear systems effectively to
   enhance the anti-interference ability of retrieval results. Two common
   kinds of monomodal and bimodal ASDs, log-normal (L-N) and Gamma
   distributions, are estimated, respectively. Numerical tests show that
   the LSQR algorithm can be successfully applied to retrieve the ASD with
   high stability in the presence of random noise and low susceptibility to
   the shape of distributions. Finally, the experimental measurement ASD
   over Harbin in China is recovered reasonably. All the results confirm
   that the LSQR algorithm combined with the optimal wavelength selection
   method is an effective and reliable technique in non-parametric
   estimation of ASD. (C) 2015 Elsevier B.V. All rights reserved.}},
