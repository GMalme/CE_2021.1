@article{ WOS:000593707900014,
Author = {Shi, Lan-Ting and Hu, Cui-E and Munoz, Alfonso and Ji, Lin-Xiang and
   Huang, Yao-Yao and Wei, Yong-Kai and Ji, Guang-Fu and Cheng, Yan},
Title = {{The structural phase transition of ammonia borane under high pressure}},
Journal = {{INTERNATIONAL JOURNAL OF HYDROGEN ENERGY}},
Year = {{2020}},
Volume = {{45}},
Number = {{58}},
Pages = {{33047-33058}},
Month = {{NOV 27}},
Abstract = {{We have systematically studied the high-pressure (up to 100 GPa)
   behavior of ammonia borane at 0 K using particle optimization algorithm
   combined with the first principles calculation. The long-unsolved
   high-pressure structures and a complete phase transition sequence were
   obtained: P6(3)mc -> Pmn2(1)-> P2(1)(Z = 2)-> P2(1)(Z = 4)-> C2/m ->
   P2(1)/m, and the phase transition pressures are 1.23, 2.77, 11.9, 13.8,
   22.34 GPa, respectively. The lack of imaginary phonon frequency
   effectively proves the rationality and dynamic stability of the above
   structures. The Raman modes calculated by density functional theory
   (DFT) reveals the evolution of the vibration mode under pressure, which
   is in good agreement with the experiment results. The analysis of
   dihydrogen bonds in ammonia borane shows the bonding mechanism of
   different high-pressure phase structures. Our research not only enriches
   the high-pressure phase structure of the ammonia borane system but also
   provides essential information for hydrogen storage materials. (c) 2020
   Hydrogen Energy Publications LLC. Published by Elsevier Ltd. All rights
   reserved.}},
