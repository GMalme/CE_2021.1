@article{ WOS:000377396300018,
Author = {Ma, Tao and Hu, Xiaoping and Zhang, Lilian and He, Xiaofeng},
Title = {{Calibration of a polarization navigation sensor using the NSGA-II
   algorithm}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2016}},
Volume = {{376}},
Pages = {{107-114}},
Month = {{OCT 1}},
Abstract = {{A bio-inspired polarization navigation sensor is designed based on the
   polarization sensitivity mechanisms of insects. A new calibration model
   by formulating the calibration problem as a multi-objective optimization
   problem is presented. Unlike existing calibration models, the proposed
   model makes the calibration problem well-posed. The calibration
   parameters are optimized through Non-dominated Sorting Genetic
   Algorithm-II (NSGA-II) approach to minimize both angle of polarization
   (AOP) residuals and degree of linear polarization (DOLP) dispersions.
   The results of simulation and experiments show that the proposed
   algorithm is more stable than the compared methods for the calibration
   applications of polarization navigation sensors. (c) 2016 Elsevier B.V.
   All rights reserved.}},
