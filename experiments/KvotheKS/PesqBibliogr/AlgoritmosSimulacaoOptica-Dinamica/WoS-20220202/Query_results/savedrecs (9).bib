@article{ WOS:000595910800005,
Author = {Zhao, Yongjun and Lu, Cheng and Fei, Chengwei and An, Liqiang and Hu,
   Yan and Huang, Bo and Yuan, Liu},
Title = {{Transient Reliability Evaluation Approach of Flexible Mechanism with
   GA-Extremum Neural Network}},
Journal = {{MATHEMATICAL PROBLEMS IN ENGINEERING}},
Year = {{2020}},
Volume = {{2020}},
Month = {{NOV 16}},
Abstract = {{Efficient analytical model directly enhances the reliability evaluation
   of flexible mechanism under operation. In this paper, genetic
   algorithm-based extremum neural network (GA-ENN) is developed as
   reliability model by introducing the thoughts of extremum and genetic
   algorithm (GA) into artificial neural network to address the key
   problems comprising transient response and modeling precision in the
   dynamic reliability analysis of flexible mechanism in a time domain. The
   thought of extremum is adopted to simplify transient response process as
   one extremum value to the difficulty of dynamic reliability analysis
   induced by transient process response, and the GA is applied to find the
   optimal model parameters of reliability model. The dynamic reliability
   analysis of two-link flexible robot manipulator (TFRM) (a typical
   flexible mechanism) was implemented based on the GA-ENN method,
   regarding the input random variables of material density, elastic
   modulus, section sizes of components, and the output response of
   components' deformations. From the analysis, the comprehensive
   reliability of the TFRM is 0.951 when the allowable deformation is 1.8 x
   10(-2) m. Besides, the maximum deformations of the two components follow
