@article{ WOS:000519955600001,
Author = {Foust, Rebecca and Chung, Soon-Jo and Hadaegh, Fred Y.},
Title = {{Optimal Guidance and Control with Nonlinear Dynamics Using Sequential
   Convex Programming}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
Year = {{2020}},
Volume = {{43}},
Number = {{4}},
Pages = {{633-644}},
Month = {{APR}},
Note = {{AIAA Science and Technology Forum and Exposition (SciTech), San Diego,
   CA, JAN 07-11, 2019}},
Organization = {{AIAA}},
Abstract = {{This paper presents a novel method for expanding the use of sequential
   convex programming (SCP) to the domain of optimal guidance and control
   problems with nonlinear dynamics constraints. SCP is a useful tool in
   obtaining real-time solutions to direct optimal control, but it is
   unable to adequately model nonlinear dynamics due to the linearization
   and discretization required. As nonlinear program solvers are not yet
   functioning in real-time, a tool is needed to bridge the gap between
   satisfying the nonlinear dynamics and completing execution fast enough
   to be useful. Two methods are proposed, SCP with nonlinear dynamics
   correction (SCPn) and modified SCPn (M-SCPn), which mixes SCP and SCPn
   to reduce runtime and improve algorithmic robustness. Both methods are
   proven to generate optimal state and control trajectories that satisfy
   the nonlinear dynamics. Simulations are presented to validate the
   efficacy of the methods as compared with SCP.}},
