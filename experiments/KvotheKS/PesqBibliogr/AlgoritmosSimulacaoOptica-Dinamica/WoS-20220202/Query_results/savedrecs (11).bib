@article{ WOS:000583718000026,
Author = {Du, Hubing and He, Zhouxuan and Ma, Pu and Chen, Xiaodong and Yin, Peili},
Title = {{Phase-shift extraction of multiple-frame randomly phase-shifted
   interferograms by analysis of the amplitude of the analytic signal}},
Journal = {{APPLIED OPTICS}},
Year = {{2020}},
Volume = {{59}},
Number = {{31}},
Pages = {{9844-9849}},
Month = {{NOV 1}},
Abstract = {{We present a powerful phase-shift extraction algorithm for
   multiple-frame random phase-shifting fringe patterns. The proposed
   method is based on changing the regularity of the amplitude of a
   demodulated analytic signal with respect to different phase shifts and a
   one-dimensional optimization method. Compared with the existing
   universal phase-reconstruction method, the proposed method is accurate,
   stable, and efficient. Both numerical simulations and experimental data
   demonstrate the high accuracy and efficiency of the proposed method. (C)
