@article{ WOS:000745524600005,
Author = {Singh, Shubham and Russell, Ryan P. and Wensing, Patrick M.},
Title = {{Efficient Analytical Derivatives of Rigid-Body Dynamics Using Spatial
   Vector Algebra}},
Journal = {{IEEE ROBOTICS AND AUTOMATION LETTERS}},
