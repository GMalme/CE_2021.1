@article{ WOS:000470022500036,
Author = {Guo, Fangliang and Yan, Ying and Hong, Yang and Tian, Ziyang and Li, Jie},
Title = {{Prediction and optimization design for thermal expansion coefficients of
   three-dimensional n-directional-braided composites}},
