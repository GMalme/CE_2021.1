@article{ WOS:000505990000007,
Author = {Wu, Chenxue and Chen, Jiajia and Zhang, Biwei and Zheng, Yao and Zhu,
   Xinpei and Si, Ke and Gong, Wei},
Title = {{Multiple guide stars optimization in conjugate adaptive optics for deep
   tissue imaging}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2020}},
Volume = {{459}},
Month = {{MAR 15}},
Abstract = {{Adaptive optics (AO) has been widely used in optical microscopy to
   recover high-resolution images in deep tissue. However, in conventional
   AO systems, the corrected field of view (FOV) of a single guide star is
   usually quite limited. Here we demonstrate a conjugate AO system based
   on automatic optimal multiple guide stars selection algorithm to achieve
   large effective corrected FOV with a small number of guide stars. For a
   random phase mask as the scattering medium, the effective correction
   coverage ratio can be increased to similar to 5.09 times than that in a
   conventional CAO system. For a mouse brain slice with 117 pm thickness,
   the effective corrected FOV is larger than that of conventional CAO
   system by a factor of similar to 2.58. Therefore, our method shows
   potentials in aberration correction with large FOV for deep tissue
   imaging}},
