@article{ WOS:000575083500001,
Author = {Chen, Elin and Chen, Junfu and Mohamed, Ali Wagdy and Wang, Bi and Wang,
   Zhendong and Chen, Yang},
Title = {{Swarm Intelligence Application to UAV Aided IoT Data Acquisition
   Deployment Optimization}},
Journal = {{IEEE ACCESS}},
