@article{ WOS:000278465300022,
Author = {Treeby, Bradley E. and Cox, B. T.},
Title = {{k-Wave: MATLAB toolbox for the simulation and reconstruction of
   photoacoustic wave fields}},
Journal = {{JOURNAL OF BIOMEDICAL OPTICS}},
