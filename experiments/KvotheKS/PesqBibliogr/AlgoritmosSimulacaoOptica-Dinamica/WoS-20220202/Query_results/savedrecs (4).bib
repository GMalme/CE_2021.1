@article{ WOS:000298726000006,
Author = {Byun, Wanil and Kim, Min Ki and Park, Kook Jin and Kim, Seung Jo and
   Chung, Minho and Cho, Jin Yeon and Park, Sung-Han},
Title = {{Buckling analysis and optimal structural design of supercavitating
   vehicles using finite element technology}},
Journal = {{INTERNATIONAL JOURNAL OF NAVAL ARCHITECTURE AND OCEAN ENGINEERING}},
