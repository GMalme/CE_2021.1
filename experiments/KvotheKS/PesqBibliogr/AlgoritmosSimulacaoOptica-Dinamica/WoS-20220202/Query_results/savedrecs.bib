@article{ WOS:000515907500002,
Author = {Chen, Wengang and Zhang, Weiwei and Liu, Yilang and Kou, Jiaqing},
Title = {{Accelerating the convergence of steady adjoint equations by dynamic mode
   decomposition}},
Journal = {{STRUCTURAL AND MULTIDISCIPLINARY OPTIMIZATION}},
Year = {{2020}},
Volume = {{62}},
Number = {{2}},
Pages = {{747-756}},
Month = {{AUG}},
Abstract = {{To improve the efficiency of adjoint-based optimization algorithms,
   dynamic mode decomposition (DMD) technology is used to accelerate the
   convergence of steady adjoint equations in this paper. During
   pseudo-time marching, adjoint fields are projected onto the modal space
   and modal analysis is carried out by DMD. When only first zero frequency
   mode is reserved, the convergence speed of adjoint equations is
   significantly enhanced. Through two examples of flow past airfoils in
   subsonic and transonic flows and a transonic optimization example, the
   validity of the proposed methodology is verified. Results indicate that
   the proposed methodology can improve optimization efficiency remarkably,
   and the iteration step number of adjoint equations is reduced by almost
   60\%.}},
