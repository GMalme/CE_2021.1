@article{ WOS:000358164200001,
Author = {Kang, Hyuk and Park, F. C.},
Title = {{Motion optimization using Gaussian process dynamical models}},
Journal = {{MULTIBODY SYSTEM DYNAMICS}},
Year = {{2015}},
Volume = {{34}},
Number = {{4}},
Pages = {{307-325}},
Month = {{AUG}},
Abstract = {{We propose an efficient method for generating suboptimal motions for
   multibody systems using Gaussian process dynamical models. Given a
   dynamical model for a multibody system, and a trial motion, a
   lower-dimensional Gaussian process dynamical model is fitted to the
   trial motion. New motions are then generated by performing a dynamic
   optimization in the lower-dimensional space. We introduce the notion of
   variance tubes as an intuitive and efficient means of restricting the
   optimization search space. The performance of our algorithm is evaluated
   through detailed case studies of raising motions for an arm and jumping
   motions for a humanoid.}},
