@article{ WOS:A1996VA99500005,
Author = {Dresel, T},
Title = {{Optimization of symmetric Dammann gratings with a multidimensional error
   feedback algorithm}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1996}},
Volume = {{129}},
Number = {{1-2}},
Pages = {{19-26}},
Month = {{AUG 1}},
Abstract = {{A multidimensional control algorithm or multidimensional error feedback
   algorithm for the solution of nonlinear equation sets is introduced. It
   allows the direct optimization of symmetric Dammann gratings without
   introducing a figure of merit as required by simulated annealing,
   gradient search, etc. It has fast convergence properties and produces
   all possible symmetric Dammann solutions for a given number of
   diffraction orders with certainty. Thus, an alternative solution to a
   problem thoroughly investigated in the past is provided which treats the
   design of diffraction gratings from another mathematical point of view.}},
