Title = {{Impact of temporal variations in vegetation optical depth and vegetation
   temperature on L-band passive soil moisture retrievals over a tropical
   forest using in-situ information}},
Journal = {{INTERNATIONAL JOURNAL OF REMOTE SENSING}},
Year = {{2020}},
Volume = {{41}},
