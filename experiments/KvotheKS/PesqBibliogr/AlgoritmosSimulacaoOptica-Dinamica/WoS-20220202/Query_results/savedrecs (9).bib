@article{ WOS:000473748100035,
Author = {Wang, Liangjun and Zhang, Xiaoxiao and Zhu, Wenhao and Xu, Kangle and
   Wu, Weiguo and Chu, Xuesen and Zhang, Wu},
Title = {{Accurate Computation of Airfoil Flow Based on the Lattice Boltzmann
   Method}},
Journal = {{APPLIED SCIENCES-BASEL}},
Year = {{2019}},
Volume = {{9}},
Number = {{10}},
Month = {{MAY 2}},
Abstract = {{The lattice Boltzmann method (LBM) is an important numerical algorithm
   for computational fluid dynamics. This study designs a two-layer
   parallel model for the Sunway TaihuLight supercomputer SW26010 many-core
   processor, which implements LBM algorithms and performs optimization.
   Numerical experiments with different problem sizes proved that the
   proposed model has better parallel performance and scalability than
   before. In this study, we performed numerical simulations of the flows
   around the two-dimensional (2D) NACA0012 airfoil, and the results of a
   series of flows around the different angles of attack were obtained. The
   results of the pressure coefficient and lift coefficient were in good
   agreement with those in the literature.}},
