Title = {{Fuzzy sliding mode control for skid-to-turn missiles based on
   output-redefinition}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2005}},
Volume = {{12}},
Number = {{S}},
Pages = {{162-171}},
