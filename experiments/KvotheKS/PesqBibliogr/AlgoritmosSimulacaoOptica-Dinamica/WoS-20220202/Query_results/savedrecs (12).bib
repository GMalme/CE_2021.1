@article{ WOS:000185584600005,
Author = {Ymeti, A and Kanger, JS and Greve, J and Lambeck, PV and Wijn, R and
   Heideman, RG},
Title = {{Realization of a multichannel integrated Young interferometer chemical
   sensor}},
Journal = {{APPLIED OPTICS}},
Year = {{2003}},
Volume = {{42}},
Number = {{28}},
Pages = {{5649-5660}},
Month = {{OCT 1}},
Abstract = {{We report on the design, realization, and characterization of a
   four-channel integrated optical Young interferometer device that enables
   simultaneous and independent monitoring of three binding processes. The
   generated interference pattern is recorded by a CCD camera and analyzed
   with a fast-Fourier-transform algorithm. We present a thorough
   theoretical analysis of such a device. The realized device is tested by
   monitoring glucose solutions that induce well defined phase changes
   between output channels. The simultaneous measurement of three different
   glucose concentrations shows the multipurpose feature of such devices.
   The observed errors, caused by the mismatching of spatial frequencies of
   individual interference patterns with those determined from the CCD
   camera, are reduced with different reduction schemes. The phase
   resolution for different pairs of channels was similar to1 x 10(-4)
   fringes, which corresponds to a refractive-index resolution of similar
   to8.5 x 10(-8). The measured sensitivity coefficient of the phase change
   versus refractive-index change of similar to1.22 x 10(3) x 2pi agrees
   well with the calculated coefficient of similar to1.20 x 10(3) x 2pi.
   (C) 2003 Optical Society of America.}},
