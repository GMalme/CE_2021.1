@article{ WOS:000441174200085,
Author = {de Carvalho, Luiz Melk and de Lima Monteiro, Davies William and de
   Oliveira Costa, Rodolfo Felipe},
Title = {{On how to design IOLs more robust to tilt caused by fabrication or
   implantation: A case study}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2018}},
Volume = {{427}},
Pages = {{535-544}},
Month = {{NOV 15}},
Abstract = {{We present an intraocular lens (IOL) design that renders it more
   compliant to actual conditions where tilt may be present in the
   fabricated lens or the implanted locus. When an IOL is implanted in the
   eye some degree of tilt is commonly observed, negatively affecting the
   retinal image (Eppig et al., 2009). The distinguishing feature of the
   proposed design method is the use of a tilted lens as reference, with a
   genetic algorithm (NSGA-II) applied and followed by a Damped Least
   Square (DLS) optimization. It is compared to the classical IOL
   optimization based on the DLS applied to a perfectly aligned lens. The
   optimized IOL was evaluated considering the specifications of ISO and
   Liou and Brennan eye models. The image resolution (MTF curves) of the
   resulting IOL is analyzed for both to compare performance. It is
   demonstrated that in the Liou and Brennan eye model the visual acuity is
   comparable for both design methods when the lens is aligned to the
   optical axis, however, when tilt is present the proposed method presents
   a significant benefit in relation to the classical method. This approach
   can be used for a more robust design of any lens system at no additional
   cost.}},
