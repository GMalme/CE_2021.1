@article{ WOS:000337660100035,
Author = {Govender, Nicolin and Wilke, Daniel N. and Kok, Schalk and Els, Rosanne},
Title = {{Development of a convex polyhedral discrete element simulation framework
   for NVIDIA Kepler based GPUs}},
