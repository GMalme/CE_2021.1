@article{ WOS:000278265600015,
Author = {Poudel, Milan P. and Kolomenskii, Alexandre A. and Schuessler, Hans A.},
Title = {{Two-photon fluorescence of Coumarin 30 excited by optimally shaped
   pulses}},
Journal = {{APPLIED OPTICS}},
Year = {{2010}},
Volume = {{49}},
Number = {{16}},
Pages = {{3075-3081}},
Month = {{JUN 1}},
Abstract = {{We optimized the two-photon fluorescence (TPF) of a Coumarin 30 dye by
   using a feedback-controlled femtosecond pulse shaping technique. For
   optimization we implemented an evolutionary algorithm with a liquid
   crystal phase-only pulse shaper in a folded 4f setup. The optimization
   procedure applied to the second harmonic generation, and TPF noticeably
   improved the output signals and demonstrated good convergence. In
   addition, signal ratios involving TPF and second harmonic generation
   (SHG) were successfully optimized. The correlation between TPF and SHG
   was studied, and it was found to decrease when the pulse shape was close
   to the optimum. These experimental results are of interest for potential
   applications of coherent control to complex molecular systems as well as
   in biomedical imaging. (c) 2010 Optical Society of America}},
