@article{ WOS:000297683800022,
Author = {Anselmi, Francesca and Ventalon, Cathie and Begue, Aurelien and Ogden,
   David and Emiliani, Valentina},
Title = {{Three-dimensional imaging and photostimulation by remote-focusing and
   holographic light patterning}},
Journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA}},
Year = {{2011}},
Volume = {{108}},
Number = {{49}},
Pages = {{19504-19509}},
Month = {{DEC 6}},
Abstract = {{Access to three-dimensional structures in the brain is fundamental to
   probe signal processing at multiple levels, from integration of synaptic
   inputs to network activity mapping. Here, we present an optical method
   for independent three-dimensional photoactivation and imaging by
   combination of digital holography with remote-focusing. We
   experimentally demonstrate compensation of spherical aberration for
   out-of-focus imaging in a range of at least 300 mu m, as well as
   scanless imaging along oblique planes. We apply this method to perform
   functional imaging along tilted dendrites of hippocampal pyramidal
   neurons in brain slices, after photostimulation by multiple spots
   glutamate uncaging. By bringing extended portions of tilted dendrites
   simultaneously in-focus, we monitor the spatial extent of dendritic
   calcium signals, showing a shift from a widespread to a spatially
   confined response upon blockage of voltage-gated Na+ channels.}},
