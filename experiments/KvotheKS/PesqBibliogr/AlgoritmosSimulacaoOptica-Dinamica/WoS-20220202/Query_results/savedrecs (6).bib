@article{ WOS:000369045900016,
Author = {Zhao Bo and Xiang Shuang and An Yang and Tao Wei},
Title = {{DPTSV: A Dynamic Priority Task Scheduling Strategy for TSS Deadlock
   Based on Value Evaluation}},
