@article{ WOS:000477647200002,
Author = {Ghazi, Georges and Botez, Ruxandra Mihaela},
Title = {{Identification and Validation of an Engine Performance Database Model
   for the Flight Management System}},
Journal = {{JOURNAL OF AEROSPACE INFORMATION SYSTEMS}},
