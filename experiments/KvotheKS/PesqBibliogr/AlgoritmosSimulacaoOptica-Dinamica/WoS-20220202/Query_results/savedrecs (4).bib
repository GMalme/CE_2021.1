@article{ WOS:000406392600018,
Author = {Liu, A. and Bross, A. and Neuffer, D.},
Title = {{A FODO racetrack ring for nuSTORM: design and optimization}},
Journal = {{JOURNAL OF INSTRUMENTATION}},
Year = {{2017}},
Volume = {{12}},
Month = {{JUL}},
Abstract = {{The goal of nuSTORM is to provide well-defined neutrino beams for
   precise measurements of neutrino cross-sections and oscillations. The
   nuSTORM decay ring is a compact racetrack storage ring with a
   circumference of similar to 480m that incorporates large aperture (60 cm
   diameter) magnets. There are many challenges in the design. In order to
   incorporate the Orbit Combination section (OCS), used for injecting the
   pion beam into the ring, a dispersion suppressor is needed adjacent to
   the OCS. Concurrently, in order to maximize the number of useful muon
   decays, strong bending dipoles are needed in the arcs to minimize the
   arc length. These dipoles create strong chromatic effects, which need to
   be corrected by nonlinear sextupole elements in the ring. In this paper,
   a FODO racetrack ring design and its optimization using sextupolar
   fields via both a Genetic Algorithm (GA) and a Simulated Annealing (SA)
   algorithm will be discussed.}},
