@article{ WOS:000474488400024,
Author = {Nieto, Michelle Guzman and ElSayed, Mostafa S. A. and Walch, Denis},
Title = {{Efficient global optimization and modal strain energy coefficient-based
   algorithm for fast prediction of dynamic aeroelastic loads}},
