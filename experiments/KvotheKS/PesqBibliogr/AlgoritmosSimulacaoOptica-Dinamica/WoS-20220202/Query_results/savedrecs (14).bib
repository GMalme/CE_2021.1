@article{ WOS:000300082100014,
Author = {Lin, Chenxi and Huang, Ningfeng and Povinelli, Michelle L.},
Title = {{Effect of aperiodicity on the broadband reflection of silicon nanorod
   structures for photovoltaics}},
Journal = {{OPTICS EXPRESS}},
Year = {{2012}},
Volume = {{20}},
Number = {{1}},
Pages = {{A125-A132}},
Month = {{JAN 2}},
Abstract = {{We carry out a systematic numerical study of the effects of aperiodicity
   on silicon nanorod anti-reflection structures. We use the scattering
   matrix method to calculate the average reflection loss over the solar
   spectrum for periodic and aperiodic arrangements of nanorods. We find
   that aperiodicity can either improve or deteriorate the anti-reflection
   performance, depending on the nanorod diameter. We use a guided
   random-walk algorithm to design optimal aperiodic structures that
   exhibit lower reflection loss than both optimal periodic and random
   aperiodic structures. (C)2011 Optical Society of America}},
