@article{ WOS:000329473900026,
Author = {Qi, Hong and Zhang, Biao and Ren, Yatao},
Title = {{Retrieval of spherical particle size distribution using ant colony
   optimization algorithm}},
Journal = {{CHINESE OPTICS LETTERS}},
Year = {{2013}},
Volume = {{11}},
Number = {{11}},
Month = {{NOV 10}},
Abstract = {{The ant colony optimization (AGO) algorithm based on the probability
   density function is applied for the retrieval of spherical particle size
   distribution (PSD). The spectral extinction data based on the Mie theory
   and the Lambert Beer Law served as input for estimating five commonly
   use monomodal PSDs, i.e., Rosin Rammer distribution, normal
   distribution, logarithmic normal distribution, modified beta
   distribution, and Johnson's S-B distribution. The retrieval results show
   that the AGO algorithm has high feasibility and reliability, thus
   providing a new method for the retrieval of PSD.}},
