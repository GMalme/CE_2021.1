@article{ WOS:000169288300013,
Author = {Wang, CYE and Timoszyk, WK and Bobrow, JE},
Title = {{Payload maximization for open chained manipulators: Finding
   weightlifting motions for a Puma 762 robot}},
Journal = {{IEEE TRANSACTIONS ON ROBOTICS AND AUTOMATION}},
Year = {{2001}},
Volume = {{17}},
