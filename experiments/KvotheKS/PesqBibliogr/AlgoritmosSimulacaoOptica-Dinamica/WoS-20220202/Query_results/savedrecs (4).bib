@article{ WOS:000399872100003,
Author = {Liu, Weidong and Zhang, Liangchi},
Title = {{Numerical optimization platform for precision glass molding by the
   simplex algorithm}},
Journal = {{APPLIED OPTICS}},
Year = {{2017}},
Volume = {{56}},
Number = {{12}},
Pages = {{3245-3250}},
Month = {{APR 20}},
Abstract = {{Precision glass molding (PGM) can fabricate aspherical lens and
   irregular optical products in a single step, but its applicability is
   currently limited by the thermally induced residual stresses and lens
   shape derivation after molding. To remove this barrier, this paper
   develops a numerical optimization platform for PGM based on a simplex
   algorithm and finite element simulation. It was found that the platform
   can effectively reduce the residual stress in a molded lens through
   cooling process optimization and minimize the lens shape derivation by
   die shape compensation. The platform established can improve the lens
   quality by PMG and make molded lenses have better quality than those
   manufactured by ultraprecision machining processes. (C) 2017 Optical
   Society of America}},
