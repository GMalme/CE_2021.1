@article{ WOS:000427856300043,
Author = {Baghel, Gaurav Singh and Kartikeyan, M. V.},
Title = {{Output System of A 220-/247.5-/275-GHz, 1.0-MW, Triple-Frequency Regime
   Gyrotron}},
