@article{ WOS:000560931200098,
Author = {Whiting, Eric B. and Campbell, Sawyer D. and Kang, Lei and Werner,
   Douglas H.},
Title = {{Meta-atom library generation via an efficient multi-objective shape
   optimization method}},
