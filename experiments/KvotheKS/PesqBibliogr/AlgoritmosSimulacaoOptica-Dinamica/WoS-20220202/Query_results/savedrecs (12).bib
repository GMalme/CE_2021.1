@article{ WOS:000424117300057,
Author = {Bartczak, P. and Dudzinski, G.},
Title = {{Shaping asteroid models using genetic evolution (SAGE)}},
Journal = {{MONTHLY NOTICES OF THE ROYAL ASTRONOMICAL SOCIETY}},
Year = {{2018}},
Volume = {{473}},
Number = {{4}},
Pages = {{5050-5065}},
Month = {{FEB}},
Abstract = {{In this work, we present SAGE (shaping asteroid models using genetic
   evolution), an asteroid modelling algorithm based solely on photometric
   lightcurve data. It produces non-convex shapes, orientations of the
   rotation axes and rotational periods of asteroids. The main concept
   behind a genetic evolution algorithm is to produce random populations of
   shapes and spin-axis orientations by mutating a seed shape and iterating
   the process until it converges to a stable global minimum. We tested
   SAGE on five artificial shapes. We also modelled asteroids 433 Eros and
   9 Metis, since ground truth observations for them exist, allowing us to
   validate the models. We compared the derived shape of Eros with the NEAR
   Shoemaker model and that of Metis with adaptive optics and stellar
   occultation observations since other models from various inversion
   methods were available for Metis.}},
