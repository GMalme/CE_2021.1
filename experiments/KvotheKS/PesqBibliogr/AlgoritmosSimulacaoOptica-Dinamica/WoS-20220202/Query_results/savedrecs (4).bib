@article{ WOS:000294126300004,
Author = {Bullo, Francesco and Frazzoli, Emilio and Pavone, Marco and Savla, Ketan
   and Smith, Stephen L.},
Title = {{Dynamic Vehicle Routing for Robotic Systems}},
