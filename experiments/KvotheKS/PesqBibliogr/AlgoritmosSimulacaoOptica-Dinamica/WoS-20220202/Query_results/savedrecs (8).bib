@inproceedings{ WOS:000082521300024,
Author = {DuBois, R},
Editor = {{Sipper, M and Mange, D and PerezUribe, A}},
Title = {{A biologically inspired object tracking system}},
Booktitle = {{EVOLVABLE SYSTEMS: FROM BIOLOGY TO HARDWARE}},
Series = {{LECTURE NOTES IN COMPUTER SCIENCE}},
