@article{ WOS:000447380000009,
Author = {Leboucher, Cedric and Shin, Hyo-Sang and Chelouah, Rachid and Le Menec,
   Stephane and Siarry, Patrick and Formoso, Mathias and Tsourdos, Antonios
   and Kotenkoff, Alexandre},
Title = {{An Enhanced Particle Swarm Optimization Method Integrated With
   Evolutionary Game Theory}},
Journal = {{IEEE TRANSACTIONS ON GAMES}},
Year = {{2018}},
Volume = {{10}},
Number = {{2}},
Pages = {{221-230}},
Month = {{JUN}},
Abstract = {{This paper describes a novel particle swarmoptimizer algorithm. The
   focus of this study is how to improve the performance of the classical
   particle swarm optimization approach, i.e., how to enhance its
   convergence speed and capacity to solve complex problems while reducing
   the computational load. The proposed approach is based on an improvement
   of particle swarm optimization using evolutionary game theory. This
   method maintains the capability of the particle swarm optimizer to
   diversify the particles' exploration in the solution space. Moreover,
   the proposed approach provides an important ability to the optimization
   algorithm, that is, adaptation of the search direction, which improves
   the quality of the particles based on their experience. The proposed
   algorithm is tested on a representative set of continuous benchmark
   optimization problems and compared with some other classical
   optimization approaches. Based on the test results of each benchmark
   problem, its performance is analyzed and discussed.}},
