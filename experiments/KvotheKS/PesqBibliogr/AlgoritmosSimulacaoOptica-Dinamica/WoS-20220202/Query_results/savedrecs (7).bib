Address = {{2010 MASSACHUSETTS AVE NW, WASHINGTON, DC 20036 USA}},
Type = {{Article}},
Language = {{English}},
Affiliation = {{Batshev, VI (Corresponding Author), Russian Acad Sci, Lab Acoustoopt Spect, Sci \& Technol Ctr Unique Instrumentat, 15 Butlerova Str, Moscow 117342, Russia.
