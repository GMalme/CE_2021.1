@article{ WOS:000379636200003,
Author = {Gong, Chunye and Bao, Weimin and Liu, Jie and Tang, Guojian and Jiang,
   Yuewen},
Title = {{An efficient wavefront parallel algorithm for structured three
   dimensional LU-SGS}},
Journal = {{COMPUTERS \& FLUIDS}},
