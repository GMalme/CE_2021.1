@article{ WOS:000248671300016,
Author = {Park, Juyong and Haan, Jaeyoung and Park, F. C.},
Title = {{Convex optimization algorithms for active balancing of humanoid robots}},
Journal = {{IEEE TRANSACTIONS ON ROBOTICS}},
Year = {{2007}},
Volume = {{23}},
Number = {{4}},
Pages = {{817-822}},
Month = {{AUG}},
Abstract = {{We show that a large class of active balancing problems for legged
   robots can be framed as a second-order cone programming (SOCP) problem,
   a convex optimization problem for which efficient and numerically robust
   algorithms exist. We describe this general SOCP balancing framework,
   show that several existing optimization-based balancing strategies
   reduce to special cases of this more general formulation, and
   investigate the computational performance of our SOCP algorithms through
   simulation studies involving a humanoid model.}},
