@article{ WOS:000375190000007,
Author = {Jung, Jaedal and Taciroclu, Ertugrul},
Title = {{A DIVIDE-ALTERNATE-AND-CONQUER APPROACH FOR LOCALIZATION AND SHAPE
   IDENTIFICATION OF MULTIPLE SCATTERERS IN HETEROGENEOUS MEDIA USING
