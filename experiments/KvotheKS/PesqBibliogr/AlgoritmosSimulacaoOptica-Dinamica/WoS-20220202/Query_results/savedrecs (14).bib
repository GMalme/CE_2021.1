@article{ WOS:000668347900018,
Author = {Babanezhad, Meisam and Zabihi, Samyar and Behroyan, Iman and Nakhjiri,
   Ali Taghvaie and Marjani, Azam and Shirazian, Saeed},
Title = {{Prediction of gas velocity in two-phase flow using developed fuzzy logic
   system with differential evolution algorithm}},
Journal = {{SCIENTIFIC REPORTS}},
Year = {{2021}},
Volume = {{11}},
Number = {{1}},
Month = {{JAN 27}},
Abstract = {{In this investigation, differential evolution (DE) algorithm with the
   fuzzy inference system (FIS) are combined and the DE algorithm is
   employed in FIS training process. Considered data in this study were
   extracted from simulation of a 2D two-phase reactor in which gas was
   sparged from bottom of reactor, and the injected gas velocities were
   between 0.05 to 0.11 m/s. After doing a couple of training by making
   some changes in DE parameters and FIS parameters, the greatest
   percentage of FIS capacity was achieved. By applying the optimized
   model, the gas phase velocity in x direction inside the reactor was
   predicted when the injected gas velocity was 0.08 m/s.}},
