@article{ WOS:000343841700022,
Author = {Huang, Huan and Yang, Le-ping and Zhu, Yan-wei and Zhang, Yuan-wen},
Title = {{Collective trajectory planning for satellite swarm using inter-satellite
   electromagnetic force}},
Journal = {{ACTA ASTRONAUTICA}},
Year = {{2014}},
Volume = {{104}},
Number = {{1, SI}},
