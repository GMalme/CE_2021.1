@article{ WOS:000662372200001,
Author = {Chen, Hao and Li, Weikun and Cui, Weicheng and Yang, Ping and Chen,
   Linke},
Title = {{Multi-Objective Multidisciplinary Design Optimization of a Robotic Fish
   System}},
Journal = {{JOURNAL OF MARINE SCIENCE AND ENGINEERING}},
