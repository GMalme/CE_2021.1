@article{ WOS:000438408400003,
Author = {Liu, Zijing and Liu, Xuejun and Cai, Xinye},
Title = {{A new hybrid aerodynamic optimization framework based on differential
   evolution and invasive weed optimization}},
Journal = {{CHINESE JOURNAL OF AERONAUTICS}},
Year = {{2018}},
Volume = {{31}},
Number = {{7}},
Pages = {{1437-1448}},
Month = {{JUL}},
Abstract = {{Since many aerodynamic optimization problems in the area of aeronautics
   contain highly nonlinear objectives and multiple local optima, it is
   still a challenge for most of the traditional optimization methods to
   find the global optima. In this paper, a new hybrid optimization
   framework based on Differential Evolution and Invasive Weed Optimization
   (IWO\_DE/Ring) is developed, which combines global and local search to
   improve the performance, where a Multiple-Output Gaussian Process (MOGP)
   is used as the surrogate model. We first use several test functions to
   verify the performance of the IWO\_DE/Ring method, and then apply the
   optimization framework to a supercritical airfoil design problem. The
   convergence and the robustness of the proposed framework are compared
   against some other optimization methods. The IWO\_DE/Ringbased approach
   provides much quicker and steadier convergence than the traditional
   methods. The results show that the stability of the dynamic optimization
   process is an important indication of the confidence in the obtained
   optimum, and the proposed optimization framework based on IWO\_DE/Ring
   is a reliable and promising alternative for complex aeronautical
   optimization problems. (C) 2018 Chinese Society of Aeronautics and
   Astronautics. Production and hosting by Elsevier Ltd.}},
