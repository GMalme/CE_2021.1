@article{ WOS:000710928500057,
Author = {Seo, Sangwon and Lee, Jae Hoon and Lee, Sang-Bum and Park, Sang Eon and
   Seo, Meung Ho and Park, Jongcheol and Kwon, Taeg Yong and Hong, Hyun-Gue},
Title = {{Maximized atom number for a grating magneto-optical trap via
   machine-learning assisted parameter optimization}},
Journal = {{OPTICS EXPRESS}},
Year = {{2021}},
Volume = {{29}},
Number = {{22}},
Pages = {{35623-35639}},
Month = {{OCT 25}},
Abstract = {{We present a parameter set for obtaining the maximum number of atoms in
   a grating magneto-optical trap (gMOT) by employing a machine learning
   algorithm. In the multi-dimensional parameter space, which imposes a
   challenge for global optimization, the atom number is efficiently
   modeled via Bayesian optimization with the evaluation of the trap
   performance given by a Monte-Carlo simulation. Modeling gMOTs for six
   representative atomic species - Li-7, Na-23, Rb-87, Sr-88, Cs-I33,
   Yb-174 - allows us to discover that the optimal grating reflectivity is
   consistently higher than a simple estimation based on balanced optical
   molasses. Our algorithm also yields the optimal diffraction angle which
   is independent of the beam waist. The validity of the optimal parameter
   set for the case of Rb-87 is experimentally verified using a set of
   grating chips with different refiectivities and diffraction angles. (C)
   2021 Optical Society of America under the terms of the OSA Open Access
   Publishing Agreement}},
