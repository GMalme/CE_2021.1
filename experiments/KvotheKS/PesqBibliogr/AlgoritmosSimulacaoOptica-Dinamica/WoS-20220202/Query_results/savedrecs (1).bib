@article{ WOS:000414134300004,
Author = {Zhang, Xinjie and Li, Zhihua and Guo, Konghui and Zheng, Fumiao and
   Wang, Zhong},
Title = {{A novel pumping magnetorheological damper: Design, optimization, and
   evaluation}},
Journal = {{JOURNAL OF INTELLIGENT MATERIAL SYSTEMS AND STRUCTURES}},
Year = {{2017}},
Volume = {{28}},
Number = {{17}},
Pages = {{2339-2348}},
Month = {{OCT}},
Abstract = {{Magnetorheological damper has been widely used for improving vehicle
   ride comfort and handling because of its excellent performance. Numerous
   reinforced structures of magnetorheological dampers have been developed
   to enhance their dynamic performance and heat dissipation. This article
   presents a novel pumping magnetorheological damper with one-way pumping
   flow and dual-stage coil-in-cylinder magnetic system. Its dynamic model
   and inductive time constant are derived. Then, the structural parameters
   are optimized via a fast multi-objective genetic algorithm. Furthermore,
   a pumping magnetorheological damper prototype is tested and evaluated.
   The proposed pumping magnetorheological damper is able to perfectly
   balance the dynamic performance, heat dissipation efficiency, and
   structure flexibility via one-way pumping flow and coil-in-cylinder
   magnetic system, which has a promising application prospect in
   semi-active suspensions.}},
