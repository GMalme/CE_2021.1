@article{ WOS:000260728600011,
Author = {Lu, Ping and Griffin, Brian J. and Dukeman, Gregory A. and Chavez, Frank
   R.},
Title = {{Rapid Optimal Multiburn Ascent Planning and Guidance}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
Year = {{2008}},
Volume = {{31}},
