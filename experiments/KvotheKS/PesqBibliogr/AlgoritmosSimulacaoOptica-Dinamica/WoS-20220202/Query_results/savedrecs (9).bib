@article{ WOS:000275643000059,
Author = {Shin, Jongho and Kim, H. Jin and Park, Sewook and Kim, Youdan},
Title = {{Model predictive flight control using adaptive support vector regression}},
Journal = {{NEUROCOMPUTING}},
