@article{ WOS:000248630500006,
Author = {Chiba, Kazuhisa and Oyama, Akira and Morino, Hiroyuki},
Title = {{Multidisciplinary design optimization and data mining for transonic
   regional-jet wing}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{2007}},
Volume = {{44}},
