@article{ WOS:000342756500139,
Author = {Arteaga-Sierra, F. R. and Milian, C. and Torres-Gomez, I. and
   Torres-Cisneros, M. and Molto, G. and Ferrando, A.},
Title = {{Supercontinuum optimization for dual-soliton based light sources using
   genetic algorithms in a grid platform}},
Journal = {{OPTICS EXPRESS}},
Year = {{2014}},
Volume = {{22}},
Number = {{19}},
Pages = {{23686-23693}},
Month = {{SEP 22}},
Abstract = {{We present a numerical strategy to design fiber based dual pulse light
   sources exhibiting two predefined spectral peaks in the anomalous group
   velocity dispersion regime. The frequency conversion is based on the
   soliton fission and soliton self-frequency shift occurring during
   supercontinuum generation. The optimization process is carried out by a
   genetic algorithm that provides the optimum input pulse parameters:
   wavelength, temporal width and peak power. This algorithm is implemented
   in a Grid platform in order to take advantage of distributed computing.
   These results are useful for optical coherence tomography applications
   where bell-shaped pulses located in the second near-infrared window are
   needed. (C) 2014 Optical Society of America}},
