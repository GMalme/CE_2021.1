@article{ WOS:000591175500003,
Author = {Zhu, Hongjun and Li, Yan and Jin, Xuelian and Huang, Jiangping and Liu,
   Xin and Qian, Ying and Tan, Jindong},
Title = {{Transmission dynamics and control methodology of COVID-19: A modeling
   study}},
Journal = {{APPLIED MATHEMATICAL MODELLING}},
