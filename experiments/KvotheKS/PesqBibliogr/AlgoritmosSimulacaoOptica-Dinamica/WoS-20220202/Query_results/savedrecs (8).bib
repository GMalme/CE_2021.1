@article{ WOS:000276677200001,
Author = {Apostolakis, G. and Dargush, G. F.},
Title = {{Optimal seismic design of moment-resisting steel frames with hysteretic
   passive devices}},
Journal = {{EARTHQUAKE ENGINEERING \& STRUCTURAL DYNAMICS}},
Year = {{2010}},
