@article{ WOS:000546929000034,
Author = {Michalko, Aaron M. and Fienup, James R.},
Title = {{Development of a concave freeform surface measurement using transverse
   translation-diverse phase retrieval}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2020}},
Volume = {{59}},
