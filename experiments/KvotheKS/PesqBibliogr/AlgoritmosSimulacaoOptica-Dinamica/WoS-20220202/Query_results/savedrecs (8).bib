@article{ WOS:000258679400007,
Author = {Allen, Benoit and Gosselin, Louis},
Title = {{Optimal geometry and flow arrangement for minimizing the cost of
   shell-and-tube condensers}},
Journal = {{INTERNATIONAL JOURNAL OF ENERGY RESEARCH}},
