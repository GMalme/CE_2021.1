@article{ WOS:000285915300017,
Author = {Poletti, Francesco and Feng, Xian and Ponzo, Giorgio M. and Petrovich,
   Marco N. and Loh, Wei H. and Richardson, David J.},
Title = {{All-solid highly nonlinear singlemode fibers with a tailored dispersion
   profile}},
