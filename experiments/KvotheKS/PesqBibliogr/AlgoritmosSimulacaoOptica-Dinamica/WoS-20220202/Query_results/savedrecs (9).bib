@article{ WOS:000701251500025,
Author = {Yao, Runkui and Li, Hongxiang and Zhang, Bohao and Chen, Weiwei and
   Wang, Pengjun and Dai, Shixun and Liu, Yuxiao and Li, Jun and Li, Yan
   and Fu, Qiang and Dai, Tingge and Yu, Hui and Yang, Jianyi and Pavesi,
   Lorenzo},
Title = {{Compact and Low-Insertion-Loss 1xN Power Splitter in Silicon Photonics}},
Journal = {{JOURNAL OF LIGHTWAVE TECHNOLOGY}},
Year = {{2021}},
Volume = {{39}},
Number = {{19}},
Pages = {{6253-6259}},
Month = {{OCT 1}},
Abstract = {{In this paper, a novel design of a 1xN multimode-interference power
   splitter is proposed and investigated. By using the finite difference
   time domain method and particle swarm optimization algorithm, our
   proposed 1xN optical power splitter can be optimized to realize compact
   size, good uniformity, low insertion loss, and broad bandwidth. To
   verify the effectiveness of this design, the designed 1x4 and 1x8
   multimode-interference power splitters are experimentally demonstrated
   on an silicon-on-insulator platform. The lengths of the designed 1x4 and
   1x8 power splitters are 36 mu m and 47.8 mu m, respectively.
   Experimental results show that, for the fabricated 1x4 power splitter,
   the uniformity ranges from 0.07 to 0.89 dB and the corresponding maximum
   insertion loss is 0.62 dB within a bandwidth from 1520 to 1624 nm. For
   the fabricated 1x8 power splitter, within a bandwidth from 1519 to 1623
   nm, the uniformity varies from 0.19 to 0.83 dB and a maximum insertion
   loss of 0.64 dB are obtained.}},
