@article{ WOS:000231716400068,
Author = {Wu, LC and Sun, ZQ and Sun, FC and Zhen, L},
Title = {{A dynamic optimization approach of flexible redundant manipulators for
   vibration reducing and torque minimization}},
Journal = {{DYNAMICS OF CONTINUOUS DISCRETE AND IMPULSIVE SYSTEMS-SERIES
   B-APPLICATIONS \& ALGORITHMS}},
Year = {{2005}},
Volume = {{1}},
Number = {{SI}},
Pages = {{353-357}},
Note = {{4th International Conference on Engineering Applications and
   Computational Algorithms, Univ Guelph, Guelph, CANADA, JUL 27-29, 2005}},
Organization = {{DCDIS}},
Abstract = {{Firstly, a complex number mode analysis based approach is proposed for
   vibration reducing of structural flexible redundant manipulators by
   utilizing self-motion. In the proposed approach, the self-motion is
   evaluated to nullify the low rank of modal exciting-force of flexural
   motion, and the approach can be freely used when the degree of freedom
   of flexural motion is much greater than the available degree of
   redundancy. Then, a dynamic optimization approach of flexible redundant
   manipulators is proposed. The approach perform end-effector vibration
   reducing and joint torque optimization at the same time through use of
   Lagrange multiplier method to minimizing the joint driving torque and
   the modal exciting force of flexural motion of the manipulator. The
   availability and effectiveness of the proposed approach have been
   demonstrated through numerical simulation with a four-link spatial
   robotic manipulator possessing an end flexible link.}},
