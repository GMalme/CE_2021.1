@article{ WOS:000493501300001,
Author = {Nichols, Rosanna and Mineh, Lana and Rubio, Jesus and Matthews, Jonathan
   C. F. and Knott, Paul A.},
Title = {{Designing quantum experiments with a genetic algorithm}},
Journal = {{QUANTUM SCIENCE AND TECHNOLOGY}},
