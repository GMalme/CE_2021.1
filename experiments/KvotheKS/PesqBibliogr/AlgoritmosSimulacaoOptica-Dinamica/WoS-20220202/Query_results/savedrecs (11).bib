@article{ WOS:000560671600011,
Author = {Inyang-Udoh, Uduak and Guo, Yijie and Peters, Joost and Oomen, Tom and
   Mishra, Sandipan},
Title = {{Layer-to-Layer Predictive Control of Inkjet 3-D Printing}},
Journal = {{IEEE-ASME TRANSACTIONS ON MECHATRONICS}},
Year = {{2020}},
Volume = {{25}},
Number = {{4}},
Pages = {{1783-1793}},
Month = {{AUG}},
Note = {{IEEE/ASME International Conference on Advanced Intelligent Mechatronics
   (AIM), ELECTR NETWORK, JUL, 2020}},
Organization = {{IEEE; ASME}},
Abstract = {{This article develops and experimentally validates a distributed
   predictive control algorithm for closed-loop control of inkjet 3-D
   printing to handle constraints, e.g., droplet volume bounds, as well as
   the large-scale nature of the 3-D printing problem. The large number of
   decision variables, i.e., droplet volumes at each grid point, in high
   resolution inkjet 3-D printing makes centralized methods extremely
   time-consuming, thus, a distributed implementation of the controller is
   necessary. First, a graph-based height evolution model that captures the
   liquid spreading dynamics is described. Based on this model, a scalable
   closed-loop control algorithm using distributed model predictive control
   (MPC) that can reduce computation time significantly is designed and
   experimentally implemented. The performance and efficiency of the
   algorithm are shown to outperform open-loop printing and closed-loop
   printing with existing centralized MPC methods.}},
