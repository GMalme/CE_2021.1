@article{ WOS:000424288200019,
Author = {Li, Kun and Han, Ying},
Title = {{Modelling for motor load torque with dynamic load changes of beam
   pumping units based on a serial hybrid model}},
Journal = {{TRANSACTIONS OF THE INSTITUTE OF MEASUREMENT AND CONTROL}},
