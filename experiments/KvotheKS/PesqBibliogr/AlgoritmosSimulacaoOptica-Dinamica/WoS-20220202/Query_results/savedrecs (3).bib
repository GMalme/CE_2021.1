@article{ WOS:000409326900028,
Author = {Hill, Alexander D. and Hervas, David and Nash, Joseph and Graham, Martin
   and Burgers, Alexander and Paudel, Uttam and Steel, Duncan and
   Schneider, Christian and Kamp, Martin and Hoefling, Sven and Wang, Jin
   and Lin, Jiahe and Zhao, Wanying and Kwiat, Paul G.},
Title = {{Optimizing single-mode collection from pointlike sources of single
   photons with adaptive optics}},
Journal = {{OPTICS EXPRESS}},
Year = {{2017}},
Volume = {{25}},
Number = {{16}},
Pages = {{18629-18642}},
Month = {{AUG 7}},
Abstract = {{The collection efficiency of light from a point-like emitter may be
   extremely poor due to aberrations induced by collection optics and the
   emission distribution of the source. Analyzing the aberrant wavefront
   (e.g., with a Shack-Hartmann sensor) and correcting accordingly can be
   infeasible on the single-photon level. We present a technique that uses
   a genetic algorithm to control a deformable mirror for correcting
   wavefront aberrations in single-photon signals from point emitters. We
   apply our technique to both a simulated point source and a real InAs
   quantum dot, achieving coupling increases of up to 50\% and automatic
   reduction of system drift. (C) 2017 Optical Society of America}},
