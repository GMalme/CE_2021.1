@article{ WOS:000390097300008,
Author = {Namura, Nobuo and Obayashi, Shigeru and Jeong, Shinkyu},
Title = {{Efficient Global Optimization of Vortex Generators on a Supercritical
   Infinite Wing}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{2016}},
