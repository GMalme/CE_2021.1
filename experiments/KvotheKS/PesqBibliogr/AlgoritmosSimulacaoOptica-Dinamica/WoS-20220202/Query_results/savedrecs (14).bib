@article{ WOS:000294781200015,
Author = {Lin, Chenxi and Povinelli, Michelle L.},
Title = {{Optimal design of aperiodic, vertical silicon nanowire structures for
   photovoltaics}},
Journal = {{OPTICS EXPRESS}},
Year = {{2011}},
Volume = {{19}},
Number = {{19}},
Pages = {{A1148-A1154}},
Month = {{SEP 12}},
Abstract = {{We design a partially aperiodic, vertically-aligned silicon nanowire
   array that maximizes photovoltaic absorption. The optimal structure is
   obtained using a random walk algorithm with transfer matrix method based
   electromagnetic forward solver. The optimal, aperiodic structure
   exhibits a 2.35 times enhancement in ultimate efficiency compared to its
   periodic counterpart. The spectral behavior mimics that of a periodic
   array with larger lattice constant. For our system, we find that
   randomly-selected, aperiodic structures invariably outperform the
   periodic array. (C) 2011 Optical Society of America}},
