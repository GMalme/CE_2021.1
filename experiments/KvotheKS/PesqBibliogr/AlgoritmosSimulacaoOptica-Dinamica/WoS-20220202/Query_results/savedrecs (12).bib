@article{ WOS:000572932300001,
Author = {Meyer, Eivind and Heiberg, Amalie and Rasheed, Adil and San, Omer},
Title = {{COLREG-Compliant Collision Avoidance for Unmanned Surface Vehicle Using
   Deep Reinforcement Learning}},
