@article{ WOS:000648654700005,
Author = {Ye, Jianchuan and Wang, Jiang and Song, Tao and Zhu, Yifei and Wu,
   Zeliang and Tang, Pan},
Title = {{Propulsion optimization of a quadcopter in forward state}},
Journal = {{AEROSPACE SCIENCE AND TECHNOLOGY}},
Year = {{2021}},
Volume = {{113}},
Month = {{JUN}},
Abstract = {{A propulsion system suitable for the forward state of a quadcopter is
   optimally designed in this study. The forward model of the rotor in tilt
   flow is established using a linear regression method to process the
   aerodynamics data acquired from the empirical blade element momentum
   theory (BEMT). Fuselage model is constructed by high fidelity
   computational fluid dynamics (CFD) method. Based on the models, the
   performance of the UAV can be derived precisely and quickly with the
   propulsion system parameters. Then, the propulsion system is optimized
   to better realize the forward flight endurance by using genetic
   intelligent optimization algorithm. Finally, wind-tunnel tests are
   conducted and the results indicate that the optimal propulsion system
   can efficiently improve the forward flight endurance. (C) 2021 Elsevier
