@article{ WOS:000470999900038,
Author = {Liu, Chengxiang and Xiong, Wei and Zhang, Ying and Sun, Yang and Xiong,
   Minghui and Guo, Chao},
Title = {{Research on Invulnerability Technology of Node Attack in Space-Based
   Information Network Based on Complex Network}},
Journal = {{ELECTRONICS}},
