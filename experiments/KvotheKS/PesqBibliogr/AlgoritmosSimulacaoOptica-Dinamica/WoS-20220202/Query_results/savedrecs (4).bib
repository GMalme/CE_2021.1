@article{ WOS:000431880000043,
Author = {Gonzalez-Alcalde, Alma K. and Salas-Montiel, Rafael and Mohamad, Habib
   and Morand, Alain and Blaize, Sylvain and Macias, Demetrio},
Title = {{Optimization of all-dielectric structures for color generation}},
