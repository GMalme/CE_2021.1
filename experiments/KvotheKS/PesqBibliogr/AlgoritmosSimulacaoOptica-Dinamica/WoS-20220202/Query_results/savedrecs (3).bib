@article{ WOS:000699554800001,
Author = {Leal, Isaac S. and Abeykoon, Chamil and Perera, Yasith S.},
Title = {{Design, Simulation, Analysis and Optimization of PID and Fuzzy Based
   Control Systems for a Quadcopter}},
Journal = {{ELECTRONICS}},
