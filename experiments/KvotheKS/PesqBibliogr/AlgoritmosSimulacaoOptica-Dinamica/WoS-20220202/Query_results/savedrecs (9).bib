@article{ WOS:A1995RX21000002,
Author = {LIVET, T and KUBICA, F and MAGNI, JF},
Title = {{ROBUST FLIGHT CONTROL DESIGN WITH RESPECT TO DELAYS, CONTROL
   EFFICIENCIES AND FLEXIBLE MODES}},
Journal = {{CONTROL ENGINEERING PRACTICE}},
Year = {{1995}},
Volume = {{3}},
Number = {{10}},
Pages = {{1373-1384}},
Month = {{OCT}},
Abstract = {{This paper presents an iterative technique which makes it possible to
   design a robust flight control system. The methodology described in this
   paper has multiple applications. Two different applications are
   considered: the first consists of computing a robust feedback with
   respect to parameter uncertainties by considering a bank of models,
   while the second provides a robust flight-control design for a highly
   flexible aircraft with significant cross-coupling between flight
   mechanics and structural dynamics modes.
   The proposed method will solve these problems via an iterative quadratic
   optimization under linear inequality constraints. Finally, the iterative
   algorithm is applied to a civil aircraft control system in lateral
   flight.}},
