@article{ WOS:000241577500011,
Author = {Jin, Jaehyun and Park, Bongkyu and Park, Youngwoong and Tahk, Min-Jea},
Title = {{Attitude control of a satellite with redundant thrusters}},
Journal = {{AEROSPACE SCIENCE AND TECHNOLOGY}},
