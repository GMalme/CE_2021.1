@article{ WOS:000413651800001,
Author = {Zhao, Hong and Hu, Hao-Ran},
Title = {{Optimal design of a pipe isolation plugging tool using a computational
   fluid dynamics simulation with response surface methodology and a
   modified genetic algorithm}},
