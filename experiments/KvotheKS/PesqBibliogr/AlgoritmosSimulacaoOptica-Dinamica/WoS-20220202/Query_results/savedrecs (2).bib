@article{ WOS:000334768800027,
Author = {Kim, Hyoungjin and Liou, Meng-Sing},
Title = {{Adaptive directional local search strategy for hybrid evolutionary
   multiobjective optimization}},
Journal = {{APPLIED SOFT COMPUTING}},
