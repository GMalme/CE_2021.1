@article{ WOS:000404743500019,
Author = {Cui, Wei and Gao, Liang},
Title = {{Optical mapping near-eye three-dimensional display with correct focus
   cues}},
Journal = {{OPTICS LETTERS}},
Year = {{2017}},
Volume = {{42}},
Number = {{13}},
Pages = {{2475-2478}},
Month = {{JUL 1}},
Abstract = {{We present an optical mapping near-eye (OMNI) three-dimensional display
   method for wearable devices. By dividing a display screen into different
   subpanels and optically mapping them to various depths, we create a
   multiplane volumetric image with correct focus cues for depth
   perception. The resultant system can drive the eye's accommodation to
   the distance that is consistent with binocular stereopsis, thereby
   alleviating the vergence accommodation conflict, the primary cause for
   eye fatigue and discomfort. Compared with the previous methods, the OMNI
   display offers prominent advantages in adaptability, image dynamic
   range, and refresh rate. (C) 2017 Optical Society of America.}},
