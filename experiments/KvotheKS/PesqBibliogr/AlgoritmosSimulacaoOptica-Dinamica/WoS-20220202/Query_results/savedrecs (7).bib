@article{ WOS:000469913800017,
Author = {Umenberger, Jack and Manchester, Ian R.},
Title = {{Specialized Interior-Point Algorithm for Stable Nonlinear System
   Identification}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
