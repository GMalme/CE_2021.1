@article{ WOS:A1996UT06700009,
Author = {Takaki, Y and Ishida, K and Kume, Y and Ohzu, H},
Title = {{Incoherent pattern detection using a liquid-crystal active lens}},
Journal = {{APPLIED OPTICS}},
Year = {{1996}},
Volume = {{35}},
Number = {{17}},
Pages = {{3134-3140}},
Month = {{JUN 10}},
Abstract = {{Incoherent pattern detection by a simple imaging system using a
   liquid-crystal active lens is proposed. The imaging system works as a
   spatial filtering system with a rewritable phase-only filter. We found
   that, in the incoherent matched filtering system, a conventional
   phase-only filter has a higher optical efficiency but a lower pattern
   discrimination than a complex filter. To improve the pattern
   discrimination ability, we optimized the phase-only filter by using
   simulated annealing and a genetic algorithm. We designed phase-only
   filters that have discrimination ability comparable with that in a
   complex filter. The performance of optimized phase-only filters is
   experimentally demonstrated. (C) 1996 Optical Society of America}},
