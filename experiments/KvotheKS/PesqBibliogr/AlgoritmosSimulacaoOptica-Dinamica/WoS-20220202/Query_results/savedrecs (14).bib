@article{ WOS:A1991FU39100007,
Author = {DAMES, MP and DOWLING, RJ and MCKEE, P and WOOD, D},
Title = {{EFFICIENT OPTICAL-ELEMENTS TO GENERATE INTENSITY WEIGHTED SPOT ARRAYS -
   DESIGN AND FABRICATION}},
Journal = {{APPLIED OPTICS}},
Year = {{1991}},
Volume = {{30}},
Number = {{19}},
Pages = {{2685-2691}},
Month = {{JUL 1}},
Abstract = {{We report on the design and fabrication of fully 2-D surface relief
   diffraction elements that can split a single collimated beam into many
   beams in an arbitrary intensity distribution.  These splitters were
   designed by computer using simulated annealing, and made into phase
   gratings by electron-beam lithography followed by plasma etching into
   quartz glass.  Both two and four phase level gratings have been
   fabricated, allowing a wide range of uniform and weighted spot patterns
   to be generated.  These grating elements have a measured diffraction
   efficiency of over 74\%, with the beam intensity ratios accurate to
   within 1\% of their target values.}},
