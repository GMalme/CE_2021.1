Author = {Sahoo, Swaroop and Bosch-Lluis, Xavier and Reising, Steven C. and
   Vivekanandan, Jothiram},
Title = {{Optimization of Background Information and Layer Thickness for Improved
   Accuracy of Water-Vapor Profile Retrieval from Ground-Based Microwave
   Radiometer Measurements at K-Band}},
Journal = {{IEEE JOURNAL OF SELECTED TOPICS IN APPLIED EARTH OBSERVATIONS AND REMOTE
   SENSING}},
