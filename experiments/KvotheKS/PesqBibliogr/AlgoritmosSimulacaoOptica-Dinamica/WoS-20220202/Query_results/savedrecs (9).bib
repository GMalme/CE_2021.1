@article{ WOS:000460415600006,
Author = {Sinquin, Baptiste and Verhaegen, Michel},
Title = {{K4SID: Large-Scale Subspace Identification With Kronecker Modeling}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
