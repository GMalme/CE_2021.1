@article{ WOS:000332518100046,
Author = {Silvennoinen, Martti and Kaakkunen, Jarno and Paivasaari, Kimmo and
   Vahimaa, Pasi},
Title = {{Parallel femtosecond laser ablation with individually controlled
   intensity}},
