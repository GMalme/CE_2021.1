@article{ WOS:000245764000018,
Author = {St-Amant, Yves and Gariepy, Daniel and Rancourt, Denis},
Title = {{Model-based far-field alignment algorithm for Gaussian beamlike
   single-mode optical devices}},
Journal = {{APPLIED OPTICS}},
Year = {{2007}},
Volume = {{46}},
Number = {{12}},
Pages = {{2297-2306}},
Month = {{APR 20}},
Abstract = {{Single-mode device-to-fiber alignment automation is usually achieved
   with a classical mathematical optimization approach. We present a
   different approach, which is based on the identification of particular
   intrinsic characteristics of the coupled optical power and on estimating
   residual axial, transverse, and angular misalignments in the far field.
   Such a model-based approach is based on the physical nature of the
   optical coupling phenomenon and can replace or be complementary to
   already known automated alignment methods. An alignment algorithm is
   described and validated experimentally using two singlemode fibers as
   Gaussian beam emitter and receiver. (c) 2007 Optical Society of America.}},
