@article{ WOS:000248030300010,
Author = {Luo, Ya-Zhong and Lei, Yong-Jun and Tang, Guo-Jin},
Title = {{Optimal multi-objective nonlinear impulsive rendezvous}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
Year = {{2007}},
Volume = {{30}},
Number = {{4}},
Pages = {{994-1002}},
Month = {{JUL-AUG}},
Abstract = {{The multi-objective optimization of unperturbed two-body impulsive
   rendezvous is investigated in this paper. In addition to the total
   characteristic velocity and the time of rendezvous flight, the minimum
   relative distance between the chaser and the target in the chaser's
   free-flying path is proposed as another design performance index. The
   feasible iteration optimization model of three-objective impulsive
   rendezvous with path constraints including trajectory safety and field
   of view constraints is established using a Lambert algorithm. The
   multi-objective nondominated sorting genetic algorithm is employed to
   obtain the Pareto solution set. The -V-bar and +V-bar homing rendezvous
   missions are examined by the proposed approach. It is shown that
   tradeoffs between time of flight, propellant cost and passive trajectory
   safety, and several inherent principles of the rendezvous trajectory are
   demonstrated by this approach.}},
