@article{ WOS:000585299600006,
Author = {Huang, Jingwei and Chen, Yongchao and Peng, Hekuo and Zhou, Pengwei and
   Song, Qiuheng and Huang, Peng and Xiao, Qian and Jia, Bo},
Title = {{A 150 km distributed fiber-optic disturbance location sensor with no
   relay based on the dual-Sagnac interferometer employing time delay
   estimation}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2021}},
Volume = {{479}},
Month = {{JAN 15}},
Abstract = {{A distributed fiber-optic sensor based on a hybrid configuration of
   dual-Sagnac interferometer is proposed and demonstrated. The
   polarization compensation module is used to stabilize the polarization
   state of Sagnac interferometer so that it can be used for sensing.
   Through the time delay estimation algorithm, two signals with a time
   delay can be constructed, and the vibration position can be obtained by
   extracting the time delay. The experimental results show that the
   maximum sensing distance of the sensor can reach 150 km and the
   positioning error is within +/- 30 m. It is easy to install and can be
