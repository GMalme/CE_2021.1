@article{ WOS:000482628500007,
Author = {Marshall, Curtis J. and Roberts, Blake and Grenn, Michael W.},
Title = {{Context-Driven Autonomy for Enhanced System Resilience in Emergent
   Operating Environments}},
Journal = {{IEEE SYSTEMS JOURNAL}},
Year = {{2019}},
Volume = {{13}},
