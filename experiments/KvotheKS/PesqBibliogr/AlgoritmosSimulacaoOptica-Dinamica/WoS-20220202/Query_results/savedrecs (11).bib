@article{ WOS:000418736700059,
Author = {Taeb, A. and Reager, J. T. and Turmon, M. and Chandrasekaran, V.},
Title = {{A Statistical Graphical Model of the California Reservoir System}},
Journal = {{WATER RESOURCES RESEARCH}},
Year = {{2017}},
Volume = {{53}},
