@article{ WOS:000358941200027,
Author = {Chen, Ming and Chen, Liqun and Zeng, Aijun and Zhu, Jing and Yang, Baoxi
   and Huang, Huijie},
Title = {{Generation of trapezoidal illumination for the step-and-scan
   lithographic system}},
Journal = {{APPLIED OPTICS}},
Year = {{2015}},
Volume = {{54}},
Number = {{22}},
Pages = {{6820-6826}},
Month = {{AUG 1}},
Abstract = {{To meet the uniform dose exposure in optical lithography, it is
   desirable to get uniform illumination and trapezoidal illumination in
   the scanning direction on the wafer for the step-and-scan lithographic
   system. A design strategy for trapezoidal illumination is proposed which
   offers both high uniformity illumination and fixed integral energy of
   trapezoidal illumination in a different illumination pattern. The
   strategy describes a light uniform device which contains a microlens
   array, a microcylindrical array, and a Fourier lens and produces
   trapezoidal illumination directly at the scanning slit. Compared with
