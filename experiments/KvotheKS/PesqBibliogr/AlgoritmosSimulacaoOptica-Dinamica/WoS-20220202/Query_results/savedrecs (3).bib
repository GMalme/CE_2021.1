@article{ WOS:000506419900011,
Author = {Zhou, Peng and Ma, Xinran and Zhang, Shuang and Liu, Zhiguo and Meng,
   Zhaoying and Xiang, Zhujun and Wang, Xingyi and Sun, Tianxi and Lin,
   Xiaoyan and Li, Yude},
Title = {{Application of particle swarm optimization in the design of a
   mono-capillary X-ray lens}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION
   A-ACCELERATORS SPECTROMETERS DETECTORS AND ASSOCIATED EQUIPMENT}},
Year = {{2020}},
Volume = {{953}},
Month = {{FEB 11}},
Abstract = {{The non-uniformities of a fabricated mono-capillary X-ray lens (MCXRL)
   degrade the performance of the X-ray analysis system. The main
   evaluation criterion of a fabricated MCXRL is total root mean square
   error, which consists of the profile error (PE) and the central axis
   straightness error (CASE). In the present paper, we develop an
   open-source code package to evaluate the PE value of varying diameter
   glass tubes (VDGT). The source code named `PSO X-ray Lens Finder' in the
   package is written using the particle swarm optimization (PSO)
   algorithm, and the other source code named `MMN X-ray Lens Finder' is
   developed using the multiparameter multilevel nested-loop (MMN)
   algorithm. We take the research of a paraboloidal MCXRL as an example to
   introduce the source code package, and the mathematical theory and
   calculation details of the package are given in the method section. As a
   result, the `MMN X-ray Lens Finder' can be used alone to guide engineers
   in cutting a high-performance MCXRL from VDGT; the `PSO X-ray Lens
   Finder' can be applied as a faster alternative strategy for cutting
   work.}},
