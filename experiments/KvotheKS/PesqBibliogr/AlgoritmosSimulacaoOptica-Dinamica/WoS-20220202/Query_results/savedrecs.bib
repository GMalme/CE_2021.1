@article{ WOS:000271308300007,
Author = {Gasbarri, Paolo and Chiwiacowsky, Leonardo D. and de Campos Velho,
   Haroldo F.},
Title = {{A hybrid multilevel approach for aeroelastic optimization of composite
   wing-box}},
Journal = {{STRUCTURAL AND MULTIDISCIPLINARY OPTIMIZATION}},
