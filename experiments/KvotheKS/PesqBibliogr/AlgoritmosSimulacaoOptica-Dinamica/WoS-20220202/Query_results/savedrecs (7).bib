@article{ WOS:A1996UD28800010,
Author = {Ozyoruk, Y and Long, LN},
Title = {{A new efficient algorithm for computational aeroacoustics on parallel
   processors}},
Journal = {{JOURNAL OF COMPUTATIONAL PHYSICS}},
Year = {{1996}},
