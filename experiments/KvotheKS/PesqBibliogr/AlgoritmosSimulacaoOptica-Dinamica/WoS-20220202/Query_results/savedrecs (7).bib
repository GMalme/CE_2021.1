@article{ WOS:000462038000009,
Author = {Marafona, Joao D. M. and Marque, Pedro M. T. and Martins, Ramiro C. and
   Seabra, Jorge H. O.},
Title = {{Towards constant mesh stiffness helical gears: The influence of integer
   overlap ratios}},
Journal = {{MECHANISM AND MACHINE THEORY}},
Year = {{2019}},
Volume = {{136}},
Pages = {{141-161}},
Month = {{JUN}},
Abstract = {{This work presents the development and dynamic analysis of Constant Mesh
   Stiffness Gears. First, a study of the contact length variation between
   gears is conducted since it has great influence on the gear mesh
   stiffness. Following that, an algorithm capable of generating Constant
   Mesh Stiffness Gears (CMSGears) was developed while considering the
   gear's safety factors and mesh efficiency. The dynamic performance of
   these gears was evaluated resorting to a single degree of freedom
   dynamic model which considered teeth contact loss. The CMSGears created
   respect the gear design principles and both the gear safety factors and
   gear mesh efficiency were successfully taken into account. With regard
   to the dynamic behavior of these gears, they ended up revealing great
   potential in the reduction of noise, vibration and dynamic overload. (C)
   2019 Elsevier Ltd. All rights reserved.}},
