@article{ WOS:000309847800008,
Author = {Han, Younghee and de Callafon, Raymond A.},
Title = {{Identification of Wiener-Hammerstein benchmark model via rank
   minimization}},
Journal = {{CONTROL ENGINEERING PRACTICE}},
Year = {{2012}},
Volume = {{20}},
Number = {{11}},
Pages = {{1149-1155}},
Month = {{NOV}},
Abstract = {{In this paper, a Wiener-Hammerstein system identification problem is
   formulated as a semidefinite programming (SDP) problem which provides a
   sub-optimal solution for a rank minimization problem. In the proposed
   identification method, the first linear dynamic system, the static
   nonlinear function, and the second linear dynamic system are
   parameterized as an FIR model, a polynomial function, and a rational
   transfer function respectively. Subsequently the optimization problem is
   formulated by using the over-parameterization technique and an iterative
   approach is proposed to update two unmeasurable intermediate signals.
   For the modeling of static nonlinearity, the monotonically non-deceasing
   condition was applied to limit the number of possible selections for
