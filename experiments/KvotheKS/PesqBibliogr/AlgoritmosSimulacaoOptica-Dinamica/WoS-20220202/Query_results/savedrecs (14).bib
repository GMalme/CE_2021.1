@article{ WOS:000427126700014,
Author = {Yu, Yinshan and Yu, Xiaolei and Zhao, Zhimin and Qian, Kun and Wang,
   Donghua},
Title = {{Image Analysis System for Optimal Geometric Distribution of RFID Tags
   Based on Flood Fill and DLT}},
Journal = {{IEEE TRANSACTIONS ON INSTRUMENTATION AND MEASUREMENT}},
