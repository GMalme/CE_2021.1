@article{ WOS:000427340200003,
Author = {Zhao, Dan and Lu, Zhengli and Zhao, He and Li, X. Y. and Wang, Bing and
   Liu, Peijin},
Title = {{A review of active control approaches in stabilizing combustion systems
   in aerospace industry}},
