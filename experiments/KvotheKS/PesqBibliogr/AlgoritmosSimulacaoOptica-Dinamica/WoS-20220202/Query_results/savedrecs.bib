@article{ WOS:000395676900010,
Author = {Zhang, Bo and Duan, Haibin},
Title = {{Three-Dimensional Path Planning for Uninhabited Combat Aerial Vehicle
   Based on Predator-Prey Pigeon-Inspired Optimization in Dynamic
   Environment}},
Journal = {{IEEE-ACM TRANSACTIONS ON COMPUTATIONAL BIOLOGY AND BIOINFORMATICS}},
