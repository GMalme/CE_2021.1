@article{ WOS:A1994PL58300006,
Author = {HUTTUNEN, J and TUTUNEN, J},
Title = {{SYNTHESIS OF WIDE-FIELD DIFFRACTIVE WAVE-GUIDE LENSES}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1994}},
Volume = {{111}},
Number = {{5-6}},
Pages = {{445-450}},
Month = {{OCT 15}},
Abstract = {{A synthesis algorithm is presented for wide-field diffractive focusing
   lenses in waveguides.  Analytic optimization of the phase transformation
   is used to widen the angular field, and the efficiency is improved by
   parametric optimization of the local grating profile.  If the
   effective-index modulation is DELTAn(eff) almost-equal-to 0.1, a curved
   F:5 singlet in contact with the pupil can achieve a diffraction-limited
   angular field of approximately +/ 3-degrees, whereas a planar lens in a
   telecentric configuration can achieve approximately +/- 8-degrees.}},
