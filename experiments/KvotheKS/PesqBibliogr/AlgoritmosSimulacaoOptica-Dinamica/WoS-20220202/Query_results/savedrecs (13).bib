@article{ WOS:000382793800027,
Author = {Song, Feihu and Xu, Chuanlong and Wang, Shimin and Li, Zhenfeng},
Title = {{Measurement of temperature gradient in a heated liquid cylinder using
   rainbow refractometry assisted with infrared thermometry}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2016}},
Volume = {{380}},
Pages = {{179-185}},
Month = {{DEC 1}},
Abstract = {{In a heated liquid cylinder, a temperature gradient exists between the
   hotter surface and the colder core. Measurement of the temperature
   gradient is very important for the in-depth investigation into the
   mechanism and optimized control of the heat transfer process. Rainbow
   refractometry has been attempted to achieve the measurement since
   several years ago. Yet there is no effective inversion algorithm without
   predicting refractive index profiles based on experience. In the paper,
   an improved rainbow refractometry assisted with infrared thermometry was
   proposed to measure the diameter and the inside temperature gradient of
   a heated liquid cylinder. The inversion algorithm was designed based on
   the nonlinear least square method and an optimization process. To
   evaluate the feasibility of the proposed method, numerical simulations
   and experiments were carried out. The results of the numerical
   simulation showed that the relative error of the inversion diameter was
   about 1\%, and the error of the refractive index was less than 6 x
   10(-4) at all the radial locations. In the experimental research, t
   rainbows reconstructed with the reversion parameters were all similar to
   the corresponding captured ones. (C) 2016 Published by Elsevier B.V.}},
