@article{ WOS:000074040400005,
Author = {Sartoros, C and Goltz, DM and Salin, ED},
Title = {{Program considerations for simplex optimization of ion lenses in ICP-MS}},
Journal = {{APPLIED SPECTROSCOPY}},
