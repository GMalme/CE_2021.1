@article{ WOS:000312688300015,
Author = {Yin, Xiao-li and Tu, Jing-lun and Chen, Yu-lu and Xin, Xiang-jun and Yu,
   Chong-Xiu},
Title = {{Optimization of 24 GHz-band impulse radio ultra-wideband optical signal
   generator}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2013}},
Volume = {{287}},
Pages = {{85-89}},
Month = {{JAN 15}},
Abstract = {{Based on the optical architecture that uses optical pulse shaping to
   generate 24 GHz-band arbitrary impulse radio ultra-wideband (IR-UWB)
   signals, an optimization method is proposed to increase the spectral
   efficiency of the generated signals. Simulated annealing algorithm is
   used as the optimization method and the spectrum efficiency is utilized
   as the optimization condition. Numerical analysis shows that
   power-efficient FCC-Compliant IR-UWB signals are achieved for the given
   structure. Moreover, the relationship between the spectrum efficiency
   and the length of pulse trains and the robustness of the optical pulse
   shaper are analyzed. (C) 2012 Elsevier B.V. All rights reserved.}},
