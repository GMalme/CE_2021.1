@article{ WOS:000573788500040,
Author = {Cherukuri, Ashish and Cortes, Jorge},
Title = {{Cooperative Data-Driven Distributionally Robust Optimization}},
Journal = {{IEEE TRANSACTIONS ON AUTOMATIC CONTROL}},
Year = {{2020}},
Volume = {{65}},
Number = {{10}},
Pages = {{4400-4407}},
Month = {{OCT}},
Note = {{55th Annual Allerton Conference on Communication, Control, and Computing
   (Allerton), Monticello, IL, OCT 03-06, 2017}},
Organization = {{Coordinated Sci Lab; Univ Illinois, Dept Elect \& Comp Engn; IEEE; IEEE
   Informat Theory Soc; IEEE Control Syst Soc}},
Abstract = {{We study a class of multiagent stochastic optimization problems where
   the objective is to minimize the expected value of a function which
   depends on a random variable. The probability distribution of the random
   variable is unknown to the agents. The agents aim to cooperatively find,
   using their collected data, a solution with guaranteed out-of-sample
   performance. The approach is to formulate a data-driven distributionally
   robust optimization problem using Wasserstein ambiguity sets, which
   turns out to be equivalent to a convex program. We reformulate the
   latter as a distributed optimization problem and identify a
   convex-concave augmented Lagrangian, whose saddle points are in
   correspondence with the optimizers, provided a min-max
   interchangeability criteria is met. Our distributed algorithm design,
   then consists of the saddle-point dynamics associated to the augmented
   Lagrangian. We formally establish that the trajectories converge
   asymptotically to a saddle point and, hence, an optimizer of the
   problem. Finally, we identify classes of functions that meet the min-max
   interchangeability criteria.}},
