
@article{ WOS:000629325100001,
Author = {Honorio, Herminio T. and Martins, Bruno and Maliska, Clovis R.},
Title = {{A real-time optimization algorithm for the fixed-stress splitting scheme}},
Journal = {{INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN ENGINEERING}},
Year = {{2021}},
Volume = {{122}},
