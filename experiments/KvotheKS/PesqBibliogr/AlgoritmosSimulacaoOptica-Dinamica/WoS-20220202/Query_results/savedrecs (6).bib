@article{ WOS:000278512300013,
Author = {Jiang, Hai-ming and Xie, Kang and Wang, Ya-fei},
Title = {{Pump scheme for gain-flattened Raman fiber amplifiers using improved
   particle swarm optimization and modified shooting algorithm}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{11}},
Pages = {{11033-11045}},
Month = {{MAY 24}},
Abstract = {{An effective pump scheme for the design of broadband and flat gain
   spectrum Raman fiber amplifiers is proposed. This novel approach uses a
   new shooting algorithm based on a modified Newton-Raphson method and a
   contraction factor to solve the two point boundary problems of Raman
   coupled equations more stably and efficiently. In combination with an
   improved particle swarm optimization method, which improves the
   efficiency and convergence rate by introducing a new parameter called
   velocity acceptability probability, this scheme optimizes the
   wavelengths and power levels for the pumps quickly and accurately.
   Several broadband Raman fiber amplifiers in C + L band with optimized
   pump parameters are designed. An amplifier of 4 pumps is designed to
   deliver an average on-off gain of 13.3 dB for a bandwidth of 80 nm, with
   about +/- 0.5 dB in band maximum gain ripples. (C) 2010 Optical Society
   of America}},
