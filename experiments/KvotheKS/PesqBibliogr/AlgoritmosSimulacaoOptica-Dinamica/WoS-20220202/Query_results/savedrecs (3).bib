@article{ WOS:000613201200012,
Author = {Kamalakis, Thomas and Dogkas, Leonidas and Simou, Flora},
Title = {{Optimization of a discrete multi-tone visible light communication system
   using a mixed-integer genetic algorithm}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2021}},
Volume = {{485}},
Month = {{APR 15}},
Abstract = {{Visible light communications (VLC) are generally considered an important
   pillar of 5G and beyond 5G networks. In this paper, we show how a VLC
   system using multiple sub-carrier modulation and in particular, discrete
   multitone, can be designed using a mixed-integer genetic algorithm. We
   propose a suitable figure-of-merit that can serve as a fitness function
   for each candidate design. The estimation of the fitness function is
   carried out using a rigorous system model incorporating both the linear
   and the non-linear aspects of the transceiver. The algorithm can be used
   to identify a suitable set of values for various parameters of the
   system, including the power allocation at each sub-carrier, the
   modulation order, the sub-carrier spacing and the clipping level. Our
   Python implementation is publicly available on the web under an
   open-source license.}},
