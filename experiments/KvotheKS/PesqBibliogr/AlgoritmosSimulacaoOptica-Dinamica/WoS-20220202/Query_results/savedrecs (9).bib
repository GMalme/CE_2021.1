@article{ WOS:000446489700001,
Author = {Zhou, Qian and Qin, Xiaolin and Xie, Xiaojun},
Title = {{Dimension Incremental Feature Selection Approach for Vertex Cover of
   Hypergraph Using Rough Sets}},
