@article{ WOS:000388399100093,
Author = {Tripathi, Ashish and Mcnulty, Ian and Munson, Todd and Wild, Stefan M.},
Title = {{Single-view phase retrieval of an extended sample by exploiting edge
   detection and sparsity}},
Journal = {{OPTICS EXPRESS}},
Year = {{2016}},
Volume = {{24}},
Number = {{21}},
Pages = {{24719-24738}},
Month = {{OCT 17}},
Abstract = {{We propose a new approach to robustly retrieve the exit wave of an
   extended sample from its coherent diffraction pattern by exploiting
   sparsity of the sample's edges. This approach enables imaging of an
   extended sample with a single view, without ptychography. We introduce
   nonlinear optimization methods that promote sparsity, and we derive
   update rules to robustly recover the sample's exit wave. We test these
   methods on simulated samples by varying the sparsity of the
   edge-detected representation of the exit wave. Our tests illustrate the
   strengths and limitations of the proposed method in imaging extended
   samples. (C) 2016 Optical Society of America}},
