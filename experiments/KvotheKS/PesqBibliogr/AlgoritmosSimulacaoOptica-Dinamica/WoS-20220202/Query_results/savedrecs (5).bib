@article{ WOS:000252045400006,
Author = {Neto, B. and Teixeira, A. L. J. and Wada, N. and Andre, P. S.},
Title = {{Efficient use of hybrid Genetic Algorithms in the gain optimization of
   distributed Raman amplifiers}},
