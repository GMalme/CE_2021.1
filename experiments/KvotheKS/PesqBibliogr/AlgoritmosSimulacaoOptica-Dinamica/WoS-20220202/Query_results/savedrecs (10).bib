@article{ WOS:000265108900059,
Author = {van Turnhout, Maarten and Bociort, Florian},
Title = {{Chaotic behavior in an algorithm to escape from poor local minima in
   lens design}},
Journal = {{OPTICS EXPRESS}},
Year = {{2009}},
Volume = {{17}},
Number = {{8}},
Pages = {{6436-6450}},
Month = {{APR 13}},
Abstract = {{In lens design, damped least-squares methods are typically used to find
   the nearest local minimum to a starting configuration in the merit
   function landscape. In this paper, we explore the use of such a method
   for a purpose that goes beyond local optimization. The merit function
   barrier, which separates an unsatisfactory solution from a neighboring
   one that is better, can be overcome by using low damping and by allowing
   the merit function to temporarily increase. However, such an algorithm
   displays chaos, chaotic transients and other types of complex behavior.
   A successful escape of the iteration trajectory from a poor local
   minimum to a better one is associated with a crisis phenomenon that
   transforms a chaotic attractor into a chaotic saddle. The present
   analysis also enables a better understanding of peculiarities
   encountered with damped least-squares algorithms in conventional local
   optimization tasks. (C) 2009 Optical Society of America}},
