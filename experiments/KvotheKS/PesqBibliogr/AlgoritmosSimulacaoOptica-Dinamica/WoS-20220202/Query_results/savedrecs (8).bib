@article{ WOS:000226297700006,
Author = {Camelli, F and Lohner, R},
Title = {{Assessing maximum possible damage for contaminant release events}},
Journal = {{ENGINEERING COMPUTATIONS}},
Year = {{2004}},
Volume = {{21}},
Number = {{7-8}},
Pages = {{748-760}},
Abstract = {{The combined use of damage criteria, genetic algorithms and advanced CFD
   solvers provides an effective strategy to identify locations of releases
   that produce maximum damage. The implementation is simple and does not
   require any change to flow solvers. A rather general criterion has been
   formulated to determine the damage inflicted by the intentional or
   unintentional release of contaminants. Results of two typical cases show
   that damage can vary considerably as a function of release location,
   implying that genetic algorithms are perhaps the only techniques suited
   for this type of optimization problem.}},
