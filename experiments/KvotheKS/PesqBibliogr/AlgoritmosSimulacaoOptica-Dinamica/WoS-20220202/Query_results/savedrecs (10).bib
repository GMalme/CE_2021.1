@article{ WOS:000222641200009,
Author = {Koshel, RJ and Walmsley, IA},
Title = {{Non-edge-ray design: improved optical pumping of lasers}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2004}},
Volume = {{43}},
