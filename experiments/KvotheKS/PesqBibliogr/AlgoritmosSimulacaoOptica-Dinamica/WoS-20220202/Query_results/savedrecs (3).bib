@article{ WOS:000461678300137,
Author = {Jiang, Jin and Sun, Ke and Zheng, Xiongbo and Ju, Quanyong},
Title = {{The Blade Pitching Optimization Method of Vertical Axis Tidal Turbine to
   Maximum Power Coefficient}},
Journal = {{EKOLOJI}},
Year = {{2019}},
Volume = {{28}},
Number = {{107}},
Pages = {{1173-1184}},
Abstract = {{In order to improve the power coefficient and the self-starting
   performance of H-Darrieus tidal turbine, researchers have been studying
   the variable pitch turbines, mainly by types: active control and passive
   control system. Aiming at searching the blade pitching law to maximum
   the power coefficient (Cp) of variable pitch H-Darrieus turbine, we
   hypothesize that the hydraulic rotor is rotating with constant speed in
   uniform fluid velocity. Combining with single disk multiple stream-tube
   model and genetic algorithm, the optimum blade pitching laws are solved
   at any given tip speed ratio (TSR). The turbine Cp and the instantaneous
   loads are calculated through computational fluid dynamics (CFD) method
   after careful model verification based on Strickland's experiment.
   Compared with fixed pitch angle laws and those in some other published
   articles, the CFD results shows the optimization deflection angle law
   proposed has the maximum Cp at all chosen TSRs, and the peak of Cp
   increased by 21\% and 56.4\% in the whole range of TSR compared with
   traditional pitch fixed turbine. The results also show that optimum
   pitch turbines have the minimum thrust force fluctuation among all.}},
