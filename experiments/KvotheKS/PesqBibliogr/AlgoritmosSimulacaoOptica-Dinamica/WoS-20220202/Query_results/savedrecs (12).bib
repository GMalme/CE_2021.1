@article{ WOS:000085775000007,
Author = {Lemaire, TJ and Bassrei, A},
Title = {{Three-dimensional reconstruction of dielectric objects by the
   coupled-dipole method}},
Journal = {{APPLIED OPTICS}},
Year = {{2000}},
Volume = {{39}},
Number = {{8}},
Pages = {{1272-1278}},
Month = {{MAR 10}},
Abstract = {{We present an inversion procedure for electromagnetic scattering, based
   on the powerful and flexible technique called the coupled-dipole method
   combined with an optimization algorithm. This method permits us to
   realize imaging of dielectric objects whose dimensions are comparable
   with the incident wavelength and is shown to be efficient with corrupted
   data (scattered electric field). The feasibility of this method is shown
   in a synthetic example in which the scattered field is corrupted with
   Gaussian noise. Two methods are used to invert the scattered field to
   recover the refractive index of the medium: conventional matrix
   inversion and an iterative method. (C) 2000 Optical Society of America.}},
