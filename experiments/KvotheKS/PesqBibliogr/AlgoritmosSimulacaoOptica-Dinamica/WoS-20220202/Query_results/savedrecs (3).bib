@article{ WOS:000482244300046,
Author = {De Masi, Rosa Francesca and de Ross, Filippo and Ruggiero, Silvia and
   Vanoli, Giuseppe Peter},
Title = {{Numerical optimization for the design of living walls in the
   Mediterranean climate}},
