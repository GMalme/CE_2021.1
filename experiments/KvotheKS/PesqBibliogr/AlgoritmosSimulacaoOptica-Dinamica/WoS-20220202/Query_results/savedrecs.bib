@article{ WOS:000237100200011,
Author = {Bousson, K and Correia, SD},
Title = {{Optimization algorithm based on densification and dynamic canonical
   descent}},
Journal = {{JOURNAL OF COMPUTATIONAL AND APPLIED MATHEMATICS}},
Year = {{2006}},
Volume = {{191}},
Number = {{2}},
Pages = {{269-279}},
Month = {{JUL 1}},
Note = {{International Conference of Computational Methods in Sciences and
   Engineering (ICCMSE 2004), Athens, GREECE, NOV 19-23, 2004}},
Abstract = {{Stochastic methods have gained some popularity in global optimization in
   that most of them do not assume the cost functions to be differentiable.
   They have capabilities to avoid being trapped by local optima, and may
   converge even faster than gradient-based optimization methods on some
   problems. The present paper proposes an optimization method, which
