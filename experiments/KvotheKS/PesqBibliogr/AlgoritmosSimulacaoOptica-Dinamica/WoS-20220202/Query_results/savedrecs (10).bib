@article{ WOS:000078550300020,
Author = {Masters, BP and Crawley, EF},
Title = {{Evolutionary design of controlled structures}},
Journal = {{JOURNAL OF AIRCRAFT}},
Year = {{1999}},
Volume = {{36}},
Number = {{1}},
Pages = {{209-217}},
Month = {{JAN-FEB}},
Abstract = {{An evolutionary controls/structures design method is developed. The
   basis of the method is an accurate model formulation for dynamic
   compensator optimization coupled with a genetic-algorithm-based updating
   of sensor/actuator placement and structural properties. One- and
   three-dimensional examples from the literature are used to validate the
   method. Frequency domain interpretation of the controlled structure
   systems provide physical insight as to how the objective is optimized.
   Several designs for a stellar interferometer precision structure are
   found for two different spectra of disturbance under assumed closed-loop
   optical conditions. Physical limitations in achieving performance are
   given in terms of average system transfer function gain and system phase
