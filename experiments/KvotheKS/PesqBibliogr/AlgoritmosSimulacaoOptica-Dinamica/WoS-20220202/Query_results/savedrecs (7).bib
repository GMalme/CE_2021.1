@article{ WOS:A1995RW65400004,
Author = {TOWERS, DP and BUCKBERRY, CH and STOCKLEY, BC and JONES, MP},
Title = {{MEASUREMENT OF COMPLEX VIBRATIONAL-MODES AND SURFACE FORM - A COMBINED
   SYSTEM}},
