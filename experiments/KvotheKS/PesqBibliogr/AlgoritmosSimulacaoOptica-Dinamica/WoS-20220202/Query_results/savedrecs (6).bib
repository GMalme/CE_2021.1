@article{ WOS:000621718200002,
Author = {Guo, Youming and Wang, Jiaying and Kong, Ling and Chen, Kele and Gu,
   Naiting and Rao, Xuejun and Rao, Changhui},
Title = {{Vibration mitigation experiment on the Chinese Large Solar Telescope
   based on the linear quadratic Gaussian control}},
Journal = {{OPTIK}},
Year = {{2021}},
Volume = {{230}},
Month = {{MAR}},
Abstract = {{Vibrations caused by thermal control system severely affect the Chinese
   Large Solar Telescope (CLST). The linear quadratic Gaussian (LQG)
   controller has been tested on the solar adaptive optics (SAO) system
   equipped in the CLST to mitigate these vibrations. An automatic
   identification method based on the particle swarm optimization (PSO)
   algorithm is used to compute the parameters of the LQG controller and
   verified on the CLST. Experimental results are presented in this paper,
   which show a significant reduction in vibrations in both X and Y axes,
   illustrating the superior performance of the LQG controller compared to
   the conventional proportional-integral (PI) controller.}},
