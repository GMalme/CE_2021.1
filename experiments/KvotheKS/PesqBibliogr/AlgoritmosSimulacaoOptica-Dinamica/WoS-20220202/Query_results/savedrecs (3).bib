@article{ WOS:000454472000131,
Author = {Zhang, Dongxiao and Chen, Zhibin and Xiao, Cheng and Qin, Mengze and Wu,
   Hao},
Title = {{Accurate simulation of turbulent phase screen using optimization method}},
Journal = {{OPTIK}},
Year = {{2019}},
Volume = {{178}},
Pages = {{1023-1028}},
Abstract = {{We proposed an accurate method for simulating turbulent phase screen.
   Different from the subharmonic method, the low-frequency grid is not
   equally spaced in our method, and the distribution of sampling points
   can be determined by only one parameter. It is found that the accuracy
   of simulation depends on the distribution of sampling points, and there
   is an optimal distribution that minimizes the relative error between
   expected structure function and theoretical structure function. In order
   to get the optimal distribution, a search algorithm is designed.
   Compared with existing subharmonic method, the maximum relative error
   can be reduced from 10\% to 0.1\%, and the computation costs reduced
   more than two times at the same time. And compared with other methods,
   this method still has the advantage of precision.}},
