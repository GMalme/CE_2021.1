@article{ WOS:000546414500032,
Author = {Cuervo Pinto, Victor Dario and Rodriguez-Molina, Alejandro and German
   Corona-Ramirez, Leonel and Gabriel Villarreal-Cervantes, Miguel},
Title = {{Optimum Nozzle Design for a Viscous Liquid by Using Multi-Objective
   Search Approaches}},
