@article{ WOS:000080892200003,
Author = {Zeggel, T and Bryngdahl, O},
Title = {{Relation between grayvalue and noise frequencies in two- and three-level
   digital halftoning}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1999}},
Volume = {{164}},
Number = {{1-3}},
Pages = {{13-18}},
Month = {{JUN 1}},
Abstract = {{In digital halftoning a close relationship exists between the local
   grayvalues in the input image and the spatial frequencies of the
   quantization noise of the quantized image. Halftoning algorithms usually
   reproduce low frequencies well by introducing quantization noise of
   higher frequencies. We present and discuss a simple model for the
   relationship between grayvalue and quantization noise frequencies.
   Optimization experiments with two and three quantization levels are
   presented. (C) 1999 Elsevier Science B.V. All rights reserved.}},
