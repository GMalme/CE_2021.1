@article{ WOS:000641964800038,
Author = {Wang, Zheng and Liu, Ling and Ling, Cong},
Title = {{Sliced Lattice Gaussian Sampling: Convergence Improvement and Decoding
   Optimization}},
Journal = {{IEEE TRANSACTIONS ON COMMUNICATIONS}},
