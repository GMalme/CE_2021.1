@article{ WOS:000254469400005,
Author = {Ditz, P. and Borzi, A.},
Title = {{A cascadic monotonic time-discretized algorithm for finite-level quantum
   control computation}},
Journal = {{COMPUTER PHYSICS COMMUNICATIONS}},
Year = {{2008}},
Volume = {{178}},
Number = {{5}},
Pages = {{393-399}},
Month = {{MAR 1}},
Abstract = {{A computer package (CNMS) is presented aimed at the solution of
   finite-level quantum optimal control problems. This package is based on
   a recently developed computational strategy known as monotonic schemes.
   Quantum optimal control problems arise in particular in quantum optics
   where the optimization of a control representing laser pulses is
   required. The purpose of the external control field is to channel the
   system's waveftinction between given states in its most efficient way.
   Physically motivated constraints, such as limited laser resources, are
   accommodated through appropriately chosen cost functionals. (C) 2007
   Elsevier B.V. All rights reserved.}},
