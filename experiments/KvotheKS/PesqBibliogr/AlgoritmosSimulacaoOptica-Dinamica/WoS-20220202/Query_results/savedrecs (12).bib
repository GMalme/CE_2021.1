@article{ WOS:000187727400023,
Author = {Zheng, XP and Qin, T and Zhang, HY},
Title = {{Dynamic routing and wavelength assignment for two-stage
   multi-granularity WDM optical networks}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2004}},
Volume = {{229}},
Number = {{1-6}},
Pages = {{191-196}},
Month = {{JAN 2}},
Abstract = {{Multi-granularity optical cross-connect (MG-OXC) has been proposed to
   reduce the complexity and size of optical switch. This paper studies
   dynamic routing and wavelength assignment (RWA) algorithm for WDM
   optical networks consisting of MG-OXCs. We present an online adaptive
   routing algorithm to solve this problem. Simulation results show that
   for a two-stage multi-granularity optical network, the fiber-tunnel
   topology can greatly affect the blocking probability. To further improve
   the network performance, a fiber-tunnel topology optimization algorithm
   is proposed, and simulation results show that the blocking probability
   can be drastically reduced after fiber-tunnel topology reconfiguration.
   (C) 2003 Published by Elsevier B.V.}},
