@article{ WOS:000484088600030,
Author = {Wahl, Daniel J. and Zhang, Pengfei and Mocci, Jacopo and Quintavalla,
   Martino and Muradore, Riccardo and Jian, Yifan and Bonora, Stefano and
   Sarunic, V, Marinko and Zawadzki, Robert J.},
Title = {{Adaptive optics in the mouse eye: wavefront sensing based vs.
   image-guided aberration correction}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
Year = {{2019}},
Volume = {{10}},
Number = {{9}},
Pages = {{4757-4774}},
Month = {{SEP 1}},
Abstract = {{Adaptive Optics (AO) is required to achieve diffraction limited
   resolution in many real-life imaging applications in biology and
   medicine. AO is essential to guarantee high fidelity visualization of
   cellular structures for retinal imaging by correcting ocular
   aberrations. Aberration correction for mouse retinal imaging by direct
   wavefront measurement has been demonstrated with great success. However,
   for mouse eyes, the performance of the wavefront sensor (WFS) based AO
   can be limited by several factors including non-common path errors,
   wavefront reconstruction errors. and an ill-defined reference plane.
   Image-based AO can avoid these issues at the cost of algorithmic
   execution time. Furthermore, image-based approaches can provide
   improvements to compactness, accessibility, and even the performance of
   AO systems. Here, we demonstrate the ability of image-based AO to
   provide comparable aberration correction and image resolution to the
   conventional Shack-Hartmann WFS-based AO approach. The residual
   wavefront error of the mouse eye was monitored during a wavefront
   sensorless optimization to allow comparison with classical AO. This also
   allowed us to improve the performance of our AO system for small animal
   retinal imaging. (C) 2019 Optical Society of America under the terms of
   the OSA Open Access Publishing Agreement}},
