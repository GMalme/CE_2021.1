@article{ WOS:000357435200011,
Author = {Li, Shengfu and Montcel, Bruno and Yuan, Zhen and Liu, Wanyu and Vray,
   Didier},
Title = {{Multigrid-based reconstruction algorithm for quantitative photoacoustic
   tomography}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
Year = {{2015}},
Volume = {{6}},
Number = {{7}},
Pages = {{2424-2434}},
Month = {{JUL 1}},
Abstract = {{This paper proposes a multigrid inversion framework for quantitative
   photoacoustic tomography reconstruction. The forward model of optical
   fluence distribution and the inverse problem are solved at multiple
   resolutions. A fixed-point iteration scheme is formulated for each
   resolution and used as a cost function. The simulated and experimental
   results for quantitative photoacoustic tomography reconstruction show
   that the proposed multigrid inversion can dramatically reduce the
   required number of iterations for the optimization process without loss
   of reliability in the results. (C) 2015 Optical Society of America}},
