@article{ WOS:000407087900028,
Author = {Nascimento, E. O. and Oliveira, L. N.},
Title = {{Numerical Optimization of Flight Trajectory for Rockets Via Artificial
   Neural Networks}},
Journal = {{IEEE LATIN AMERICA TRANSACTIONS}},
Year = {{2017}},
Volume = {{15}},
Number = {{8}},
Pages = {{1556-1565}},
Month = {{AUG}},
Abstract = {{This research aims to optimize the flight trajectory for rockets. Then,
   it was applied hybrid techniques, based on the numerical implementation
   of the Finite Difference Method (FDM) in the scheme of the Euler Method
   to obtain the solution and the numerical analysis of the non-linear
   differential equations provided by the analytic modeling. So aiming at
   the optimizations were applied the Artificial Neural Networks (ANN) by
   the Levenberg-Marquardt training algorithm into two curves of the thrust
   rocket engines, in which it was possible to adjust a neural network for
   the control of temporal discretization in FDM, consequently an algorithm
   was developed on a numerical computational environment to perform all
   these previous steps. The results showed that after the use of ANN, the
