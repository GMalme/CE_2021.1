@article{ WOS:000401676700027,
Author = {Chai, Runqi and Savvaris, Al and Tsourdos, Antonios and Chai, Senchun},
Title = {{Multi-objective trajectory optimization of Space Manoeuvre Vehicle using
   adaptive differential evolution and modified game theory}},
