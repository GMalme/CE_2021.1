@article{ WOS:000505418300002,
Author = {Pawar, Suraj and San, Omer and Rasheed, Adil and Vedula, Prakash},
Title = {{A priori analysis on deep learning of subgrid-scale parameterizations
   for Kraichnan turbulence}},
