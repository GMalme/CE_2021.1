@article{ WOS:000690470800001,
Author = {Zhong, Qi and Wang, Xiele and Xie, Geng and Yang, Huayong and Yu, Cheng
   and Xu, Enguang and Li, Yanbiao},
Title = {{Analysis of Dynamic Characteristics and Power Losses of High Speed
   on/off Valve with Pre-Existing Control Algorithm}},
Journal = {{ENERGIES}},
Year = {{2021}},
Volume = {{14}},
Number = {{16}},
Month = {{AUG}},
Abstract = {{A high-speed on/off valve (HSV) is generally the core component of a
   digital hydraulic transmission system. Therefore, its dynamic
   characteristics often restrict the overall performance of the digital
   hydraulic system. Most of the current studies focus on the optimization
   on the dynamic characteristics or the energy characteristics, few
   studies have comprehensively considered the two characteristics of the
   valve together. In this paper, a pre-existing control algorithm (PECA)
   is proposed to improve the dynamic characteristics of the HSV, and
   simultaneously optimize the power losses of the HSV to improve its
   energy conversion efficiency. The results show that, compared with the
   traditional single-voltage driven strategy, the opening time of the PECA
   decreases by 29.4\%, the closing time decreases by 59.6\%, and the
   energy conversion rate increases by 7.9\%.}},
