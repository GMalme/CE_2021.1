@article{ WOS:000167751100001,
Author = {Tan, QF and Yan, YB and Jin, GF and Wu, MX},
Title = {{Design of diffractive optical element for true beam smoothing}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2001}},
Volume = {{189}},
Number = {{4-6}},
Pages = {{167-173}},
Month = {{MAR 15}},
Abstract = {{Diffractive optical element (DOE) can be used for beam smoothing. The
   phase of the DOE is generally designed by the optimization algorithm. In
   optimization, the intensities of the sampling points on the focal plane
   are optimized to be consistent with the required beam smoothing, but the
   other points are not consistent with such a requirement. In this paper.
   a new method with a more restrictive way in the optimization is proposed
   to design the DOE for realizing a true beam smoothing. The result
   obtained by this new method is that the intensity of any point on the
   focal plane is fully filled with the required demand, besides the used
   sampling points in the optimization. (C) 2001 Published by Elsevier
   Science B.V.}},
