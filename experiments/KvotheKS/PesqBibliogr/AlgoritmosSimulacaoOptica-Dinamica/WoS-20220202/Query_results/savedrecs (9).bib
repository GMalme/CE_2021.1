@article{ WOS:000328537600003,
Author = {You, Chenglong and Kong, Weijin and Cao, Kaihua and Wang, Zhao and Wang,
   Wenxin and Yun, Maojin and Liu, Junhai and Sun, Xin and Wang, Jiming},
Title = {{Optimization design and analysis of reflecting polarizing beam splitter
   based on metal-multilayer dielectric grating for 800 nm}},
Journal = {{JOURNAL OF MODERN OPTICS}},
Year = {{2013}},
Volume = {{60}},
Number = {{19}},
Pages = {{1598-1602}},
Month = {{NOV 1}},
Abstract = {{A new reflecting polarizing beam splitter (RPBS), based on a
   metal-multilayer dielectric grating (MMDG), was designed using rigorous
   coupled wave analysis method and a genetic algorithm. The RPBS reflects
   the TE wave in the -1st order and TM wave in the 0th order for 800nm.
   The optimized RPBS has an extinction ratio of over 20 dB, from 765nm to
   823nm, and a large angle, from 43.8 degrees to 60 degrees. At 800nm, the
   extinction ratios of the 0th and -1st orders reach a maximum of 41.0 dB
   and 53.2 dB, respectively. Furthermore, analysis suggests that the
   designed RPBS allows sufficient manufacture tolerance. These results
   show the potential of MMDGs in fabrication of a RPBS, which can be
   widely used in optical systems.}},
