@article{ WOS:000306272400014,
Author = {Bhowmik, Bishanka Brata and Gupta, Sumanta and Gangopadhyay, Ranjan},
Title = {{Enhancement of the equalization range in a nonlinear micro-ring
   resonator based all-optical packet power equalizer using a genetic
   algorithm}},
Journal = {{JOURNAL OF OPTICS}},
Year = {{2012}},
Volume = {{14}},
Number = {{7}},
Month = {{JUL}},
Abstract = {{We have proposed an optimization technique using a genetic algorithm
   (GA) to design an all-optical packet power equalizer, which uses launch
   power induced self-switching in a nonlinear micro-ring resonator (MRR)
   to achieve an enhanced equalization range. Moreover, we also show that
   using higher nonlinearity in MRRs the equalization range of power
   equalization can be further increased. The results show more than 8 dB,
   8 dB and 7 dB equalization ranges from all-optical packet power
   equalizers, using 13, 14 and 15 rings respectively, when the equalizers
   are optimized using our proposed GA based design method.}},
Publisher = {{IOP PUBLISHING LTD}},
Address = {{TEMPLE CIRCUS, TEMPLE WAY, BRISTOL BS1 6BE, ENGLAND}},
