@article{ WOS:000280842400022,
Author = {Yang, P. and Lei, X. and Yang, R. and Ao, M. and Dong, L. and Xu, B.},
Title = {{Fast and stable enhancement of the far-field peak power by use of an
   intracavity deformable mirror}},
Journal = {{APPLIED PHYSICS B-LASERS AND OPTICS}},
Year = {{2010}},
Volume = {{100}},
Number = {{3}},
Pages = {{591-595}},
Month = {{SEP}},
Abstract = {{We demonstrate that an intracavity deformable mirror (DM) is capable of
   compensating for the aberrations of a continuous-wave (CW) Nd:YAG
   solid-state laser and improving its far-field peak power fast and
   stably. The deformable mirror is controlled by a stochastic parallel
   gradient descend (SPGD) algorithm. Experimental results reveal that
   high-order-like transverse modes can be transformed into Gaussian-like
   modes quickly without any pinhole added in the laser cavity. At the same
   time, the far-field peak powers are also increased to different extents.}},
