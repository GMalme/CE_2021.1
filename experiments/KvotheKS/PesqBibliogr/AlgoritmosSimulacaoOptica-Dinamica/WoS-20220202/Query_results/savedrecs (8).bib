@article{ WOS:000609873500007,
Author = {Deng, Ji and Li, Jian and Feng, Hao and Xiao, Yu and Ding, Shumeng and
   Han, Wenzhong and Zeng, Zhoumo},
Title = {{Reconstruction-free fast depth segmentation via temporal orthogonal
   frequency fringe patterns}},
Journal = {{OPTICS AND LASER TECHNOLOGY}},
Year = {{2021}},
Volume = {{137}},
Month = {{MAY}},
Abstract = {{In many machine vision techniques, depth segmentation is a central
   process to segment targeted objects with abruptly changing depth into
   different regions for 3D data analysis. However, current techniques are
   limited by data-acquisition speed, computational cost, and the precision
   of segmentation. To overcome these challenges, this paper proposes a
   dynamic depth-segmentation technique that allows the segmentation of
   target objects into isolated depth regions without requiring
   three-dimensional reconstruction and background removal. Taking full
   advantage of the merits of the phase-shifting algorithm, singular-points
   theory about depth segmentation is summarized and applied to extract the
   segmenting lines as the markers for depth segmentation. Meanwhile, by
   using the proposed temporal orthogonal frequency scheme, the dynamic
   performance and the high-frequency components of the measured objects
   are both guaranteed during the computing process. Finally, a low-cost
   morphology-based optimization is created to solve the over-segmentation
   problem and enlarge the measurable dynamic range. The simulation result
   demonstrates the segmentation accuracy of this technique. Experimental
   results on objects of differing status show that the proposed approach
   provides satisfactory omnidirectional depth-segmentation results with
   high accuracy and fidelity.}},
