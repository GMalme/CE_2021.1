@article{ WOS:000417977900007,
Author = {Li, Ming and Li, Yupeng and Han, Jiawei},
Title = {{Gerchberg-Saxton algorithm based phase correction in optical wireless
   communication}},
Journal = {{PHYSICAL COMMUNICATION}},
Year = {{2017}},
Volume = {{25}},
Number = {{2}},
Pages = {{323-327}},
Month = {{DEC}},
Abstract = {{Optical wireless communication (OWC) enables to establish the backhaul
   in the fifth generation (5G) wireless communication networks. The air
   turbulence, however, could distort the phases of optical signals, thus
   limit transmission capacity. In this paper, we study the correction of
   phase distortions by using the Gerchberg-Saxton (GS) algorithm. The air
   turbulence-induced phase is generated by the Monte-Carlo phase screen
   method, which characterizes the realistic air turbulence effect on the
   optical signals. The numerical results reveal that the GS algorithm is
   able to retrieve the phase information with fast convergence by adopting
   a proper initial condition. Also, the GS algorithm based phase
   correction in OWC is confirmed. (c) 2017 Elsevier B.V. All rights
   reserved.}},
