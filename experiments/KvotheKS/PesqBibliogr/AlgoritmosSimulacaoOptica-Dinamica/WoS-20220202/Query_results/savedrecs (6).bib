
@article{ WOS:000287573800013,
Author = {Bloom, Guillaume and Larat, Christian and Lallier, Eric and
   Lee-Bouhours, Mane-Si Laure and Loiseaux, Brigitte and Huignard,
   Jean-Pierre},
Title = {{Design and optimization of a high-efficiency array generator in the
   mid-IR with binary subwavelength grooves}},
Journal = {{APPLIED OPTICS}},
Year = {{2011}},
Volume = {{50}},
Number = {{5}},
Pages = {{701-709}},
Month = {{FEB 10}},
Abstract = {{We have designed a high-efficiency array generator composed of
   subwavelength grooves etched in a GaAs substrate for operation at 4.5 mu
   m. The method used combines rigorous coupled wave analysis with an
   optimization algorithm. The optimized beam splitter has both a high
   efficiency (similar to 96\%) and a good intensity uniformity (similar to
   0.2\%). The fabrication error tolerances are numerically calculated, and
   it is shown that this subwavelength array generator could be fabricated
   with current electron beam writers and inductively coupled plasma
   etching. Finally, we studied the effect of a simple and realistic
   antireflection coating on the performance of the beam splitter. (C) 2011
   Optical Society of America}},
