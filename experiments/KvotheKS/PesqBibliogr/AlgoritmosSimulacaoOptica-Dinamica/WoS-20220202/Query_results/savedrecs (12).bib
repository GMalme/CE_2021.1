@article{ WOS:000274323600010,
Author = {Cui, Haihua and Liao, Wenhe and Cheng, Xiaosheng and Dai, Ning and Yuan,
   Tianran},
Title = {{A three-step system calibration procedure with error compensation for 3D
   shape measurement}},
Journal = {{CHINESE OPTICS LETTERS}},
Year = {{2010}},
Volume = {{8}},
Number = {{1}},
Pages = {{33-37}},
Month = {{JAN 10}},
Abstract = {{System calibration, which usually involves complicated and
   time-consuming procedures, is crucial for any three-dimensional (3D)
   shape measurement system based on vision. A novel improved method is
   proposed for accurate calibration of such a measurement system. The
   system accuracy is improved with considering the nonlinear measurement
   error created by the difference between the system model and real
   measurement environment. We use Levenberg-Marquardt optimization
   algorithm to compensate the error and get a good result. The improved
   method has a 50\% improvement of re-projection accuracy compared with
   our previous method. The measurement accuracy is maintained well within
   1.5\% of the overall measurement depth range.}},
