@article{ WOS:000352694300033,
Author = {Ran, Maopeng and Wang, Qing and Dong, Chaoyang and Ni, Maolin},
Title = {{Multistage anti-windup design for linear systems with saturation
   nonlinearity: enlargement of the domain of attraction}},
Journal = {{NONLINEAR DYNAMICS}},
Year = {{2015}},
