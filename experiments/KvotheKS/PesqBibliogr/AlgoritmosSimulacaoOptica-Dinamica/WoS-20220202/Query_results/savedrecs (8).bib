@article{ WOS:A1995QD59200009,
Author = {FETTHAUER, F and WEISSBACH, S and BRYNGDAHL, O},
Title = {{COMPUTER-GENERATED FRESNEL HOLOGRAMS - QUANTIZATION WITH THE ERROR
   DIFFUSION ALGORITHM}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1995}},
Volume = {{114}},
Number = {{3-4}},
Pages = {{230-234}},
Month = {{FEB 1}},
Abstract = {{Error diffusion is a quantization algorithm for both images and
   computer-generated holograms. Its free parameters allow the shaping of
   the quantization noise in the Fourier domain independent of the object
   information. Thus optimization strategies have been developed to find
   free parameters which allow the quantization of computer-generated
   Fourier holograms so that there is low noise in the regions where the
   object information lies. If the same weights are used to quantize a
   computer-generated Fresnel hologram the noise is drastically increased
   in those regions. A method is shown how a Fresnel hologram can be
   quantized with comparable noise as in the Fourier case. This method uses
   the same optimized weights of the Fourier case, so no further
   optimization is necessary.}},
