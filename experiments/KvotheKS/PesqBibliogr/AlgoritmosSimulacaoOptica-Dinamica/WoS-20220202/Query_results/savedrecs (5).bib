@article{ WOS:000254678100019,
Author = {Al-Baghdadi, Maher A. R. Sadiq},
Title = {{Three-dimensional computational fluid dynamics model of a tubular-shaped
   PEM fuel cell}},
Journal = {{RENEWABLE ENERGY}},
Year = {{2008}},
Volume = {{33}},
Number = {{6}},
Pages = {{1334-1345}},
Month = {{JUN}},
Abstract = {{A full three-dimensional, non-isothermal computational fluid dynamics
   model of a tubular-shaped proton exchange membrane (PEM) fuel cell has
   been developed. This comprehensive model accounts for the major
   transport phenomena in a PEM fuel cell: convective and diffusive heat
   and mass transfer, electrode kinetics, and potential fields. In addition
   to the tubular-shaped geometry, the model feature an algorithm that
   allows for more realistic representation of the local activation
   overpotentials which leads to improved prediction of the local current
   density distribution. Three-dimensional results of the species profiles,
   temperature distribution, potential distribution, and local current
   density distribution are presented. The model is shown to be able to
   understand the many interacting, complex electrochemical, and transport
   phenomena that cannot be studied experimentally. (C) 2007 Elsevier Ltd.
   All rights reserved.}},
