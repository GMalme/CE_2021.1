@article{ WOS:000380112400001,
Author = {Peng, Yifan and Fu, Qiang and Heide, Felix and Heidrich, Wolfgang},
Title = {{The Diffractive Achromat Full Spectrum Computational Imaging with
   Diffractive Optics}},
