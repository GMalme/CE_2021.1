@article{ WOS:000449898000028,
Author = {Yao, Yongxiang and Li, Jia and Duan, Ping and Cheng, Feng},
Title = {{Research on three-dimensional model reconstruction of slope erosion
   based on sequence images}},
Journal = {{OPTICS AND LASER TECHNOLOGY}},
Year = {{2019}},
Volume = {{110}},
Number = {{SI}},
Pages = {{209-218}},
Month = {{FEB}},
Abstract = {{To solve the problems of incomplete 2D display soil erosion, difficult
   visual expression, poor detail simulation of eroding slope, complex
   modeling technology, and high cost, a three-dimensional model
   reconstruction method for slope erosion based on sequence images is
   proposed. The method mainly uses structure from motion (SFM) and
   multi-view stereo (MVS) technology. First, the image sequence is feature
   matched using the scale-invariant feature transform algorithm for images
   captured by ordinary cameras. The random sampling and consensus
   algorithm is used to remove the false matching point generated by the
   nearest neighbor. Next, using the SFM method, the camera matrix and
   three-dimensional point coordinates are solved iteratively, and the
   nonlinear optimization is realized by bundle adjustment to distribute
   the errors evenly and improve the accuracy of the model. Finally, based
   on the patch-based multi-view stereo algorithm, the sparse point cloud
   generated by SFM is used as the seed patch to diffuse under local
   photometric uniformity and global visibility constraints to reconstruct
   the dense point cloud and establish the 3D model. To verify the
   reliability of the experimental methods, this paper takes the red soil
   erosion slope in Chenggong District of Kunming, Yunnan, China as the
   experimental object and realizes the rapid reconstruction of the
   three-dimensional model of the erosion gully on the slope. Multiple
   groups of adjacent eroded gully images are taken to verify the model,
   which involves method and accuracy of model. The results show that the
   modeling method has a good effect on soil modeling. The error from the
   control points is small and the relative accuracy is as high as 94.20\%,
   98.01\%, etc. As a result, the control points can provide a reference
   for corrosion monitoring research for its reliable results. (C) 2017
   Elsevier Ltd. All rights reserved.}},
