@article{ WOS:000288136500011,
Author = {Zhang, T. -L. and Chen, L. and Yan, Z. -H. and Li, B.},
Title = {{DESIGN OF DUAL OFFSET SHAPED REFLECTOR ANTENNA BASED ON DEGL ALGORITHM}},
Journal = {{JOURNAL OF ELECTROMAGNETIC WAVES AND APPLICATIONS}},
Year = {{2011}},
Volume = {{25}},
Number = {{5-6}},
Pages = {{723-732}},
Abstract = {{This paper presents a synthesis technique of the dual offset shaped
   reflector antenna using Differential Evolution with Global and Local
   neighborhoods (DEGL) algorithm. The main reflector and subreflector are
   parameterized by Zernike polynomial expansion. With these expansion
   coefficients, the antenna is optimized to achieve a contoured beam
   covering the China territory in Ku-band. The performance of the antenna
   is calculated by Physical Optics (PO) and Physical Theory of Diffraction
   (PTD) techniques. The calculated results are validated by the commercial
   software GRASP9.3.01.}},
