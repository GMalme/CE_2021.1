@article{ WOS:000293363200014,
Author = {Zhang, Yong and Sun, Guangyong and Li, Guangyao and Rungsiyakull, Chaiy
   and Li, Qing},
Title = {{Identification of Material Parameters for High Strength Steel Under
   Impact Loading}},
Journal = {{ADVANCED SCIENCE LETTERS}},
Year = {{2011}},
Volume = {{4}},
Number = {{3}},
Pages = {{708-714}},
Month = {{MAR}},
Abstract = {{Vehicle impact can be considered as a highly nonlinear dynamic process,
   where high strength steel severely deforms under different strain rates
   and also suffered from thermal softening. However, its constitutive
   material parameters may not consider the abovementioned effects in
   uniaxial tensile tests. Since accuracy and reliability of finite element
   predictions largely rely on material constitutive model, thus an
   accurate material model, which takes deformation rate and thermal
   effects into account, has to be established for a more accurate
   numerical simulation. In this respect, the Johnson-Cook (JC)
   constitutive model considers the effects of high-strain rate dependency,
   thermal softening and strain rate-temperature coupling on the material
   flow stress. This paper presents a new procedure to predict JC
   constitutive model parameters for DP800 high strength steel based on the
   successive response surface (RS) model and simulated annealing (SA)
   algorithm. Constitutive parameters were identified with respect to
   stress, strain rate, temperature of the material by matching the
   numerical simulation curve to the experimental data under impact
   loading. The presented procedure exhibits a good combination of low
   computational cost and high accuracy. It is thus recommended for
   identification of other similar high strength materials.}},
