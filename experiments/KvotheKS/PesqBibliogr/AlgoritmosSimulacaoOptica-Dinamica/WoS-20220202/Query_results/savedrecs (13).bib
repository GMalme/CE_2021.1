@article{ WOS:000535463200001,
Author = {Xie, Yaen and Zhang, Zhidan and Wu, Xiande and Shi, Zhen and Chen,
   Yangyang and Wu, Baixuan and Mantey, Kofi Akrofi},
Title = {{Obstacle Avoidance and Path Planning for Multi-Joint Manipulator in a
   Space Robot}},
