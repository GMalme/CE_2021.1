@article{ WOS:000324312400005,
Author = {Thomas, Benjamin and Momany, Michelle and Kner, Peter},
Title = {{Optical sectioning structured illumination microscopy with enhanced
   sensitivity}},
Journal = {{JOURNAL OF OPTICS}},
Year = {{2013}},
Volume = {{15}},
Number = {{9, SI}},
Month = {{SEP}},
Abstract = {{A new approach to reconstructing an optically sectioned image using
   structured illumination is presented. Compared to the algorithm proposed
   by Neil et al (1997 Opt. Lett. 22 1905), this method uses the same
   number of images to construct a final image with a flat linear transfer
   function at the cost of slightly more complexity in the reconstruction
   algorithm. Compared to other optical sectioning algorithms using
   structured illumination, this approach produces images with higher
   contrast and better image fidelity at low signal intensities.}},
