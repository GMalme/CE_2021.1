@article{ WOS:000237095600006,
Author = {Silvestre, E and Pinheiro-Ortega, T and Andres, P and Miret, JJ and
   Coves, A},
Title = {{Differential toolbox to shape dispersion behavior in photonic crystal
   fibers}},
Journal = {{OPTICS LETTERS}},
Year = {{2006}},
Volume = {{31}},
Number = {{9}},
Pages = {{1190-1192}},
Month = {{MAY 1}},
Abstract = {{We present an analytical procedure to compute the first derivatives of
   the propagation constants with respect to several structural parameters
   in photonic crystal fibers (PCFs). From them we can easily evaluate the
   same derivatives of other directly related magnitudes. The above
   derivatives provide the trend of the magnitude at issue, which allows us
   to take advantage of a gradient-based algorithm to shape the properties
   of the guiding structure. In this way we implement an optimization
   process to carry out real inverse design in PCFs. We focus our attention
   on designing PCFs with a specific chromatic dispersion behavior.
   Likewise, the same approach makes it possible to analyze their
   fabrication tolerances. (c) 2006 Optical Society of America.}},
