@article{ WOS:000537415900004,
Author = {Mishra, Gargi and Vishwakarma, Virendra P. and Aggarwal, Apoorva},
Title = {{Constrained L-1-optimal sparse representation technique for face
   recognition}},
Journal = {{OPTICS AND LASER TECHNOLOGY}},
Year = {{2020}},
Volume = {{129}},
Month = {{SEP}},
Abstract = {{The performance of face recognition (FR) system essentially depends on
   the availability of training data. With limited training data available,
   it creates a major challenge in real world applications. This motivates
   researchers to investigate for such technique which generates optimum
   results using limited training samples or even single sample per person
   (SSPP). In this paper, a novel FR method is developed as Constrained
   L-1-Optimal Sparse Representation Technique (L-1-CSRT) for SSPP. An
   optimal sparse representation technique is formulated using the
   constrained Cuckoo search algorithm (CSA) for estimation of
   coefficients. Further, a novel fitness function is developed based on
   the L-1-norm for better classification accuracy yielding better FR. The
   motivation behind the optimization of coefficients using CSA is to
   increase the sparsity by a better exploration of search space. The
   efficiency and accuracy of proposed L-1-CSRT is shown based on the
   extensive simulations carried out on standard databases. The
   experimental results are compared with existing methods in terms of mean
   classification error. The performance of L-1-CSRT is analysed with an
   improvement of 2-6\% in terms of classification accuracy.}},
