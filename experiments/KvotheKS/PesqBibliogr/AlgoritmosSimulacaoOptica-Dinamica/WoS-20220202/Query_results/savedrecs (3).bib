@article{ WOS:000494605200001,
Author = {Mueller, Jens-Dominik and Zhang, Xingchen and Akbarzadeh, Siamak and
   Wang, Yang},
Title = {{Geometric continuity constraints of automatically derived
   parametrisations in CAD-based shape optimisation}},
Journal = {{INTERNATIONAL JOURNAL OF COMPUTATIONAL FLUID DYNAMICS}},
Year = {{2019}},
Volume = {{33}},
Number = {{6-7, SI}},
Pages = {{272-288}},
Month = {{AUG 9}},
Abstract = {{CAD geometries are most often exchanged between analysis tools using
   NURBS patches to represent the boundary (BRep). We present a method
   where the control points of the BRep are used to automatically derive
   parametrisations suitable for shape optimisation with gradient-based
   methods. Particular focus is on ensuring geometric continuity between
   the NURBS patches through formulation of discrete constraints. Design
   variables then arise from an orthogonal basis to the remaining design
   space using a singular value decomposition (SVD). The manuscript
   presents the extension of earlier work on B-spline surfaces to full
   NURBS surfaces and investigates the effect of the SVD cut-off on the
   optimisation results. An estimation of the effective rank is proposed
   which allows to automatically determine a suitable cut-off value. The
   effectiveness of the algorithm is demonstrated for the minimisation of
   total pressure loss over a section of an automotive climate duct, and a
   U-bend cooling channel.}},
