@article{ WOS:000428519600020,
Author = {Safikhani, Hamed and Zamani, Javid and Musa, Mohammadreza},
Title = {{Numerical study of flow field in new design cyclone separators with one,
   two and three tangential inlets}},
Journal = {{ADVANCED POWDER TECHNOLOGY}},
