@article{ WOS:000080971500055,
Author = {Chen, L and Yu, C},
Title = {{Phase reversal electrode optimisation in broadband electro-optic
   modulators by genetic algorithm}},
Journal = {{ELECTRONICS LETTERS}},
Year = {{1999}},
Volume = {{35}},
Number = {{11}},
Pages = {{931-932}},
Month = {{MAY 27}},
Abstract = {{Phase reversal electrode optimisation by using a genetic algorithm for
   obtaining wide, flat frequency response in broadband electrooptic phase
   modulators is demonstrated. The genetic algorithm utilises a novel
   coding, crossover and mutation method directly based on electrode
   figures. The simulated calculations show that this method provides a
   rather flat frequency response of > 100GHz with an electrode length of
   1cm.}},
