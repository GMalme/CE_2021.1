@article{ WOS:000077929800004,
Author = {Yang, GG},
Title = {{An optical pickup using a diffractive optical element for a high-density
   optical disc}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1999}},
Volume = {{159}},
Number = {{1-3}},
Pages = {{19-22}},
Month = {{JAN 1}},
Abstract = {{An optical pickup using the combination of an objective lens and a
   diffractive optical element for a high-density optical disk is proposed
   in this paper. The new system will enhance the resolution and extend the
   depth of focus as required in high-density compact disc (CD) pickup
   systems. Optimisation algorithms are used to synthesise the diffractive
   optical element (DOE) to meet a detailed specification of the point
   spread function (PSF) of the system. The theoretical simulation and
   experimental results show that the system produces good resolution
   enhancement, extended depth of focus, high diffraction efficiency and
   moderate sidelobe level. (C) 1999 Elsevier Science B.V. All rights
   reserved.}},
