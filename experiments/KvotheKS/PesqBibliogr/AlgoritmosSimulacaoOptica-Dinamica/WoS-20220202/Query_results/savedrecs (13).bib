@article{ WOS:000436215700030,
Author = {Guo, Jiamin and Zhou, Guangen and Zhou, Dai and Chen, Weigang and Xiong,
   ZhiXin and Dong, Shilin},
Title = {{Cable fracture simulation and experiment of a negative Gaussian
   curvature cable dome}},
Journal = {{AEROSPACE SCIENCE AND TECHNOLOGY}},
Year = {{2018}},
Volume = {{78}},
Pages = {{342-353}},
Month = {{JUL}},
Abstract = {{This paper describes a newly developed negative Gaussian cable-strut
   structure. It focuses on the fracture of a member using an experimental
   model and the corresponding vector form intrinsic finite element (VFIFE)
   model. First, this study considered the VFIFE algorithm for bars,
   cables, and beams. Next, an experimental model of a negative Gaussian
   curvature cable dome with its supporting system and the corresponding
   VFIFE model were built. Then, the given ridge cable was fractured using
   a shear force, and the structural dynamic response during fracture was
   tested. Finally, to further study the influence of fracture on the
   structure, different sections of three main ridge cables were selected,
   and their fractures were successively simulated using the VFIFE model.
   The results indicated that the VFIFE was an efficient and accurate way
   to simulate the member fracture of a negative Gaussian curvature cable
   dome. Furthermore, the results confirmed that members in a section with
   a negative curvature not only have a much greater effect on the
   structure but also are much more sensitive to member fracture. (C) 2018
   Elsevier Masson SAS. All rights reserved.}},
