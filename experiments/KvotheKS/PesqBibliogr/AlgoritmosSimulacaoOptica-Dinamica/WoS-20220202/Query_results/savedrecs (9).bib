@article{ WOS:000281779600009,
Author = {Mores, Jr., J. A. and Malheiros-Silveira, G. N. and Fragnito, H. L. and
   Hernandez-Figueroa, H. E.},
Title = {{Efficient calculation of higher-order optical waveguide dispersion}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
