@article{ WOS:000561810900001,
Author = {Okundalaye, Oluwaseun Olumide and Othman, Wan Ainun Mior and Kumaresan,
   Nallasamy},
Title = {{Optimal Homotopy Asymptotic Method-Least Square for Solving Nonlinear
   Fractional-Order Gradient-Based Dynamic System from an Optimization
   Problem}},
Journal = {{ADVANCES IN MATHEMATICAL PHYSICS}},
Year = {{2020}},
Volume = {{2020}},
Month = {{JUL 26}},
Abstract = {{In this paper, we consider an approximate analytical method of optimal
   homotopy asymptotic method-least square (OHAM-LS) to obtain a solution
   of nonlinear fractional-order gradient-based dynamic system (FOGBDS)
   generated from nonlinear programming (NLP) optimization problems. The
   problem is formulated in a class of nonlinear fractional differential
   equations, (FDEs) and the solutions of the equations, modelled with a
   conformable fractional derivative (CFD) of the steepest descent
   approach, are considered to find the minimizing point of the problem.
   The formulation extends the integer solution of optimization problems to
   an arbitrary-order solution. We exhibit that OHAM-LS enables us to
   determine the convergence domain of the series solution obtained by
   initiating convergence-control parameter C-j ` s. Three illustrative
   examples were included to show the effectiveness and importance of the
   proposed techniques.}},
