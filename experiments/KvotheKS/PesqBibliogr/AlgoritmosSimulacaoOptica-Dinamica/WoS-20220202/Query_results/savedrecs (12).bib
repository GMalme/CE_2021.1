@article{ WOS:000288719100009,
Author = {Du, Peijun and Zhang, Wei and Xia, Junshi},
Title = {{Hyperspectral remote sensing image classification based on decision
   level fusion}},
Journal = {{CHINESE OPTICS LETTERS}},
Year = {{2011}},
Volume = {{9}},
Number = {{3}},
Month = {{MAR 10}},
Abstract = {{To apply decision level fusion to hyperspectral remote sensing (HRS)
   image classification, three decision level fusion strategies are
   experimented on and compared, namely, linear consensus algorithm,
   improved evidence theory, and the proposed support vector machine (SVM)
   combiner. To evaluate the effects of the input features on
   classification performance, four schemes are used to organize input
   features for member classifiers. In the experiment, by using the
   operational modular imaging spectrometer (OMIS) II HRS image, the
   decision level fusion is shown as an effective way for improving the
   classification accuracy of the FIRS image, and the proposed SVM combiner
   is especially suitable for decision level fusion. The results also
   indicate that the optimization of input features can improve the
   classification performance.}},
