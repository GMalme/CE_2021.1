@article{ WOS:000187355900007,
Author = {Tyler, DW and Schulze, KJ},
Title = {{Fast phase spectrum estimation using the parallel part-bispectrum
   algorithm}},
Journal = {{PUBLICATIONS OF THE ASTRONOMICAL SOCIETY OF THE PACIFIC}},
Year = {{2004}},
Volume = {{116}},
Number = {{815}},
Pages = {{65-76}},
Month = {{JAN}},
Abstract = {{Adaptive optics image data acquired on large telescopes nearly always
   need postdetection processing. We have designed a new algorithm for fast
   phase estimation from the bispectrum. Our innovative
   ``part-bispectrum{''} approach to phase estimation addresses the large,
   instantaneous memory required to form the bispectrum of data measured on
   large-format detectors. The part-bispectrum approach has been further
   extended to run on parallel machines, hence the name ``parallel
   part-bispectrum{''} algorithm. We describe the concept, design, and
   optimization of the algorithm and provide execution times for a variety
   of scenarios.}},
