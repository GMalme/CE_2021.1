@article{ WOS:000273307300037,
Author = {Yang, P. and Yang, R. and Dong, L. and Lei, X. and Xu, B.},
Title = {{Hartmann phase pick-up method for detection and correction of piston
   aberrations in a multi-beam coherent combination system}},
Journal = {{APPLIED PHYSICS B-LASERS AND OPTICS}},
Year = {{2010}},
Volume = {{98}},
Number = {{2-3}},
Pages = {{465-469}},
Month = {{FEB}},
Abstract = {{In multi-beam coherent combination laser systems, piston aberration
   between each sub-beam is often regarded as the dominative factor to
   deteriorate the combination performance. So as to obtain a favorable
   combination result, a Hartmann phase pick-up method in combination with
   a seven-element active segmented mirror is presented to pick up and
   correct the piston aberrations of a seven-beam coherent combination
   system. A simulative result indicates that the RMS value of the
   wavefront can be reduced from 401 nm to 3.7 nm, while the far-field
   Strehl ratio can be increased from 0.22 to 0.99 after correction.}},
