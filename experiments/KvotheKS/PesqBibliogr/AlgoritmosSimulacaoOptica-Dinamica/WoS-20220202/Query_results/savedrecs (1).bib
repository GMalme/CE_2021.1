@article{ WOS:000245762700027,
Author = {Peng, Jian-Xun and Li, Kang and Thompson, Steve and Wieringa, Peter A.},
Title = {{Distribution-based adaptive bounding genetic algorithm for continuous
   optimisation problems}},
