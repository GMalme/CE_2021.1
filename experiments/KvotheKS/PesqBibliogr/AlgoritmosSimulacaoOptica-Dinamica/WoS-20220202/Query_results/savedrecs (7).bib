@article{ WOS:000422935900151,
Author = {Cao, Zhong-Chen and Cheung, Chi Fai and Liu, Ming Yu},
Title = {{Model-based self-optimization method for form correction in the computer
   controlled bonnet polishing of optical freeform surfaces}},
