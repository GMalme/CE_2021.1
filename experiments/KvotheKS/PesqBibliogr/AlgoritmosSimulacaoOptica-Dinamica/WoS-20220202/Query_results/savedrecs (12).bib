@article{ WOS:000461473400001,
Author = {Vigano, Nicola and Gil, Pablo Martinez and Herzog, Charlotte and de la
   Rochefoucauld, Ombeline and van Liere, Robert and Batenburg, Kees Joost},
Title = {{Advanced light-field refocusing through tomographic modeling of the
   photographed scene}},
Journal = {{OPTICS EXPRESS}},
Year = {{2019}},
Volume = {{27}},
Number = {{6}},
Pages = {{7834-7856}},
Month = {{MAR 18}},
Abstract = {{Recently we have shown that light-field photography images can he
   interpreted as limited-angle cone-beam tomography acquisitions. Here, we
   use this property to develop a direct-space tomographic refocusing
   formulation that allows one to refocus both unfocused and focused
   light-field images. We express the reconstruction as a convex
   optimization problem, thus enabling the use of various regularization
   terms to help suppress artifacts, and a wide class of existing advanced
   tomographic algorithms. This formulation also supports super-resolved
   reconstructions and the correction of the optical system's limited
   frequency response (point spread function). We validate this method with
   numerical and real-world examples. (C) 2019 Optical Society of America
   under the terms of the OSA Open Access Publishing Agreement}},
