@article{ WOS:000301397500003,
Author = {Pons-Prats, J. and Bugeda, G. and Zarate, F. and Onate, E.},
Title = {{Robust Design Optimization applied to aeronautics combining stochastic
   calculus with evolutionary algoritms}},
Journal = {{REVISTA INTERNACIONAL DE METODOS NUMERICOS PARA CALCULO Y DISENO EN
   INGENIERIA}},
Year = {{2012}},
Volume = {{28}},
Number = {{1}},
Pages = {{18-32}},
Abstract = {{Uncertainties are a daily issue to deal with in aerospace engineering
   and applications. Robust optimization methods commonly use a random
   generation of the inputs and take advantage of multi-point criteria to
   look for robust solutions accounting with uncertainty definition. From
   the computational point of view, the application to coupled problems,
   like fluid-dynamics (CFD) or fluid-structure interaction (FSI), can be
   extremely expensive. This work presents a coupling between stochastic
   analysis techniques and evolutionary optimization algorithms for the
   definition of a stochastic robust optimization procedure. At first, a
   stochastic procedure is proposed to be applied into optimization
   problems. The proposed method has been applied to both CFD and FSI
   problems for the reduction of drag and flutter, respectively. (C) 2010
   CIMNE (Universitat Politecnica de Catalunya). Published by Elsevier
   Espana, S.L. All rights reserved.}},
