@article{ WOS:000312431300015,
Author = {Mader, Charles A. and Martins, Joaquim R. R. A.},
Title = {{Derivatives for Time-Spectral Computational Fluid Dynamics Using an
   Automatic Differentiation Adjoint}},
Journal = {{AIAA JOURNAL}},
Year = {{2012}},
Volume = {{50}},
