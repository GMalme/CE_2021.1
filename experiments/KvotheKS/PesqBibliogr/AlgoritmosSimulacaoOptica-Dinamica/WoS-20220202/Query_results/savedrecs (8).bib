@article{ WOS:000371693300009,
Author = {Zardashti, R. and Yazdanpanah, M. J. and Nikkhah, A. A.},
Title = {{Nonlinear Multiobjective Time-Dependent TF/TA Trajectory Planning Using
   a Network Flow-Based Algorithm}},
Journal = {{JOURNAL OF AEROSPACE ENGINEERING}},
