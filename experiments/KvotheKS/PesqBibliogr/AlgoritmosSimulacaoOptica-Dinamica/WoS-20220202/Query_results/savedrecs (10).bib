@article{ WOS:A1997XG97500002,
Author = {Johnson, EG and Abushagur, MAG},
Title = {{Image deconvolution using a micro genetic algorithm}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{1997}},
