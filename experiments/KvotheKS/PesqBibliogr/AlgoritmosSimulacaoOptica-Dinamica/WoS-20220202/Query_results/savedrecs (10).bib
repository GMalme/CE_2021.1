@article{ WOS:000621988100001,
Author = {Blasi, Luciano and Borrelli, Mauro and D'Amato, Egidio and di Grazia,
   Luigi Emanuel and Mattei, Massimiliano and Notaro, Immacolata},
Title = {{Modeling and Control of a Modular Iron Bird}},
