@article{ WOS:000446117700016,
Author = {Wu, Jin and Zhou, Zebo and Fourati, Hassen and Cheng, Yuhua},
Title = {{A Super Fast Attitude Determination Algorithm for Consumer-Level
   Accelerometer and Magnetometer}},
Journal = {{IEEE TRANSACTIONS ON CONSUMER ELECTRONICS}},
Year = {{2018}},
Volume = {{64}},
Number = {{3}},
Pages = {{375-381}},
Month = {{AUG}},
Abstract = {{A super fast attitude solution is obtained for consumer electronics
   accelerometer-magnetometer combination. The quaternion parameterizing
   the orientation is analytically derived from a least-square optimization
   that maintains very simple form. Like previously developed approaches,
   this algorithm does not require predetermined magnetometer reference
   vector. It has been proven in the paper that the proposed algorithm is
   equivalent to two recent representative methods. Computational
   complexity analysis shows that the proposed algorithm has the least
   floating-number operations. Comparisons with recent and classical
   methods indicate the definite superiority of the proposed algorithm on
   execution time in real embedded applications.}},
