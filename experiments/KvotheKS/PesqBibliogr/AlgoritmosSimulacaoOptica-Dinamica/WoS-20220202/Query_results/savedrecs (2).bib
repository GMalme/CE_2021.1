@article{ WOS:000290834100006,
Author = {Jansen, J. D.},
Title = {{Adjoint-based optimization of multi-phase flow through porous media - A
   review}},
Journal = {{COMPUTERS \& FLUIDS}},
Year = {{2011}},
Volume = {{46}},
Number = {{1, SI}},
Pages = {{40-51}},
