@article{ WOS:000448658700081,
Author = {Papanikolaou, Michail and Pagone, Emanuele and Georgarakis, Konstantinos
   and Rogers, Keith and Jolly, Mark and Salonitis, Konstantinos},
Title = {{Design Optimisation of the Feeding System of a Novel Counter-Gravity
   Casting Process}},
Journal = {{METALS}},
Year = {{2018}},
Volume = {{8}},
Number = {{10}},
Month = {{OCT}},
Abstract = {{The appropriate design of feeders in a rigging system is critical for
   ensuring efficient compensation for solidification shrinkage, thus
   eliminating (shrinkage-related) porosity and contributing to the
   production of superior quality castings. In this study, a
   multi-objective optimisation framework combined with Computational Fluid
   Dynamics (CFD) simulations has been introduced to investigate the effect
   of the feeders' geometry on shrinkage porosity aiming to optimise
   casting quality and yield for a novel counter-gravity casting process
   (CRIMSON). The weighted sum technique was employed to convert this
   multi-objective optimisation problem to a single objective one.
   Moreover, an evolutionary multi-objective optimisation algorithm
   (NSGA-II) has been applied to estimate the trade-off between the
   objective functions and support decision makers on selecting the optimum
   solution based on the desired properties of the final casting product
   and the process characteristics. This study is one of the first attempts
   to combine CFD simulations with multi-objective optimisation techniques
   in counter-gravity casting. The obtained results indicate the benefits
   of applying multi-objective optimisation techniques to casting
   processes.}},
