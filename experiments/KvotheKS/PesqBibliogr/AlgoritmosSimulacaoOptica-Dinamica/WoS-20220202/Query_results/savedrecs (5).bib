@article{ WOS:000235012000003,
Author = {Brady, GR and Fienup, JR},
Title = {{Nonlinear optimization algorithm for retrieving the full complex pupil
   function}},
Journal = {{OPTICS EXPRESS}},
Year = {{2006}},
Volume = {{14}},
Number = {{2}},
Pages = {{474-486}},
Month = {{JAN 23}},
Note = {{OSA Topical Meeting on Signal Recovery and Synthesis, Charlotte, NC,
   JUN, 2005}},
Organization = {{Opt Soc Amer}},
Abstract = {{Techniques for retrieving the phase of an optical field typically depend
   on assumptions about the amplitude of the field in a desired plane,
   usually a pupil plane. We describe an approach that makes no such
   assumptions and is capable of retrieving both the amplitude and phase in
   the desired plane. Intensity measurements in two or more planes are used
   by a nonlinear optimization algorithm to retrieve the phase in the
   measurement planes. The complex field (amplitude and phase) in the
   desired plane is then computed by simple propagation. We show simulation
   results and examine the convergence of the algorithm. (c) 2005 Optical
   Society of America.}},
