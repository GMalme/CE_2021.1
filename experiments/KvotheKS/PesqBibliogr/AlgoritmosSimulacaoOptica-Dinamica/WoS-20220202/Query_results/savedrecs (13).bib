@article{ WOS:000171806300007,
Author = {Haddad, WM and Chellaboina, V and Kumar, R},
Title = {{Multiobjective L-1/H-infinity controller design for systems with
   frequency and time domain constraints}},
Journal = {{EUROPEAN JOURNAL OF CONTROL}},
Year = {{2000}},
Volume = {{6}},
Number = {{2}},
Pages = {{170-183}},
Abstract = {{lit this paper we develop, an optimal mixed-norm L-1 bound/H-infinity
   and l(1) bound/H-infinity controller synthesis framework for
   continuous-time and discrete-time systems. This multiobjective problem
   is treated by forming a convex combination of both L-1 (time domain
   worst-case peak amplitude response) and entropy (frequency domain
   worst-case H-infinity disturbance attenuation) performance measures. For
   flexibility in controller synthesis, we adopt the approach of
   fixed-structure controller design which allows consideration of
   arbitrary controller structures, including order, internal structure,
   and decentralization. Finally, using a quasi-Newton continuation
   algorithm, we demonstrate the effectiveness, of the proposed mixed-norm
   L-1/ H infinity approach via two numerical design examples.}},
