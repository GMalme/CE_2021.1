@article{ WOS:000659629700001,
Author = {Nisar, Kashif and Sabir, Zulqurnain and Zahoor Raja, Muhammad Asif and
   Ag. Ibrahim, Ag. Asri and Rodrigues, Joel J. P. C. and Shahid Khan,
   Adnan and Gupta, Manoj and Kamal, Aldawoud and Rawat, Danda B.},
Title = {{Evolutionary Integrated Heuristic with Gudermannian Neural Networks for
   Second Kind of Lane-Emden Nonlinear Singular Models}},
Journal = {{APPLIED SCIENCES-BASEL}},
Year = {{2021}},
Volume = {{11}},
Number = {{11}},
Month = {{JUN}},
Abstract = {{In this work, a new heuristic computing design is presented with an
   artificial intelligence approach to exploit the models with feed-forward
   (FF) Gudermannian neural networks (GNN) accomplished with global search
   capability of genetic algorithms (GA) combined with local convergence
   aptitude of active-set method (ASM), i.e., FF-GNN-GAASM to solve the
   second kind of Lane-Emden nonlinear singular models (LE-NSM). The
   proposed method based on the computing intelligent Gudermannian kernel
   is incorporated with the hidden layer configuration of FF-GNN models of
   differential operatives of the LE-NSM, which are arbitrarily associated
   with presenting an error-based objective function that is used to
   optimize by the hybrid heuristics of GAASM. Three LE-NSM-based examples
   are numerically solved to authenticate the effectiveness, accurateness,
   and efficiency of the suggested FF-GNN-GAASM. The reliability of the
   scheme via statistical valuations is verified in order to authenticate
   the stability, accuracy, and convergence.}},
