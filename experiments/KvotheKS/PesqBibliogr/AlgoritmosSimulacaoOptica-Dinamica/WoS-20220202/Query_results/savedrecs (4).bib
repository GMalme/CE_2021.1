@article{ WOS:000187880100017,
Author = {Bociort, F and van Driel, E and Serebriakov, A},
Title = {{Networks of local minima in optical system optimization}},
Journal = {{OPTICS LETTERS}},
Year = {{2004}},
Volume = {{29}},
Number = {{2}},
Pages = {{189-191}},
Month = {{JAN 15}},
Abstract = {{We discuss a surprising new feature of the merit function landscape in
   optical system design. When certain conditions are satisfied, the local
   minima form a network in which all nodes are connected. Each link
   between two neighboring minima contains a saddle point with a Morse
   index of 1. For a simple global optimization search (the symmetric Cooke
   triplet), the network of the corresponding set of local minima is
   presented. (C) 2004 Optical Society of America}},
