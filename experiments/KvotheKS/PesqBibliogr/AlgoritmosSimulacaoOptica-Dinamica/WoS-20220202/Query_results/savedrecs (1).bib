@article{ WOS:000309522400031,
Author = {Lei, Xiang and Wang, Shuai and Yan, Hu and Liu, Wenjin and Dong, Lizhi
   and Yang, Ping and Xu, Bing},
Title = {{Double-deformable-mirror adaptive optics system for laser beam cleanup
   using blind optimization}},
Journal = {{OPTICS EXPRESS}},
Year = {{2012}},
Volume = {{20}},
Number = {{20}},
Pages = {{22143-22157}},
Month = {{SEP 24}},
Abstract = {{An optimization-based correction method is developed to control
   simultaneously two deformable mirrors in a wavefront-sensor-less
   adaptive beam cleanup system, where the wave-front aberrations could not
   be compensated by a single deformable mirror. Stochastic parallel
   gradient decent algorithm is chosen as the optimization algorithm. In
   this control method, different aberrations are assigned to each
   deformable mirror according to their different correction quality. The
   method is proved to be effective by numerical simulations as well as
   experiments. Experimental results showed that the area containing 84\%
   energy of the laser beam in the far-field can reach 3.0 times
   diffraction limited. (C) 2012 Optical Society of America}},
