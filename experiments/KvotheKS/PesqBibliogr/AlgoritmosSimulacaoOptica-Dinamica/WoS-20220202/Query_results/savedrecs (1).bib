@article{ WOS:000478812400001,
Author = {Wang, Jinbo and Cui, Naigang and Wei, Changzhu},
Title = {{Rapid trajectory optimization for hypersonic entry using a
   pseudospectral-convex algorithm}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART G-JOURNAL OF
   AEROSPACE ENGINEERING}},
Year = {{2019}},
Volume = {{233}},
Number = {{14}},
Pages = {{5227-5238}},
Month = {{NOV}},
Abstract = {{Aiming at improving the autonomy of hypersonic entry vehicles, a rapid
   trajectory optimization algorithm, which has the potential to be
   implemented online and onboard, is proposed in this paper. The nonlinear
   and nonconvex hypersonic entry trajectory optimization problem is
   transformed into a series of convex subproblems through a proper
   combination of the pseudospectral method and an improved successive
   convexification method; thus, the high discretization accuracy of the
   pseudospectral method and the fast and deterministic convergence
   properties of the convex-optimization-based algorithm can be
   simultaneously exploited. The resulting subproblems can be solved
   efficiently by matured interior-point methods, and the solution
   converges rapidly by adopting a novel dynamic trust-region updating
   approach. The optimality of the solution is verified by the optimal
   control theory. The effectiveness of the algorithm is demonstrated by
   numerical experiments.}},
