@article{ WOS:A1995QJ41000005,
Author = {GANZ, A and WANG, XD},
Title = {{VIRTUAL TOPOLOGY DESIGN OF SHARED CHANNEL LIGHTWAVE NETWORKS}},
Journal = {{FIBER AND INTEGRATED OPTICS}},
Year = {{1995}},
Volume = {{14}},
Number = {{1}},
Pages = {{53-69}},
Abstract = {{Channel sharing in a WDM-based lightwave network provides a way to
   reduce the number of wavelengths required for the implementation of a
   large network, while the network virtual connectivity can be
   dramatically increased.  In this paper, we first introduce a so-called
   group policy that achieves a virtual topology with maximum connectivity
   and simplifies the optimal design of a traffic sensitive virtual
   topology.  We propose a two-stage algorithm:  a node-to-group assignment
