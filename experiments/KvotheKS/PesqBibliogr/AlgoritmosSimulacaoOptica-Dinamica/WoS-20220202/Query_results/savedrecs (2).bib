@article{ WOS:000410809400021,
Author = {Prada-i-Nogueira, Isaac and Alvarez-Ruiz, Francisco-Javier and
   De-Cuadra, Fernando and Sanchez-Miralles, Alvaro},
Title = {{Boiler inlet duct shape design optimization for combined cycle power
   plants}},
