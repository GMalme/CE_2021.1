@article{ WOS:000319793600005,
Author = {Enayatifar, Rasul and Abdullah, Abdul Hanan and Lee, Malrey},
Title = {{A weighted discrete imperialist competitive algorithm (WDICA) combined
   with chaotic map for image encryption}},
Journal = {{OPTICS AND LASERS IN ENGINEERING}},
Year = {{2013}},
Volume = {{51}},
Number = {{9}},
Pages = {{1066-1077}},
Month = {{SEP}},
Abstract = {{In recent years, there has been increasing interest in the security of
   digital images. This study focuses on binary image encryption using the
   weighted discrete imperialist competitive algorithm (WDICA).
   In the proposed method, a chaotic map is first used to create a
   specified number of cipher images. Then, to improve the results, WDICA
   is applied to the cipher images. In this study, entropy and correlation
   coefficient are used as WDICA's fitness functions. The goal is to
   maximize the entropy and minimize correlation coefficients. The
   advantage of this method is its ability to optimize the outcome of all
   iterations using WDICA. Simulation results show that WDICA not only
   demonstrates excellent encryption but also resists various typical
   attacks. The obtained correlation coefficient and entropy of the
   proposed WDICA are approximately 0.004 and 7.9994, respectively. (C)
   2013 Elsevier Ltd. All rights reserved.}},
