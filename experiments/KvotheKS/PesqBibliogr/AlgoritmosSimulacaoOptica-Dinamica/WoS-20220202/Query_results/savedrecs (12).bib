@article{ WOS:000733215600027,
Author = {Zhao, Yi and Zhang, Qi and Li, Zhipei and Xin, Xiangjun and Gao, Ran and
   Chai, Furong and Tao, Ying and Tian, Qinghua and Tian, Feng and Chen,
   Dong and Qian, Jinxin and Yang, Mai},
Title = {{Dynamic routing, modulation, and spectrum assignment based on fuzzy
   logic control in elastic optical networks}},
Journal = {{APPLIED OPTICS}},
Year = {{2022}},
Volume = {{61}},
Number = {{1}},
Pages = {{223-230}},
Month = {{JAN 1}},
Abstract = {{Considering the constantly changing network resources and randomly
   generated spectrum fragmentation problem, a dynamic routing, modulation,
   and spectrum assignment based on the fuzzy logic control (RMSA-FLC)
   algorithm is proposed in this paper. A fuzzy logic control (FLC) system
   based on the degree of fragmentation optimization and the degree of link
   selection is constructed. The path-control weight (PW) is achieved as
   the output of the FLC system, and the path and spectrum allocation
   scheme with the maximum PW is selected for the immediate reservation
   requests. The simulation results show that the proposed RMSA-FLC
   algorithm can effectively reduce the blocking probability and improve
   the spectrum resource utilization in a dynamic elastic optical network.
   (C) 2021 Optical Society of America}},
