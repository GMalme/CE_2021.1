@article{ WOS:000343874900033,
Author = {Hu, Xiao-ming and Zhang, Feng-juan and Dong, Fei and Zhou, Ya},
Title = {{Three-dimensional illumination procedure for photodynamic therapy of
   dermatology}},
Journal = {{JOURNAL OF BIOMEDICAL OPTICS}},
Year = {{2014}},
Volume = {{19}},
Number = {{9}},
Month = {{SEP}},
Abstract = {{Light dosimetry is an important parameter that affects the efficacy of
   photodynamic therapy (PDT). However, the irregular morphologies of
   lesions complicate lesion segmentation and light irradiance adjustment.
   Therefore, this study developed an illumination demo system comprising a
   camera, a digital projector, and a computing unit to solve these
   problems. A three-dimensional model of a lesion was reconstructed using
   the developed system. Hierarchical segmentation was achieved with the
   superpixel algorithm. The expected light dosimetry on the targeted
   lesion was achieved with the proposed illumination procedure. Accurate
   control and optimization of light delivery can improve the efficacy of
   PDT. (C) The Authors. Published by SPIE under a Creative Commons
   Attribution 3.0 Unported License.}},
