@article{ WOS:000254685700006,
Author = {Piggott, M. D. and Gorman, G. J. and Pain, C. C. and Allison, P. A. and
   Candy, A. S. and Martin, B. T. and Wells, M. R.},
Title = {{A new computational framework for multi-scale ocean modelling based on
   adapting unstructured meshes}},
Journal = {{INTERNATIONAL JOURNAL FOR NUMERICAL METHODS IN FLUIDS}},
