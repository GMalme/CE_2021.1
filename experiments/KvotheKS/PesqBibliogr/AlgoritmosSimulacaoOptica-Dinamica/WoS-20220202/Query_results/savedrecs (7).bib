@article{ WOS:000314770400001,
Author = {Karimi, J. and Pourtakdoust, Seid H.},
Title = {{Integrated motion planning and trajectory control system for unmanned
   air vehicles}},
Journal = {{PROCEEDINGS OF THE INSTITUTION OF MECHANICAL ENGINEERS PART G-JOURNAL OF
   AEROSPACE ENGINEERING}},
Year = {{2013}},
Volume = {{227}},
Number = {{G1}},
Pages = {{3-18}},
Month = {{JAN}},
Abstract = {{Motion planning and trajectory control are two basic challenges of
   unmanned vehicles. In motion planning problem, feasible trajectories are
   developed while nonlinear dynamic model and performance constraints of
   the vehicle under utility are considered. In this study, motion planning
   is performed via an enhanced particle swarm optimization algorithm. The
   resulting offline generated trajectories are tracked using a nonlinear
   trajectory control system methodology. The Lyapunov-based constrained
   backstepping approach and command filters are utilized in designing the
   trajectory control system. Command filters smoothen the input signals
   and provide their derivatives. Evaluation of the proposed integrated
   approach in several simulated scenarios has effectively demonstrated the
   potential of both algorithms in generating optimal contour matching
   trajectories as well as excellent tracking capability of the trajectory
   control system.}},
