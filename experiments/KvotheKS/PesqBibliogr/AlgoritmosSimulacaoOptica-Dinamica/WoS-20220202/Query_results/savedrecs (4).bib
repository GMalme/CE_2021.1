@article{ WOS:000562055800009,
Author = {Cai, Jingfeng and Chen, Ming and Huang, Nuo and Qian, Junhui},
Title = {{DC bias and power optimization for AV-DCO-OFDM in optical wireless
   communication}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2020}},
Volume = {{473}},
Month = {{OCT 15}},
Abstract = {{In this paper, we investigate the optimization of direct current (DC)
   bias, information-bearing power and subcarrier power allocation in
   absolute valued DC biased optical orthogonal frequency division
   multiplexing (AV-DCO-OFDM) systems. First, we analyze the effect of
   absolute value operation on the system performance using the Bussgang
   Theorem. Then, we consider the maximization of achievable rate under the
   optical power constraint. Using the optimality conditions, the
   optimization problem can be simplified and two efficient algorithms are
   developed to obtain the solutions in flat channels and dispersive
   channels, respectively. Simulation results validate the theoretical
   analysis and the effectiveness of the developed algorithms.}},
