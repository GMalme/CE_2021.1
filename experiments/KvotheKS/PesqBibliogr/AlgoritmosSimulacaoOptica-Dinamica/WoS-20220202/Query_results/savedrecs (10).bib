@article{ WOS:000412792200002,
Author = {Riedelbauch, Stefan},
Title = {{Energy transition requires hydropower IHS research focus}},
Journal = {{WASSERWIRTSCHAFT}},
Year = {{2017}},
Volume = {{107}},
Number = {{10}},
Pages = {{12-17}},
Abstract = {{Wide operation ranges of hydropower turbines automatically result in
   off-design conditions for the machines. Those operation conditions are
   often connected to vortical flow fields leading to high dynamic loads of
   the mechanical structure. Accurate flow field simulations need large
   computational grids and high resolution hybrid turbulence models. The
   additional consideration and modelling of cavitation further increases
   the complexity. Precise experimental investigations on model turbines
   allow the validation of such simulations. The design of turbine
   components using automatic optimization procedures requiring high level
   mathematical algorithms may generate geometry shapes deviating from
   known experience horizons. This paper gives an overview on the current
   main research topics of the Institute of Fluid Mechanics and Hydraulic
   Machinery (IHS) of Stuttgart University.}},
