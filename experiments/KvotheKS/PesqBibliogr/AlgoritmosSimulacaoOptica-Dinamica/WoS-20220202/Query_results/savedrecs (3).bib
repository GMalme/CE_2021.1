@article{ WOS:000291846800014,
Author = {Lin, Zone-Ching and Yang, Ching-Been},
Title = {{Establishment of a Near-Field Photolithography Processing Experiment
   Prediction Model and Parameter Optimization Model}},
Journal = {{JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE}},
Year = {{2011}},
Volume = {{8}},
Number = {{7}},
Pages = {{1183-1192}},
Month = {{JUL}},
Abstract = {{For analysis using the Taguchi method, the L18 or L27 orthogonal array
   is usually adopted. However, this requires many experiments (18 or 27
   runs, respectively), which takes time and increases costs. This paper
   proposes a progressive Taguchi neural network model that combines the
   Taguchi method with an artificial neural network (ANN), and constructs a
   prediction model for near field photolithography experiments. The
   analytical results of the progressive Taguchi neural network model show
   that, because there are few training examples in the Stage 1 preliminary
   network, there is a significant fluctuation the network prediction
   values. In the Stage 2 refining network the prediction effect in the
   region around the Taguchi factor level points is not bad, but the
   prediction in the region more remote from the learning and training
   examples has greater error. The Stage 3 precise network can provide
   optimal prediction results for the full field. Finally, we combine a
   near-field optical photolithography fabrication experiment prediction
   model and the Levenberg Marquardt search algorithm to establish a
   near-field optic photolithography fabrication parameter optimization
   model. The near-field photolithography fabrication parameter
   optimization model established in this paper can improve experimental
   accuracy and has referential value for near-field photolithography
   fabrication applications.}},
