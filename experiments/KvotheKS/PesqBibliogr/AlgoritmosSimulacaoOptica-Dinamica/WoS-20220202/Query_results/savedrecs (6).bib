@article{ WOS:000087538300015,
Author = {Poloni, C and Giurgevich, A and Onesti, L and Pediroda, V},
Title = {{Hybridization of a multi-objective genetic algorithm, a neural network
   and a classical optimizer for a complex design problem in fluid dynamics}},
Journal = {{COMPUTER METHODS IN APPLIED MECHANICS AND ENGINEERING}},
Year = {{2000}},
Volume = {{186}},
Number = {{2-4}},
Pages = {{403-420}},
Abstract = {{This paper describes the combination of several optimization
   technologies that can be used to tackle challenging design problems. The
   approach, that uses a multi-objective genetic algorithm, a neural
   network, and a gradient-based optimizer, is first outlined with the help
   of a computationally inexpensive mathematical test function. Then the
   methodology is applied to the design of a sailing yacht fin keel,
   coupling the optimization codes to 3D Navier-Stokes simulations. To
   perform the multi-objective optimization task a parallel computer is
   employed. (C) 2000 Elsevier Science S.A. All rights reserved.}},
