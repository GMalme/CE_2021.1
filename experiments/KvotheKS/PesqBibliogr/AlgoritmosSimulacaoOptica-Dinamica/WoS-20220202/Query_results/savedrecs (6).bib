@article{ WOS:000397687300012,
Author = {Wang, Q. and Elbouz, M. and Alfalou, A. and Brosseau, C.},
Title = {{Designing a composite correlation filter based on iterative optimization
   of training images for distortion invariant face recognition}},
Journal = {{OPTICS AND LASERS IN ENGINEERING}},
Year = {{2017}},
Volume = {{93}},
Pages = {{100-108}},
Month = {{JUN}},
Abstract = {{We present a novel method to optimize the discrimination ability and
   noise robustness of composite filters. This method is based on the
   iterative preprocessing of training images which can extract boundary
   and detailed feature information of authentic training faces, thereby
   improving the peak-to-correlation energy (PCE) ratio of authentic faces
   and to be immune to intra-class variance and noise interference. By
   adding the training images directly, one can obtain a composite template
   with high discrimination ability and robustness for face recognition
   task. The proposed composite correlation filter does not involve any
   complicated mathematical analysis and computation which are often
   required in the design of correlation algorithms. Simulation tests have
   been conducted to check the effectiveness and feasibility of our
   proposal. Moreover, to assess robustness of composite filters using
   receiver operating characteristic (ROC) curves, we devise a new method
   to count the true positive and false positive rates for which the
   difference between PCE and threshold is involved.}},
