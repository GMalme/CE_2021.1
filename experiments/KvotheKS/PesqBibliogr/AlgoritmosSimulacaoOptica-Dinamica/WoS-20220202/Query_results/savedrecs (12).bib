@article{ WOS:000710928500049,
Author = {Meng, Fanchao and Zhao, Xiaoting and Ding, Jinmin and Niu, Yingli and
   Zhang, Xinghua and Yang, Lvyun and Wang, Xin and Lou, Shuqin and Sheng,
   Xinzhi and Tao, Guangming and Liang, Sheng},
Title = {{Discovering extremely low confinement-loss anti-resonant fibers via
   swarm intelligence}},
Journal = {{OPTICS EXPRESS}},
Year = {{2021}},
Volume = {{29}},
Number = {{22}},
Pages = {{35544-35555}},
Month = {{OCT 25}},
Abstract = {{In this work, we obtain extremely low confinement-loss (CL)
   anti-resonant fibers (ARFs) via swarm intelligence, specifically the
   particle swarm optimization (PSO) algorithm. We construct a complex
   search space of ARFs with two layers of cladding and nested tubes. There
   are three and four structures of cladding tubes in the first and second
   layer, respectively. The ARFs are optimized by using the PSO algorithm
   in terms of both the structures and the parameters. The optimal
   structure is obtained from a total of 415900 ARF5 structures, with the
   lowest CL being 2.839x10(-7) dB/m at a wavelength of 1.55 mu m. We
   observe that the number of ARF structures with CL less than 1x10(-6)
   dB/m in our search space is 370. These structures mainly comprise four
   designs of ARFs. The results show that the optimal ARF structures
   realized by the PSO algorithm are different from the ARFs reported in
   the previous literature. This means that the swarm intelligence
   accelerates the design and invention of ARF5 and also provides new
   insights regarding the ARF structures. This work provides a fast and
   effective approach to design ARF5 with special requirements. In addition
   to providing high-performance ARF structures, this work transforms the
   ARF designs from experience-driven to data-driven. (C) 2021 Optical
   Society of America under the terms of the OSA Open Access Publishing
   Agreement}},
