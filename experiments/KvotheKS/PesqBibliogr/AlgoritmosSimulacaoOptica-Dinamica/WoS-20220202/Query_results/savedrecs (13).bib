@article{ WOS:000372039000015,
Author = {Wang, Ling and Xu, Mingen and Zhang, LieLie and Zhou, QingQing and Luo,
   Li},
Title = {{Automated quantitative assessment of three-dimensional bioprinted
   hydrogel scaffolds using optical coherence tomography}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
