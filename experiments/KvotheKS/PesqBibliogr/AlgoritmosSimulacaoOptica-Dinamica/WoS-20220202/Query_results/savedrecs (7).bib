@article{ WOS:000238351500012,
Author = {Karimov, P and Kurmaev, EZ and Kawai, J},
Title = {{Numerical simulation of planar X-ray waveguides with low-Z cladding
   layers}},
Journal = {{BUNSEKI KAGAKU}},
Year = {{2006}},
Volume = {{55}},
Number = {{6}},
Pages = {{447-452}},
Month = {{JUN}},
Abstract = {{A numerical simulation was performed to check the aptitude of low-Z
   materials, like carbon and boron, to be used as cladding layers in
   planar X-ray waveguides with a beryllium core. For a particular incident
   beam-energy value, an optimization technique based on a genetic
   algorithm was used to find the waveguide layer thicknesses, which
   provided the highest electric field intensity resonant enhancement in
   the core layer. The results of C/Be/C and B/Be/B waveguide simulations
   for incident beam energies from 10 to 15 keV were compared with the
   results obtained for Mo/Be/Mo waveguides. This,comparison showed that
   waveguides with low-Z cladding layers should provide a guided beam of
   high intensity.}},
