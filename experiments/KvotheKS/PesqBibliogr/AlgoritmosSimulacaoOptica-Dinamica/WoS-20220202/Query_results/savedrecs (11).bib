@article{ WOS:000395953900003,
Author = {Sommer, A. -E. and Wagner, M. and Reinecke, S. F. and Bieberle, M. and
   Barthel, F. and Hampel, U.},
Title = {{Analysis of activated sludge aerated by membrane and monolithic spargers
   with ultrafast X-ray tomography}},
