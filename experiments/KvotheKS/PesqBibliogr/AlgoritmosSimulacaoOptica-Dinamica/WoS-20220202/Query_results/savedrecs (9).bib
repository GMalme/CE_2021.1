@article{ WOS:000378016000042,
Author = {Chen, Mohan and Jiang, Xiang-Wei and Zhuang, Houlong and Wang, Lin-Wang
   and Carter, Emily A.},
Title = {{Petascale Orbital-Free Density Functional Theory Enabled by Small-Box
   Algorithms}},
Journal = {{JOURNAL OF CHEMICAL THEORY AND COMPUTATION}},
