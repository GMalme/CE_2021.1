@article{ WOS:000290747700015,
Author = {Assirelli, Melissa and Xu, Weiyin and Chew, Wee},
Title = {{Reactor Kinetics Studies via Process Raman Spectroscopy, Multivariate
   Chemometrics, and Kinetics Modeling}},
