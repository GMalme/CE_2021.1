@article{ WOS:000413416100057,
Author = {Sun, Yi and Cai, Haoyuan and Wang, Xiaoping},
Title = {{Theoretical analysis of metamaterial-gold auxiliary grating sensing
   structure for surface plasmon resonance sensing application based on
   polarization control method}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2017}},
Volume = {{405}},
Pages = {{343-349}},
Month = {{DEC 15}},
Abstract = {{A metamaterial-gold multilayer sensing structure designed using the
   particle swarm optimization (PSO) algorithm with an auxiliary grating is
   proposed for using in a surface plasmon resonance (SPR) sensor system
   based on the polarization control method. After numerical calculations
   and simulation analysis, it was found that the metamaterial sensing
   structure significantly improves the sensitivity of the SPR signal with
   intensity singularity. The metamaterial sensing structure also increases
   the penetration depth of evanescent wave, making it possible to detect
   low-molecular-weight biomolecules and larger cells such as bacteria. The
   auxiliary grating structure was designed to identify the refractive
   index of the sensing region on both sides of intensity singularity. The
   stability of recognition and the electric field intensity of the visible
   light band were also studied. (C) 2017 Published by Elsevier B.V.}},
