@article{ WOS:000428346900026,
Author = {Dubois, Arthur and Farhat, Charbel and Abukhwejah, Abdullah H. and
   Shageer, Hesham Mohamed},
Title = {{Parameterization Framework for the MDAO of Wing Structural Layouts}},
Journal = {{AIAA JOURNAL}},
Year = {{2018}},
Volume = {{56}},
Number = {{4}},
Pages = {{1627-1638}},
