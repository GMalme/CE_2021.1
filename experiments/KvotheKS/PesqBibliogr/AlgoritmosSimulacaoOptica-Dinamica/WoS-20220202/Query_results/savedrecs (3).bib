@article{ WOS:000227195400018,
Author = {Lieutenant, K},
Title = {{Optimization of neutron scattering instruments using different
   optimization routines}},
Journal = {{JOURNAL OF PHYSICS-CONDENSED MATTER}},
Year = {{2005}},
Volume = {{17}},
Number = {{5, SI}},
Pages = {{S167-S174}},
