@article{ WOS:000302042500005,
Author = {Dong, Wende and Feng, Huajun and Xu, Zhihai and Li, Qi},
Title = {{Multi-frame blind deconvolution using sparse priors}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2012}},
Volume = {{285}},
Number = {{9}},
Pages = {{2276-2288}},
Month = {{MAY 1}},
Abstract = {{In this paper, we propose a method for multi-frame blind deconvolution.
   Two sparse priors, i.e., the natural image gradient prior and an
   l(1)-norm based prior are used to regularize the latent image and point
   spread functions (PSFs) respectively. An alternating minimization
   approach is adopted to solve the resulted optimization problem. We use
   both gray scale blurred frames from a data set and some colored ones
   which are captured by a digital camera to verify the robustness of our
   approach. Experimental results show that the proposed method can
   accurately reconstruct PSFs with complex structures and the restored
   images are of high quality. (C) 2012 Elsevier B.V. All rights reserved.}},
