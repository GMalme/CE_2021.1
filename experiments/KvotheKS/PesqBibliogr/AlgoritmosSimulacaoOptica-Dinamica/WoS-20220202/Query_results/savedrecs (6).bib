@article{ WOS:000428268300014,
Author = {Ma, Qian and Xia, Houping and Xu, Qiang and Zhao, Lei},
Title = {{Combining kernel matrix optimization and regularization to improve
   particle size distribution retrieval}},
Journal = {{JOURNAL OF MODERN OPTICS}},
Year = {{2018}},
Volume = {{65}},
Number = {{8}},
Pages = {{999-1008}},
Abstract = {{A new method combining Tikhonov regularization and kernel matrix
   optimization by multi-wavelength incidence is proposed for retrieving
   particle size distribution (PSD) in an independent model with improved
   accuracy and stability. In comparison to individual regularization or
   multi-wavelength least squares, the proposed method exhibited better
   anti-noise capability, higher accuracy and stability. While standard
   regularization typically makes use of the unit matrix, it is not
   universal for different PSDs, particularly for Junge distributions.
   Thus, a suitable regularization matrix was chosen by numerical
   simulation, with the second-order differential matrix found to be
   appropriate for most PSD types.}},
