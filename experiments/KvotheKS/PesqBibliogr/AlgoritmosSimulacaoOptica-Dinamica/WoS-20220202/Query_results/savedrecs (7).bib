@article{ WOS:000232451300021,
Author = {Wang, XQ and Kiamilev, F and Papen, GC and Ekman, J and Gui, P and
   McFadden, MJ and Deroba, JC and Haney, MW and Kuznia, C},
Title = {{Performance-based adaptive power optimization for digital optical
   interconnects}},
Journal = {{APPLIED OPTICS}},
Year = {{2005}},
Volume = {{44}},
Number = {{29}},
Pages = {{6240-6252}},
Month = {{OCT 10}},
Abstract = {{Optical links are traditionally set to transmit maximum power for
   worst-case loss and consequently to dissipate more power than is
   required. We describe a technique to minimize power consumption based on
   the measured bit-error rate (BER) of the link. This technique uses a
   novel power-negotiation algorithm that optimizes the link power setting
   to achieve minimum power dissipation for a target BER. A 0.5 mu m
   complementary metal-oxide semiconductor optical transceiver chip was
   fabricated, and a free-space optical interconnect system was built for
   validation. The results showed that the algorithm was able to find the
   optimum power settings for the VCSELs for a target BER and to account
   for dynamic changes such as variation in the optical loss in the system.
   (c) 2005 Optical Society of America.}},
