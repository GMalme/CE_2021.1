@article{ WOS:000398937900063,
Author = {Wan, Qiushi and Deng, Yadong and Su, Chuqi and Wang, Yiping},
Title = {{Optimization of a Localized Air Conditioning System Using Thermoelectric
   Coolers for Commercial Vehicles}},
