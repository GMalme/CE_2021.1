@article{ WOS:000437335700005,
Author = {Daroczy, Laszlo and Janiga, Gabor and Thevenin, Dominique},
Title = {{Computational fluid dynamics based shape optimization of airfoil
   geometry for an H-rotor using a genetic algorithm}},
