@article{ WOS:000229689400023,
Author = {Pfeifer, T and Kemmer, R and Spitzenpfeil, R and Walter, D and
   Winterfeldt, C and Gerber, G and Spielmann, C},
Title = {{Spatial control of high-harmonic generation in hollow fibers}},
Journal = {{OPTICS LETTERS}},
Year = {{2005}},
Volume = {{30}},
Number = {{12}},
Pages = {{1497-1499}},
Month = {{JUN 15}},
Abstract = {{We demonstrate the control of high-harmonic generation in a hollow fiber
   by shaping the spatial structure of the generating laser pulse. We use a
   liquid-crystal-based two-dimensional spatial light modulator to control
   the spatial phase of the driver pulse. An evolutionary algorithm finds
   the spatial laser phase distribution that is optimal for reaching
   maximum total harmonic yield and for selectively enhancing the cutoff
   region of the spectrum. We show that enhacement of harmonic generation
   is related to coupling into a single fiber mode. Our results directly
   show that spatial properties of the laser are important parameters in
   fully controlling the high-harmonic spectrum. It is thus not possible to
   derive the controllability of the high-harmonic generation from the
   single-atom response only. (c) 2005 Optical Society of America}},
