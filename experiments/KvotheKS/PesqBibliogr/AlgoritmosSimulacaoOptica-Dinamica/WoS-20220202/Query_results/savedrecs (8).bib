@article{ WOS:000292970600006,
Author = {Mori, Syogo and Sugiyama, Masashi and Ogawa, Hidemitsu and Kitagawa,
   Katsuichi and Irie, Kei},
Title = {{Automatic parameter optimization of the local model fitting method for
   single-shot surface profiling}},
Journal = {{APPLIED OPTICS}},
Year = {{2011}},
Volume = {{50}},
Number = {{21}},
Pages = {{3773-3780}},
Month = {{JUL 20}},
Abstract = {{The local model fitting (LMF) method is a single-shot surface profiling
   algorithm. Its measurement principle is based on the assumption that the
   target surface to be profiled is locally flat, which enables us to
   utilize the information brought by nearby pixels in the single
   interference image for robust LMF. Given that the shape and size of the
   local area is appropriately determined, the LMF method was demonstrated
   to provide very accurate measurement results. However, the appropriate
   choice of the local area often requires prior knowledge on the target
   surface profile or manual parameter tuning. To cope with this problem,
   we propose a method for automatically determining the shape and size of
   local regions only from the single interference image. The effectiveness
   of the proposed method is demonstrated through experiments. (C) 2011
   Optical Society of America}},
