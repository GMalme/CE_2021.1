@article{ WOS:000326149500013,
Author = {Marques-Hueso, Jose and Sanchis, Lorenzo and Cluzel, Benoit and De
   Fornel, Frederique and Martinez-Pastor, Juan P.},
Title = {{Properties of silicon integrated photonic lenses: bandwidth, chromatic
   aberration, and polarization dependence}},
Journal = {{OPTICAL ENGINEERING}},
