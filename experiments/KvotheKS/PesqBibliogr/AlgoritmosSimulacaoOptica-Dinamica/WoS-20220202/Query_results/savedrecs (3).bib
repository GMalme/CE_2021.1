@article{ WOS:000354856600009,
Author = {Lee, Soobum and Semperlotti, Fabio},
Title = {{Design optimization for passive adaptive structural networks}},
Journal = {{JOURNAL OF INTELLIGENT MATERIAL SYSTEMS AND STRUCTURES}},
Year = {{2015}},
Volume = {{26}},
Number = {{9, SI}},
