@article{ WOS:000590673600003,
Author = {Zhang, Han and Gao, Xueping and Sun, Bowen and Qin, Zixue and Zhu,
   Hongtao},
Title = {{Parameter analysis and performance optimization for the vertical pipe
   intake-outlet of a pumped hydro energy storage station}},
Journal = {{RENEWABLE ENERGY}},
