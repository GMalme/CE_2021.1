   Zanconati, F. and Accardo, A. and Tromba, G. and Gureyev, T. E.},
Title = {{Clinical application of low-dose phase contrast breast CT: methods for
   the optimization of the reconstruction workflow}},
Journal = {{BIOMEDICAL OPTICS EXPRESS}},
