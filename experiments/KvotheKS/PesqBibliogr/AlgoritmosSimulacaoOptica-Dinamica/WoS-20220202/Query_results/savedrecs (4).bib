@article{ WOS:000234898200008,
Author = {Alcantara, LDS and Lima, MAC and Cesar, AC and Borges, BHV and Teixeira,
   FL},
Title = {{Design of a multifunctional integrated optical isolator switch based on
   nonlinear and nonreciprocal effects}},
Journal = {{OPTICAL ENGINEERING}},
Year = {{2005}},
Volume = {{44}},
Number = {{12}},
Month = {{DEC}},
Abstract = {{The design of a new multifunctional integrated optical device capable of
   operating simultaneously as an optical switch and optical isolator is
   presented. The device consists of a multilayer directional coupler
   employing one nonlinear and one magneto-optic layer, with all other
   layers being linear and isotropic. Therefore, only phase matching
   conditions are required for the device operation irrespective of the
   propagation directions and operation regime (linear/nonlinear). The
   structure is optimized via a genetic algorithm and tested with a
   numerical formalism based on the wide-angle finite-difference beam
   propagation method. The results clearly demonstrate the ability of
   performing two distinct functions on the same device. (c) 2005 Society
   of Photo-Optical Instrumentation Engineers.}},
