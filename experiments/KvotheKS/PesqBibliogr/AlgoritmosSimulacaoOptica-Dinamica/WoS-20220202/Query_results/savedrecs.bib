@article{ WOS:000432748600027,
Author = {Song Chunsheng and Shang Erwei and Huang Xiangyang and Zhang Jinguang},
Title = {{Monitoring the cohesive damage of the adhesive layer in CFRP
   double-lapped bonding joint based on non-uniform strain profile
   reconstruction using dynamic particle swarm optimization algorithm}},
Journal = {{MEASUREMENT}},
Year = {{2018}},
Volume = {{123}},
Pages = {{235-245}},
Month = {{JUL}},
Abstract = {{To well understand the failure mode and monitor the internal damage of
   the carbon fiber reinforced plastic (CFRP) bonding joint provides
   guarantee for the safety and reliability of the aerospace. This article
   obtained the mapping relationship model between the strain profiles
   change of the adherend and the damage evolution of adhesive layer in the
   CFRP double-lapped bonding joint by ABAQUS based on the cohesive zone
   model. Then the Fiber Bragg Grating (FBG) was embedded in CFRP adherend
   to calculate the reflection spectrum of FBG for reconstructing the
   adherend strain profile of FBG sensing area with the dynamic particle
   swarm optimization algorithm. The reconstructed results showed that the
   maximum error between the simulated strain profile and the reconstructed
   one under different loads was not exceeding 1.701, revealing good
   consistency and making it possible to monitor the cohesive damage of
   adhesive layer based on the non-uniform strain profile reconstruction.}},
