@article{ WOS:000336957700124,
Author = {Zuo, Hai-Jie and Zhang, Jiang-Yong and Ying, Ying-Lei and Zhang,
   Bao-Ping and Hou, Zhi-Jin and Chen, Hong-Xu and Si, Jun-Jie},
Title = {{Rigorous microlens design using vector electromagnetic method combined
   with simulated annealing optimization}},
