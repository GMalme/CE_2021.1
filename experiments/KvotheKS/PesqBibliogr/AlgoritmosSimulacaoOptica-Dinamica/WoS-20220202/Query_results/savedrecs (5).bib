@article{ WOS:000254602400013,
Author = {Volchenkov, D. and Lima, R.},
Title = {{Asymptotic series in dynamics of fluid flows: Diffusion versus
   bifurcations}},
Journal = {{COMMUNICATIONS IN NONLINEAR SCIENCE AND NUMERICAL SIMULATION}},
Year = {{2008}},
Volume = {{13}},
Number = {{7}},
Pages = {{1329-1342}},
Month = {{SEP}},
Abstract = {{The Brownian motion over the space of fluid velocity configurations
   driven by the hydrodynamical equations is considered. The Green function
   is computed in the form of an asymptotic series close to the standard
   diffusion kernel. Tile high order asymptotic coefficients are studied.
   Similarly to the models of quantum field theory, the asymptotic
   contributions show a factorial growth and are summated by means of
   Borel's procedure. The resulting corrected diffusion spectrum has a
   closed analytical form. The approach provides a possible ground for the
   optimization of existing numerical simulation algorithms and can be used
   for the analysis of other asymptotic series in turbulence. (C) 2006
   Elsevier B.V. All rights reserved.}},
