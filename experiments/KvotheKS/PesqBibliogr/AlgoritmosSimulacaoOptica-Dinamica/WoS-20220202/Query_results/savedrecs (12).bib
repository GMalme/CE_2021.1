@article{ WOS:000458786800065,
Author = {Kleinert, Sven and Tajalli, Ayhan and Nagy, Tamas and Morgner, Uwe},
Title = {{Rapid phase retrieve of ultrashort pulses from dispersion scan traces
   using deep neural networks}},
Journal = {{OPTICS LETTERS}},
Year = {{2019}},
Volume = {{44}},
Number = {{4}},
Pages = {{979-982}},
Month = {{FEB 15}},
Abstract = {{The knowledge of the temporal shape of femtosecond pulses is of major
   interest for all their applications. The reconstruction of the temporal
   shape of these pulses is an inverse problem for characterization
   techniques, which benefit from an inherent redundancy in the
   measurement. Conventionally, time-consuming optimization algorithms are
   used to solve the inverse problems. Here, we demonstrate the
   reconstruction of ultrashort pulses from dispersion scan traces
   employing a deep neural network. The network is trained with a multitude
   of artificial and noisy dispersion scan traces from randomly shaped
   pulses. The retrieval takes only 16 ms enabling video-rate
   reconstructions. This approach reveals a great tolerance against noisy
   conditions, delivering reliable retrievals from traces with
   signal-to-noise ratios down to 5. (C) 2019 Optical Society of America}},
