@article{ WOS:000287650400007,
Author = {Gillula, Jeremy H. and Hoffmann, Gabriel M. and Huang, Haomiao and
   Vitus, Michael P. and Tomlin, Claire J.},
Title = {{Applications of hybrid reachability analysis to robotic aerial vehicles}},
Journal = {{INTERNATIONAL JOURNAL OF ROBOTICS RESEARCH}},
