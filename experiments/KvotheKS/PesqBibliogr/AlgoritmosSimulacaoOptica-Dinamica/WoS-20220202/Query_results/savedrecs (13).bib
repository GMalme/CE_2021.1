@article{ WOS:000276610300088,
Author = {Shirakawa, Tatsuya and Ishikawa, Kenichi L. and Suzuki, Shuichi and
   Yamada, Yasufumi and Takahashi, Hiroyuki},
Title = {{Design of binary diffractive microlenses with subwavelength structures
   using the genetic algorithm}},
Journal = {{OPTICS EXPRESS}},
Year = {{2010}},
Volume = {{18}},
Number = {{8}},
Pages = {{8383-8391}},
Month = {{APR 12}},
Abstract = {{We present a method to design binary diffractive microlenses with
   subwavelength structures, based on the finite-difference time-domain
   method and the genetic algorithm, also accounting for limitations on
   feature size and aspect ratio imposed by fabrication. The focusing
   efficiency of the microlens designed by this method is close to that of
   the convex lens and much higher than that of the binary Fresnel lens
   designed by a previous method. Although the optimized structure appears
   to be a binary Fresnel lens qualitatively, it is hard to quantitatively
   derive directly from the convex Fresnel lens. The design of a microlens
   with reduced chromatic aberration is also presented. (C) 2010 Optical
   Society of America}},
