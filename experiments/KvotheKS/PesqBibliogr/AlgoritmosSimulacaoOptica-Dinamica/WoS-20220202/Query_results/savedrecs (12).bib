@article{ WOS:000482098300094,
Author = {Hoffman, Galen B. and Dallo, Christina and Starbuck, Andrew and Hood,
   Dana and Pomerene, Andrew and Trotter, Douglas and DeRose, Christopher
   T.},
Title = {{Improved broadband performance of an adjoint shape optimized waveguide
   crossing using a Levenberg-Marquardt update}},
Journal = {{OPTICS EXPRESS}},
Year = {{2019}},
Volume = {{27}},
Number = {{17}},
Pages = {{24765-24780}},
Month = {{AUG 19}},
Abstract = {{We derive an adjoint shape optimization algorithm with a compound figure
   of merit and demonstrate its use with both gradient descent and
   Levenberg-Marquart updates for the case of SiO2-buried SOI coplanar
   waveguide crossings. We show that a smoothing parameter, basis function
   width, can be used to eliminate small feature sizes with a small cost to
   device performance. The Levenberg-Marquardt update produces devices with
   larger bandwidth. A waveguide crossing with simulated performance values
   of > 60 dB cross power extinction ratio and > -0.08 dB through power
   over the 1500-1600 nm band is presented. A fabricated device is measured
   to have a maximum of -0.06 dB through power and a 50 dB cross power
   extinction ratio. (C) 2019 Optical Society of America under the terms of
   the OSA Open Access Publishing Agreement}},
