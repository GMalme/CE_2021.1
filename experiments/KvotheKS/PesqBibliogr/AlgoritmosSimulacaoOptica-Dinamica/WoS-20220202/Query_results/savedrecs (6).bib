@article{ WOS:000248784100011,
Author = {Chauvin, N. and Henry, S. and Flocard, H. and Fortuna, F. and Kaitasov,
   O. and Pariset, P. and Pellegrino, S. and Ruault, M. O. and Serruys, Y.
   and Trocelier, P.},
Title = {{Optics calculations and beam line design for the JANNuS facility in
   Orsay}},
Journal = {{NUCLEAR INSTRUMENTS \& METHODS IN PHYSICS RESEARCH SECTION B-BEAM
   INTERACTIONS WITH MATERIALS AND ATOMS}},
