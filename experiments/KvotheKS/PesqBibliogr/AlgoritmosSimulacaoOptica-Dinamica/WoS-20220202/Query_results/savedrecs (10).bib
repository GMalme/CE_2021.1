@article{ WOS:000417311000001,
Author = {Pineda, Fernando J.},
Title = {{Recurrent Backpropagat ion and the Dynamical Approach to Adaptive Neural
   Computation}},
Journal = {{NEURAL COMPUTATION}},
Year = {{1989}},
Volume = {{1}},
Number = {{2}},
Pages = {{161-172}},
Month = {{SUM}},
Abstract = {{Error backpropagation in feedforward neural network models is a popular
   learning algorithm that has its roots in nonlinear estimation and
   optimization. It is being used routinely to calculate error gradients in
   nonlinear systems with hundreds of thousands of parameters. However, the
   classical architecture for backpropagation has severe restrictions. The
   extension of backpropagation to networks with recurrent connections will
   be reviewed. It is now possible to efficiently compute the error
   gradients for networks that have temporal dynamics, which opens
   applications to a host of problems in systems identification and
   control.}},
