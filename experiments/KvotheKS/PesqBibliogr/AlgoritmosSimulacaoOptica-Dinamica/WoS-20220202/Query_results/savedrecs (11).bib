@article{ WOS:A1992HM11000030,
Author = {PREMOLI, A and RASTELLO, ML},
Title = {{MINIMAX REFINING OF OPTICAL MULTILAYER SYSTEMS}},
Journal = {{APPLIED OPTICS}},
Year = {{1992}},
Volume = {{31}},
Number = {{10}},
Pages = {{1597-1605}},
Month = {{APR 1}},
Abstract = {{A new minimax method for refining optical multilayer systems is
   presented.  It minimizes the maximum deviation of the spectral
   transmittance from the desired specifications.  These are assigned in
   such a general way that any shape can approximated.  The algorithm
   consists of iterating optimization steps that are obtained by developing
   the transmittance in the Taylor expansion versus the known parameters
   and solved by piecewise-linear programming.  The investigation is
   limited to the design of lossless multilayers with assigned refractive
   indices and undetermined thicknesses.  Some minimax-refined designs are
   compared favorably with those reported in the literature.}},
