@article{ WOS:000415607200048,
Author = {Fang, Longjie and Zhang, Xicheng and Zuo, Haoyi and Pang, Lin},
Title = {{Focusing light through random scattering media by four-element division
   algorithm}},
Journal = {{OPTICS COMMUNICATIONS}},
Year = {{2018}},
Volume = {{407}},
Pages = {{301-310}},
Month = {{JAN 15}},
Abstract = {{The focusing of light through random scattering materials using
   wavefront shaping is studied in detail. We propose a newfangled approach
