@article{ WOS:000347179300072,
Author = {Sieber, P. E. and Werner, D. H.},
Title = {{Infrared broadband quarter-wave and half-wave plates synthesized from
   anisotropic Bezier metasurfaces}},
Journal = {{OPTICS EXPRESS}},
