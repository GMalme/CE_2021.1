@article{ WOS:000558094900001,
Author = {Cheng, Qiuyun and Zhang, Sen and Bo, Shukui and Chen, Dengxi and Zhang,
   Haijun},
Title = {{Augmented Reality Dynamic Image Recognition Technology Based on Deep
   Learning Algorithm}},
