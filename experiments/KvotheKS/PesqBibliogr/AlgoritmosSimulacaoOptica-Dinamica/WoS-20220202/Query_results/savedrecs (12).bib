@article{ WOS:000247665000015,
Author = {Kanaev, A. V. and Scribner, D. A. and Ackerman, J. R. and Fleet, E. F.},
Title = {{Analysis and application of multiframe superresolution processing for
   conventional imaging systems and lenslet arrays}},
Journal = {{APPLIED OPTICS}},
Year = {{2007}},
Volume = {{46}},
Number = {{20}},
Pages = {{4320-4328}},
Month = {{JUL 10}},
Abstract = {{Low-cost compact sensors for ultrasmall systems are a pressing need in
   many new applications. One potential solution is a shallow aspect ratio
   system using a lenslet array to form multiple undersampled subimages of
   a scene on a single focal plane array, where processing techniques then
   produce an upsampled restored image. We have investigated the
   optimization and theoretical limits of the performance of such arrays.
   We have built a hardware simulator and developed algorithms to process
   imagery similar to that of a full lenslet imaging sensor, which allowed
   us to quickly test optical components, algorithms, and complete system
   designs for future lenslet imaging systems. (C) 2007 Optical Society of
