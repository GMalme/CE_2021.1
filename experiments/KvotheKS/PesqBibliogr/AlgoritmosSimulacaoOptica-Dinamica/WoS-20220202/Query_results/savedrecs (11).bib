   Ozimek, Martin T.},
Title = {{Analytic Gradient Computation for Bounded-Impulse Trajectory Models
   Using Two-Sided Shooting}},
Journal = {{JOURNAL OF GUIDANCE CONTROL AND DYNAMICS}},
