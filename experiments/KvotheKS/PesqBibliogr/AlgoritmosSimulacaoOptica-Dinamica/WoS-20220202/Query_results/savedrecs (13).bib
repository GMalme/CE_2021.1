@article{ WOS:000434996400069,
Author = {Wang, Huijuan and Yang, Wenrong},
Title = {{An Iterative Load Disaggregation Approach Based on Appliance Consumption
   Pattern}},
Journal = {{APPLIED SCIENCES-BASEL}},
