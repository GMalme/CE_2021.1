@article{ WOS:000671583100004,
Author = {Fan, Shengli and Smith-Dryden, Seth and Li, Guifang and Saleh, Bahaa},
Title = {{Optimization-based optical diffraction tomography using iODT
   initialization}},
Journal = {{JOURNAL OF THE OPTICAL SOCIETY OF AMERICA A-OPTICS IMAGE SCIENCE AND
   VISION}},
Year = {{2021}},
Volume = {{38}},
Number = {{7}},
Pages = {{947-953}},
Month = {{JUL 1}},
Abstract = {{Optical diffraction tomography (ODT) is a label-free and noninvasive
   technique for biological imaging. However, ODT is only applicable to
   weakly scattering objects. To extend ODT to the multiple-scattering
   regime, more advanced inversion algorithms have been developed,
   including optimization-based ODT (Opti-ODT) and iterative ODT (iODT). In
   this paper, we propose a combined strategy, namely, an iODT
   initialization for Opti-ODT, based on the observed complementarity of
   their individual advantages. This study numerically demonstrates that
   under this combined strategy, the reconstruction can accurately converge
   to a better local minimum, especially in the case of multiply scattering
   objects with large optical path differences. (c) 2021 Optical Society of
   America}},
