@article{ WOS:000508009000013,
Author = {Huang, Jie and Yang, Junbo and Chen, Dingbo and Bai, Wei and Han,
   Jingmin and Zhang, Zhaojian and Zhang, Jingjing and He, Xin and Han,
   Yunxin and Liang, Linmei},
Title = {{Implementation of on-chip multi-channel focusing wavelength
   demultiplexer with regularized digital metamaterials}},
Journal = {{NANOPHOTONICS}},
