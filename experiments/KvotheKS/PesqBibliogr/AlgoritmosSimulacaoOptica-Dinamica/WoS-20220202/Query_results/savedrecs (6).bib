@article{ WOS:000360084200010,
Author = {Yuan, Quan and Dai, Hua},
Title = {{The matrix pencil nearness problem in structural dynamic model updating}},
Journal = {{JOURNAL OF ENGINEERING MATHEMATICS}},
Year = {{2015}},
Volume = {{93}},
