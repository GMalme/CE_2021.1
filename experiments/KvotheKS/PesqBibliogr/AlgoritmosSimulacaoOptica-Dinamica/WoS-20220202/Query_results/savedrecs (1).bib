@article{ WOS:000712631800001,
Author = {Lai, V and Huang, Y. F. and Koo, C. H. and Ahmed, Ali Najah and
   El-Shafie, Ahmed},
Title = {{Optimization of reservoir operation at Klang Gate Dam utilizing a whale
   optimization algorithm and a Levy flight and distribution enhancement
   technique}},
