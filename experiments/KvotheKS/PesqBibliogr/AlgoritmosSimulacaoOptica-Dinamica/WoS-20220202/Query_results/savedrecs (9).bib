@article{ WOS:000310107300010,
Author = {Sousa, Luisa Costa and Castro, Catarina F. and Antonio, Carlos Conceicao
   and Chaves, Rui},
Title = {{Blood flow simulation and vascular reconstruction}},
Journal = {{JOURNAL OF BIOMECHANICS}},
Year = {{2012}},
Volume = {{45}},
