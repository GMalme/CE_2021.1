@article{ WOS:000287098700001,
Author = {Rostami, A. and Makouei, S.},
Title = {{Novel Low-Bend Large Effective Area Fiber for Fiber-to-the-Home
   Application}},
Journal = {{FIBER AND INTEGRATED OPTICS}},
Year = {{2011}},
Volume = {{30}},
Number = {{1}},
Pages = {{1-8}},
Abstract = {{In this article, with sophisticated modification on modal field
   distribution and introducing a new design procedure, a single-mode fiber
   with ultra-low bending loss and large effective area (Aeff) that is
   appropriate for a fiber-to-the-home operation is presented. The bending
   loss optimization procedure is based on the genetic algorithm. The most
   remarkable feature of this methodology is designing a bend-insensitive
   fiber without reduction of core radius and mode field diameter. The
   designed structure exhibits very small bending loss and large Aeff
   simultaneously. Simulation results show bending loss of 2.7 x 10-3 dB at
   1.55 m for a single turn of 5-mm radius. The calculated mode field
   diameter and Aeff for the designed structure are 9.00 m and 63.62 m2,
   respectively.}},
