@article{ WOS:000542456700001,
Author = {Ran, Xiao-ming and Wang, Wen-hua},
Title = {{Numerical simulation on heave damping of new sandglass-type FPSO}},
Journal = {{SHIPS AND OFFSHORE STRUCTURES}},
Year = {{2021}},
Volume = {{16}},
Number = {{6}},
Pages = {{632-641}},
Month = {{JUL 3}},
Abstract = {{In this paper, due to the special shape of the sandglass-type floating
   body, the characteristic of heave damping of the sandglass-type floating
   production, storage and offloading (FPSO) unit is mainly discussed by
   the computational fluid dynamics method. Firstly, the numerical model
   for free heave decay motion of the sandglass-type floating body was
   created, and the accuracy of the numerical model has been validated by
   the experimental data. Then, the scale effect on the heave damping of
   sandglass-type FPSO was further discussed. Furthermore, to provide a
   reference for the design criteria of sandglass-type floating body, the
   relationships between its heave decay coefficients and shape parameters
   were analysed based on the orthogonal test and single-factor analysis
   method. Finally, the estimation formula of the decay coefficients was
   obtained by means of the optimisation algorithm, which can be used for
   the preliminary design of the shape of the sandglass-type floating body.}},
