@article{ WOS:000502062900029,
Author = {Muhamad, Raees Kizhakkumkara and Blinder, David and Symeonidou,
   Athanasia and Birnbaum, Tobias and Watanabe, Osamu and Schretter, Colas
   and Schelkens, Peter},
Title = {{Exact global motion compensation for holographic video compression}},
Journal = {{APPLIED OPTICS}},
Year = {{2019}},
Volume = {{58}},
Number = {{34}},
Pages = {{G204-G217}},
Month = {{DEC 1}},
Abstract = {{Holographic video requires impractical bitrates for storage and
   transmission without data compression. We introduce an end-to-end
   compression pipeline for compressing holographic sequences with known
   ground truth motion. The compression strategy employs a motion
   compensation algorithm based on the rotational transformation of an
   angular spectrum. Residuals arising from the compensation step are
   represented using short-time Fourier transforms and quantized with
   uniform mid-rise quantizers whose bit depth is determined by a
   Lagrangian rate-distortion optimization criterion where the distortion
   metric is the mean squared error. Experiments use computer-generated
   holographic videos, and we report Bjontegaard delta peak signal-to-noise
   ratio gains of around 20 dB when compared to traditional image/video
   codecs. (C) 2019 Optical Society of America}},
