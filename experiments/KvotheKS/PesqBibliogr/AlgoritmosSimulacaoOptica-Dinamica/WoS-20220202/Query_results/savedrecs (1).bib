@article{ WOS:000516718800020,
Author = {Appel, Sabrina and Geithner, Wolfgang and Reimann, Stephan and Sapinski,
   Mariusz and Singh, Rahul and Vilsmeier, Dominik},
Title = {{Application of nature-inspired optimization algorithms and machine
   learning for heavy-ion synchrotrons}},
